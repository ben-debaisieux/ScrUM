     code : |1+
      PROCÉDURE PRIVÉ sGenProcAfficheFen()<métier>:chaîne
      
      sRetour		est une chaîne
      sNomProc	est une chaîne	= "Affiche"
      sTypeObj	est une chaîne
      sNomProcCol	est une chaîne
      
      
      SI :m_bMultiTopHierarchie ALORS
      	sTypeObj	= :m_sNomDbTop
      	sNomProcCol	= :m_sCleColTop
      SINON
      	sTypeObj	= :m_oClasseGen.sNomClasseGen()
      	sNomProcCol	= :m_sCleColParent
      FIN
      
      
      sRetour = [
      procédure [%sNomProc%]()<UI>:vide<§cs:2§>
      <§cs:2§>
      nRacine			est un entier<§cs:2§>
      OrgaElement		est un OrgaElément<§cs:2§>
      oEltAncetre		est un [%sTypeObj%] dynamique<§cs:2§>
      <§cs:2§>
      <§cs:2§>
      ORGA_SansNom1..AffichageActif = Faux<§cs:2§>
      OrgaSupprimeTout(ORGA_SansNom1)<§cs:2§>
      <§cs:2§>
      POUR TOUT ELEMENT oEltAncetre DE zoColTop.aItems()<§cs:2§>
      [%TAB%]OrgaElement.ID			= oEltAncetre.ZF_Get_IdUnique()<§cs:2§>
      [%TAB%]OrgaElement.Contenu		= oEltAncetre..Classe<§cs:2§>
      [%TAB%]OrgaElement.FiDessin		= "[%:sNomFI()%]"<§cs:2§>
      [%TAB%]nRacine					= OrgaAjoute(ORGA_SansNom1, OrgaElement)<§cs:2§>
      [%TAB%]AffEnfants(nRacine, [%:m_oClasseGen.sNomClasseColGen()%].[%sNomProcCol%](oEltAncetre))<§cs:2§>
      FIN<§cs:2§>
      <§cs:2§>
      ORGA_SansNom1..AffichageActif = vrai<§cs:2§>
      ]
      
      
      RENVOYER :GenBloc_ProcedureLoc(sRetour, sNomProc)
      
     type : 458752
