      SI :m_bMultiTopHierarchie ALORS
      	sTypeObj	= :m_sNomColTop
      	sNomProcCol	= :m_sCleColTop
      SINON
      	sTypeObj	= :m_oClasseGen.sNomClasseGen()
      	sNomProcCol	= :m_sCleColParent
      FIN
      
      
      sRetour = [
      procédure [%sNomProc%](nIdParent_p est un entier, oColAff_p est un [%:m_oClasseGen.sNomClasseColGen()%])<UI>:vide<§cs:2§>
      <§cs:2§>
      OrgaElement		est un OrgaElément<§cs:2§>
      nFeuille		est un entier<§cs:2§>
