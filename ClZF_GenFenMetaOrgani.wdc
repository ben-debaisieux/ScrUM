      			CAS [%:m_sNomDbTop%]
      				zoParent <- [%:m_sNomDbTop%].GetObj(gOrgaElement_p.ID)
      				BTN_Supp..Visible = Faux
      				BTN_Sauve..Visible	= Faux
      				
      			AUTRE CAS
      				ExceptionDéclenche(1,"Erreur contenu")
      		FIN
      		
