      [%TAB%][%TAB%]OrgaElement.Contenu	= oItemActu..classe<§cs:2§>
      [%TAB%][%TAB%]OrgaElement.FiDessin	= "[%:sNomFI()%]"<§cs:2§>
      [%TAB%][%TAB%]nFeuille				= OrgaAjouteFils(ORGA_SansNom1, nIdParent_p, OrgaElement)<§cs:2§>
      		<§cs:2§>
      [%TAB%][%TAB%][%sNomProc%](nFeuille, [%:m_oClasseGen.sNomClasseColGen()%].[%:m_sCleColParent%](oItemActu, Vrai))<§cs:2§>
      [%TAB%]fin<§cs:2§>
      FIN<§cs:2§>
      <§cs:2§>
      ]
      
      
      RENVOYER :GenBloc_ProcedureLoc(sRetour, sNomProc)
