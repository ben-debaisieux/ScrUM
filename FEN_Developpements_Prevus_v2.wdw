  -
    name : BTN_Afficher
    identifier : 0x200c3821415bb6c0
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         //AnalyseurDébut()
         
         nLigneUserStory	est un entier
         nLigneTache		est un entier
         aOPrj			est un tableau de ClProjet
         oTache			est un ClTache dynamique
         pclPrj			est un ClProjet dynamique
         oUS				est un ClUserStory dynamique
         oSprint			est un ClSprint dynamique
         bAvecTache		est un booléen = (SEL_Visu = 2)
         sPeriode		est une chaîne
         nNumSprint		est un entier
         bAffiche		est un booléen
         
         TableSupprimeTout(TBL_Projet)
         TBL_Projet..AffichageActif = Faux
         
         selon SEL_Visu
         	// Visu. chef de projet
         	CAS 1
         		TBL_Projet.COL_Sprint..Visible = faux
         	// Visu équipe de Dev
         	CAS 2
         		TBL_Projet.COL_Sprint..Visible = vrai
         FIN
         
         SI CBO_Projet dans(cstcboTous, cstcboFutur) ALORS
         	si goapp.m_oUsrCnx.p_bTop_Equip_Informatique ALORS
         		aOPrj = ClProjet.aoListeProjets(INT_Projets_Termines)
         	sinon
         		aOPrj = goApp.m_oUsrCnx.aoMesProjets()
         	FIN
         	
         SINON
         	TableauAjouteRéférence(aOPrj,ClProjet.GetObj(CBO_Projet))
         FIN
         
         
         POUR TOUT ELEMENT pclPrj DE aOPrj
         	POUR TOUT ELEMENT oUS DE pclPrj.aoListeUS()
         		
         		bAffiche = vrai
         		
         		
         		
         		si INT_AfficherUSFinies = 0 et oUS.p_bTermine alors
         			//L'US est finie et on ne souhaite pas l'afficher.
         			bAffiche = faux
         		FIN
         		
         		si CBO_Etat = "" ALORS
         			//ok
         		sinon
         			si CBO_Etat = cst_StatutUS_A_faire et oUS.Get_EtatUS() <> ClUserStory.ECompteNbUS.A_faire ALORS
         				bAffiche = Faux
         			FIN
         				
         			SI CBO_Etat = cst_StatutUS_Commencée ET oUS.Get_EtatUS() <> ClUserStory.ECompteNbUS.EnCours ALORS
         				bAffiche = Faux
         			FIN
         				
         			si CBO_Etat = cst_StatutUS_En_Test et oUS.Get_EtatUS() <> ClUserStory.ECompteNbUS.En_test_fonctionnel ALORS
         				bAffiche = Faux
         			FIN
         				
         			si CBO_Etat = cst_StatutUS_Terminée et oUS.Get_EtatUS() <> ClUserStory.ECompteNbUS.Terminee ALORS
         				bAffiche = Faux
         			FIN
         				
         			si CBO_Etat = cst_StatutUS_Annulée et oUS.Get_EtatUS() <> ClUserStory.ECompteNbUS.Annulée ALORS
         				bAffiche = Faux
         			FIN
         		FIN
         		
         		
         		si bAffiche alors
         			nNumSprint = oUS.Get_DemarrageTache()
         			si nNumSprint = 0 ALORS
         				sPeriode = "(Non déterminée)"
         				nNumSprint = 999999999
         			sinon
         				oSprint <- ClSprint.GetObj(nNumSprint)
         				sPeriode = oSprint.Get_IdUnique() + RC + "(" + oSprint.sSprint_Date() + ")"
         			FIN
         			
         			nLigneUserStory														= TableAjouteFils(TBL_Projet,Null)
         			
         			si bAvecTache alors
         				//On change la couleur pour les lignes qui sont des us
         				TBL_Projet[nLigneUserStory]..CouleurFond						= COL_Scrum.gnCouleur_Gris_TClair
         			fin
         			TBL_Projet[nLigneUserStory].COL_N									= oUS.Get_IdUnique()
         			TBL_Projet[nLigneUserStory].COL_EstUneUS							= vrai
         			TBL_Projet[nLigneUserStory].COL_Num_Sprint							= nNumSprint
         			TBL_Projet[nLigneUserStory].COL_Code								= oUS.p_sCodeUserStory
         			TBL_Projet[nLigneUserStory].COL_Besoin_Utilisateurs_Fonctionnalité	= oUS.sNommination_US()
         			TBL_Projet[nLigneUserStory].COL_Besoin_Utilisateurs_Fonctionnalité..Bulle	= oUS.p_sCMTR_Global
         			TBL_Projet[nLigneUserStory].COL_Sprint								= oUS.Get_DemarrageTache()
         			TBL_Projet[nLigneUserStory].COL_Periode_de_démarrage				= sPeriode
         			TBL_Projet[nLigneUserStory].COL_Charge								= oUS.User_Story_Poids()
         			TBL_Projet[nLigneUserStory].COL_Projet								= oUS.p_oProjetParent.p_sNom
         			TBL_Projet[nLigneUserStory].COL_Etat								= oUS.Get_EtatUS()
         	
         			SELON oUS.Get_EtatUS()
         				CAS ClUserStory.ECompteNbUS.Annulée
         					TBL_Projet[nLigneUserStory].COL_Etat = cst_StatutUS_Annulée
         				CAS ClUserStory.ECompteNbUS.Terminee
         					TBL_Projet[nLigneUserStory].COL_Etat = cst_StatutUS_Terminée
         				CAS ClUserStory.ECompteNbUS.EnCours
         					TBL_Projet[nLigneUserStory].COL_Etat = cst_StatutUS_Commencée
         				CAS ClUserStory.ECompteNbUS.En_test_fonctionnel
         					TBL_Projet[nLigneUserStory].COL_Etat = cst_StatutUS_En_Test
         				CAS ClUserStory.ECompteNbUS.A_faire
         					TBL_Projet[nLigneUserStory].COL_Etat = cst_StatutUS_A_faire
         				AUTRE CAS
         					
         			FIN
         			
         			
         			
         			si bAvecTache alors
         				POUR TOUT ELEMENT oTache DE oUS.aoListeTaches()
         					
         					
         					nLigneTache														= TableAjouteFils(TBL_Projet,nLigneUserStory)
         					
         					TBL_Projet[nLigneTache].COL_N									= oTache.Get_IdUnique()
         					TBL_Projet[nLigneTache].COL_Code								= oTache.p_sCodeTache
         					TBL_Projet[nLigneTache].COL_EstUneUS							= faux
         					TBL_Projet[nLigneTache].COL_Besoin_Utilisateurs_Fonctionnalité	= oTache.p_sDescription
         					TBL_Projet[nLigneTache].COL_Sprint								= oTache.p_nNum_Sprint_Realisation
         					
         					oSprint <- ClSprint.GetObj(oTache.p_nNum_Sprint_Realisation)
         					TBL_Projet[nLigneTache].COL_Periode_de_démarrage				= oSprint.sSprint_Date()
         					TBL_Projet[nLigneTache].COL_Projet								= oUS.p_oProjetParent.p_sNom
         					
         			//		SELON Vrai
         			//			CAS oTache.m_bAnnule
         			//				TBL_Projet[nLigneTache].COL_Etat	= "Annulée"
         			//				TBL_Projet[nLigneTache].COL_Filtre	= cstTacheTerminee
         			//				
         			//			CAS PAS oTache.m_bEstRealise ET oTache.p_oUsr_Dev = Null
         			//				
         			//				TBL_Projet[nLigneTache].COL_Etat	= "A faire"
         			//				TBL_Projet[nLigneTache].COL_Filtre	=  cstTacheNonTerminee
         			//				
         			//			CAS PAS oTache.m_bEstRealise ET oTache.p_oUsr_Dev <> Null
         			//				
         			//				TBL_Projet[nLigneTache].COL_Etat	= "Démarrée"
         			//				TBL_Projet[nLigneTache].COL_Filtre	=  cstTacheNonTerminee
         			//				
         			//			CAS oTache.m_bEstRealise
         			//				TBL_Projet[nLigneTache].COL_Etat	= "Terminée"
         			//				TBL_Projet[nLigneTache].COL_Filtre	= cstTacheTerminee
         			//			AUTRE CAS
         			//				TBL_Projet[nLigneTache].COL_Etat	= ""
         			//				TBL_Projet[nLigneTache].COL_Filtre	= cstTacheNonTerminee
         			//		FIN
         					
         				FIN
         			FIN
         		
         		//	SI oUS.Get_EtatUS() = ClUserStory.ECompteNbUS.Terminee ALORS
         				TableEnroule(TBL_Projet,nLigneUserStory)
         		//	FIN
         		FIN
         	FIN
         fin
         
         tabletrie(TBL_Projet, TBL_Projet.COL_Num_Sprint..nom, TBL_Projet.COL_Code..Nom)
         TBL_Projet..AffichageActif = Vrai
         
