   -
     code : |1+
      Procedure ETAT_Avancement_Projet_V2(oProjet_P est un ClDbProjet Dynamique)
      
      
      zbEnteteImprimer est un booleen
      znUSImp est un entier
      zstProjet est un STR_Projet
      
      
      local
      nNbreTotal est un entier
      oUs est un ClDbUserStory dynamique
      stUserStorie est une STR_UserStorie 
      
      
      zstProjet.nIdProjet            		= oProjet_P.ZF_Get_IdUnique()
      zstProjet.sNomProjet 		  		= oProjet_P.p_sNom
      zstProjet.bTermineProjet       		= oProjet_P.p_bTop_Termine
      zstProjet.nIDUtilisateurProjet 		= oProjet_P.p_oUsr_ChefProjet.ZF_Get_IdUnique()
      zstProjet.sResponsable         		= oProjet_P.p_oUsr_ChefProjet.p_sNom
      
      
      POUR TOUT ELEMENT oUs DE ClColUserStory.oFromProjet(oProjet_P).aitems()	//oProjet_P.aoListeUS().aItems()
      	VariableRAZ(stUserStorie)
      	
      	stUserStorie.nIDUserStorie		= oUs.ZF_Get_IdUnique()
      	stUserStorie.sCodeUserStorie	= oUs.p_sCodeUserStory
      	stUserStorie.sEnTantQue			= oUs.p_sEntantQue
      	stUserStorie.sBesoinUserStorie	= oUs.p_sJaiBesoinDe
      	stUserStorie.sAfindeUserStorie	= oUs.p_sAfinDe
      	stUserStorie.nIDResponsable		= oUs.p_oUsrResponsable.ZF_Get_IdUnique()
      	stUserStorie.bAtester			= oUs.p_bEstTestéParLeDemandeur
      	stUserStorie.eEtatUserStorie	= oUs.Get_EtatUS()
      	stUserStorie.sCharge			= oUs.User_Story_Poids()
      //	stUserStorie.nRetard			= oUs.Get_EtatRetard()
      //	stUserStorie.nNbrSprintRetard	= oUs.Get_NbSprintRetard()
      	
      	TableauAjoute(zstProjet.aTableau_UserStorie,stUserStorie)
      FIN
      
      nNbreTotal							= TableauOccurrence(zstProjet.aTableau_UserStorie)
      zstProjet.nNbrUSTerminee         	= oProjet_P.Get_StatUS(ClDbUserStory.ECompteNbUS.Terminee)
      zstProjet.rPourcentageUsTerminee 	= oProjet_P.Get_StatUS(ClDbUserStory.ECompteNbUS.Terminee) / nNbreTotal
      zstProjet.nNbrUSAfaire           	= oProjet_P.Get_StatUS(ClDbUserStory.ECompteNbUS.A_faire)
      zstProjet.rPourcentageUsAfaire   	= oProjet_P.Get_StatUS(ClDbUserStory.ECompteNbUS.A_faire) / nNbreTotal
      zstProjet.nNbrUSCommencee        	= oProjet_P.Get_StatUS(ClDbUserStory.ECompteNbUS.EnCours)
      zstProjet.rPourcentageUsCommencee	= oProjet_P.Get_StatUS(ClDbUserStory.ECompteNbUS.EnCours) / nNbreTotal
      zstProjet.nNbrUSAnnule           	= oProjet_P.Get_StatUS(ClDbUserStory.ECompteNbUS.Annulée)
      zstProjet.rPourcentageAnnule     	= oProjet_P.Get_StatUS(ClDbUserStory.ECompteNbUS.Annulée) / nNbreTotal
      zstProjet.nNbrUSEnTest          	= oProjet_P.Get_StatUS(ClDbUserStory.ECompteNbUS.En_test_fonctionnel)
      zstProjet.rPourcentageUsEnTest   	= oProjet_P.Get_StatUS(ClDbUserStory.ECompteNbUS.En_test_fonctionnel) / nNbreTotal
      
      
     type : 327680
   -
