        code : |1+
         
         
         Si TableauOccurrence(zstProjet.aTableau_UserStorie) > 0 Alors
         	
         	LIB_CodeUS = zstProjet.aTableau_UserStorie[znUSImp].nIDUserStorie
         	
         	SI zstProjet.aTableau_UserStorie[znUSImp].sCodeUserStorie <> "" alors //affichage de l'ancien code de l'us
         		LIB_CodeUS += RC + "(" + zstProjet.aTableau_UserStorie[znUSImp].sCodeUserStorie + ")"
         	FIN
         	 
         	LIB_EnTantQueUS = zstProjet.aTableau_UserStorie[znUSImp].sEnTantQue
         	LIB_Besoin = zstProjet.aTableau_UserStorie[ZnUsImp].sBesoinUserStorie 
         	SI zstProjet.aTableau_UserStorie[znUSImp].sAfindeUserStorie <> "" alors
         		LIB_Besoin += RC + "(" + zstProjet.aTableau_UserStorie[znUSImp].sAfindeUserStorie + ")"
         	FIN
         	
         	
         	
         	SELON Vrai
         		CAS zstProjet.aTableau_UserStorie[znUSImp].eEtatUserStorie = ClDbUserStory.ECompteNbUS.A_faire
         			LIB_EtatUS = "A faire" 
         			LIB_CouleurUS..CouleurFond = COL_Scrum.gnCouleur_Rougeclair
         		CAS zstProjet.aTableau_UserStorie[znUSImp].eEtatUserStorie = ClDbUserStory.ECompteNbUS.EnCours
         			LIB_EtatUS = "Commencée" 
         			LIB_CouleurUS..CouleurFond = COL_Scrum.gnCouleur_Orange_pale
         		CAS zstProjet.aTableau_UserStorie[znUSImp].eEtatUserStorie = ClDbUserStory.ECompteNbUS.Terminee
         			LIB_EtatUS = "Finie"  
         			LIB_CouleurUS..CouleurFond = COL_Scrum.gnCouleur_Vert_pale 
         		CAS zstProjet.aTableau_UserStorie[znUSImp].eEtatUserStorie = ClDbUserStory.ECompteNbUS.Annulée
         			LIB_EtatUS = "Annulee"  
         			LIB_CouleurUS..CouleurFond = COL_Scrum.gnCouleur_Vert_pale 
         		cas zstProjet.aTableau_UserStorie[znUSImp].eEtatUserStorie = ClDbUserStory.ECompteNbUS.En_test_fonctionnel
         			LIB_EtatUS = "En Test"
         			LIB_CouleurUS..CouleurFond = COL_Scrum.gnCouleur_Orange_pale
         		Autre CAS : //
         	FIN
         
         	LIB_Charge = zstProjet.aTableau_UserStorie[znUSImp].sCharge
