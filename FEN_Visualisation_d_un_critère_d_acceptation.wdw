   -
     name : ChargeCriteres
     procedure_id : 2205469005588575390
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // ChargeCriteres ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE ChargeCriteres()
      
      oCompo est un ClDbCritereAccept_Composant dynamique
      
      
      ARBRE_SansNom1..Libellé = zoCritere.p_sTitre
      
      ArbreSupprimeTout(ARBRE_SansNom1)
      
      //On force le rechargement des composants.
      zoCritere.aoComposants(Vrai)
      
      sNiv est une chaîne
      sNiv = "Etant donnée que..."
      ArbreAjoute(ARBRE_SansNom1,sNiv,Null,Null,"",aDernier)
      POUR TOUT ELEMENT oCompo DE zoCritere.Get_aoEtantDonneQue().aItems()
      	ArbreAjoute(ARBRE_SansNom1,sNiv + TAB + oCompo.p_sDescription,null,Null,"",aDernier)
      FIN
      
      sNiv = "Au moment de..."
      ArbreAjoute(ARBRE_SansNom1,sNiv,Null,Null,"",aDernier)
      ArbreAjoute(ARBRE_SansNom1,sNiv + TAB + zoCritere.Get_sAuMomentDe(),Null,Null,"",aDernier)
      
      
      sNiv = "J'attend que..."
      ArbreAjoute(ARBRE_SansNom1,sNiv,Null,Null,"",aDernier)
      POUR TOUT ELEMENT oCompo DE zoCritere.Get_aoJAttendsQue().aItems()
      	ArbreAjoute(ARBRE_SansNom1,sNiv + TAB + oCompo.p_sDescription,Null,Null,"",aDernier)
      FIN
      
      ArbreDérouleTout(ARBRE_SansNom1)
