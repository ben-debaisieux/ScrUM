     code : |1-
      procédure Actualise(LOCAL znSprint est un entier, ...
      	LOCAL znNbSprint est un entier, ...
      	LOCAL znSelecteur est un ClTache.ETypeCharge,...
      	LOCAL nChargeReccurcive est un entier)
      		
      		
      stGraphe est un col_charge_ui.STR_Charge		
      sStPrj est une col_charge_ui.STR_Charge_InfoPrj
      stSpr est une col_charge_ui.STR_Charge_Spr
      nInd est un entier
      nInd2 est un entier
      
      stGraphe = col_charge_ui.Graphe_Charge_Sprints(znSprint, znNbSprint, znSelecteur, nChargeReccurcive)
      
      
      grSupprimeSérie(GRF_Planification)
      grLibellé(GRF_Planification, grAfficheEtiquette, Vrai)
      
      
      
      
      nInd = 1
      
      //Affiche une ligne representant la vélocité moyenne actuelle
      // - type de la série
      grTypeSérie(GRF_Planification, nInd, grDroiteConstante, stGraphe.nVelocite)
      // - étiquette
      grEtiquetteSérie(GRF_Planification, nInd, "Vélocité moyenne")
      // - type de trait
      grTypeTraitSérie(GRF_Planification, nInd, TraitContinu)
      // - épaisseur du trait
      grEpaisseurTrait(GRF_Planification, nInd, 2)
      // - couleur
      grCouleurSérie(GRF_Planification, nInd,RougeClair)
      
      
      GRF_Planification..Titre = stGraphe.sTitre
      //nInd = 0
      POUR TOUT ELEMENT sStPrj DE stGraphe.aStProjets
      	nInd++
      	//nom du projet pour le nom de l'étiquette 
      	grEtiquetteSérie(GRF_Planification,nInd, sStPrj.sNom)
      	//donne une couleur à la série
      	grCouleurSérie(GRF_Planification, nInd , sStPrj.nCouleur)
      	
      	nInd2 = 0
      	//parcours les projets compris dans l'intervalle de sprints
      	POUR TOUT stSpr DE sStPrj.aSpr
      		nInd2++
      		//ajout des données dans le graphe
      		grAjouteDonnée(GRF_Planification, nInd, nInd2, stSpr.nPts)
      	FIN
      FIN
      
      sSpr est une chaîne
      nInd = 0
      pour TOUT sSpr de stGraphe.aStListeSprints
      	nInd++
      	grEtiquetteCatégorie(GRF_Planification,nInd,sSpr)
      FIN
      
      
      
      grDessine(GRF_Planification)
      //
