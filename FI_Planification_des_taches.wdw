    controls :
     -
       name : GRF_Planification
       identifier : 0x2caf297201122f58
       internal_properties : 
       type : 33
       code_elements :
        type_code : 72
        p_codes :
         -
           code : |1+
            grSupprimeSérie(GRF_Planification)
            
            grLibellé(GRF_Planification, grAfficheEtiquette, Vrai)
            
            
            pclOPrj est un ClProjet dynamique
            pclSprint est un ClSprint dynamique
            sDateSprint est une chaine
            
            i,j sont des entiers
            
            nNumSprint est un entier = znSprint
            aTableauProjet est un tableau de ClProjet = ClProjet.Projet_Dans_Graphe(nNumSprint, znNbSprint, znSelecteur)
            nPoints est un entier
            
            Selon znSelecteur
            	cas ClTache.ETypeCharge.Charge_réelle
            		GRF_Planification..Titre += " en production"
            	cas ClTache.ETypeCharge.Charge_prévisionelle
            		GRF_Planification..Titre += " prévisionnel"	
            fin
            
            POUR i = 1 _a_ znNbSprint //pour chaque sprint on effectue la boucle suivante
            	
            	//parcours les projets compris dans l'intervalle de sprints
            	POUR tout element pclOPrj de aTableauProjet
            		
            		//recuperation du nb de la charge du projet
            		nPoints = pclOPrj.Projet_Sprint_Points(nNumSprint,znSelecteur)
            		
            		//ajout des donnée dans le graphe
            		grAjouteDonnée(GRF_Planification,j,nPoints)
            		
            		//nom du projet pour le nom de l'étiquette 
            		grEtiquetteSérie(GRF_Planification,j,pclOPrj.p_oUsr_ChefProjet.m_sNom + rc + pclOPrj.p_sNom)
            		//donne une couleur à la série
            		grCouleurSérie(GRF_Planification, j, DonneCouleur(j, coulSérieDéfaut))
            	FIN
            	
            	//affichage des dates du sprint en graduation
            	pclSprint <- ClSprint.GetObj(nNumSprint)
            	sDateSprint = pclSprint.sSprint_Date()
            
            	grEtiquetteCatégorie(GRF_Planification,i,sDateSprint)
            	
            	nNumSprint++
            FIN
            
             //Affiche une ligne representant la vélocité moyenne actuelle
            // - type de la série
            grTypeSérie(GRF_Planification, (j+1), grDroiteConstante, goApp.m_nVelociteMoyenne)
            // - étiquette
            grEtiquetteSérie(GRF_Planification, (j+1), "Vélocité moyenne")
            // - type de trait
            grTypeTraitSérie(GRF_Planification,(j+1), TraitContinu)
            // - épaisseur du trait
            grEpaisseurTrait(GRF_Planification, (j+1), 2)
            // - couleur
            grCouleurSérie(GRF_Planification, (j+1),RougeClair)
            
            
            
            
            
           type : 14
