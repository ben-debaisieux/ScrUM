     type_code : 4
     p_codes :
      -
        code : |1+
         //Contrôle des valeurs
         SI SAI_Libelle = "" ALORS
         	Erreur("Libelle non renseigné")
         	RepriseSaisie(SAI_Libelle)
         sinon si SAI_Ordre = "" alors
         	Erreur("Erreur Saisie Ordre")
         	RepriseSaisie(SAI_Ordre)
         FIN
         
         //Enregistrement de la fiche
         SI ID_ComposantType = 0 ALORS HRAZ(Scr_CritereAccept_Composant_Type)
         
         Scr_CritereAccept_Composant_Type.Libelle = SAI_Libelle
         Scr_CritereAccept_Composant_Type.Ordre = SAI_Ordre
         
         SI ID_ComposantType <> 0 ALORS
         	// Modification de la fiche
         	SI PAS HModifie(Scr_CritereAccept_Composant_Type) ALORS
         		Erreur(ErreurInfo(errComplet))
         		RepriseSaisie(SAI_Libelle)
         	FIN	
         	Info("Critère Composant Type Modifié avec succès !")
         SINON
         	//Ajout	
         	SI PAS HAjoute(Scr_CritereAccept_Composant_Type) ALORS
         		Erreur(ErreurInfo(errComplet))
         		RepriseSaisie(SAI_Libelle)
         	FIN	
         	Info("Critère Composant Type Ajouter avec Succès !")
         FIN
         MaFenêtre..ValeurRenvoyée = Vrai
         Ferme()
         
         
