          name : GRPRUB_SansNom1
          identifier : 0x2fb2f82501ddf439
          internal_properties : 
          controls :
           -
             name : BTN_Rehercher
             identifier : 0x2fb2f84601eb73d9
             internal_properties : 
             type : 4
             code_elements :
              type_code : 4
              p_codes :
               -
                 code : |1+
                  sRech est une chaîne
                  oUs_Tmp est un ClUserStory dynamique
                  oTache_Tmp est un ClTache dynamique
                  
                  si saisie("Recherche :",sRech) ALORS
                  	sdRech	est une Source de Données
                  	sReq	est une chaîne
                  	
                  	sReq = ChaîneConstruit([
                  	
                  	SELECT
                  		1 AS Type,
                  		Scr_UserStory.IDScr_UserStory AS Id
                  	FROM Scr_UserStory
                  	WHERE Scr_UserStory.CodeUserStory like '%%%1%%'
                  	
                  	UNION ALL
                  		
                  	SELECT
                  		2 AS Type,
                  		Scr_Tache.IDScr_Tache AS Id
                  	FROM Scr_Tache
                  	WHERE Scr_Tache.CodeTache like '%%%1%%'
                  		OR Scr_Tache.NumTicketIncident like '%%%1%%'
                  	
                  		
                  	], sRech)
                  	
                  	SI goapp.RSQL(sdRech,sReq) ALORS
                  		
                  		POUR TOUT sdRech
                  			
                  			selon sdRech.type
                  				
                  				cas 1
                  					oUs_Tmp <- ClUserStory.GetObj(sdRech.Id)
                  					
                  					trace(oUs_Tmp.sNommination_US())
                  				cas 2
                  					oTache_Tmp <- ClTache.GetObj(sdRech.Id)
                  					trace(oTache_Tmp.p_sDescription)
                  				AUTRE CAS
                  					
                  			FIN
                  			
                  			
                  		FIN
                  		
                  	FIN
                  	
                  	HAnnuleDéclaration(sdRech)
                  	
                  FIN
                 type : 18
               -
                 type : 161
                 enabled : false
          type : 88
          code_elements :
           type_code : 123
           p_codes : []
