     type_code : 14
     code : |1+
      procédure ChangeModeAffichage()
      
      nInd est un entier
      
      
      MaFenêtre..Plan = 2
      
      TDB_General.SupprimeTout()
      
      nInd = TDB_General.AjouteWidget(FI_MesProjetsEnCours_v2, "Mes projets")
      TDB_General[nInd]..Visible = Vrai
      TDB_General.ChangeTailleWidget(nInd, 1, 2)
      TDB_General.DéplaceWidget(nInd, 1, 1)
      
      
      SELON SEL_ModeAffichage
      	// Mode dev
      	CAS 1
      		
      		nInd = TDB_General.AjouteWidget(FI_Taches_a_faire, "FI_Taches_a_faire")
      		TDB_General[nInd]..Visible = Vrai
      		TDB_General.ChangeTailleWidget(nInd, 1, 2)
      		TDB_General.DéplaceWidget(nInd, 2, 1)
      				
      		nInd = TDB_General.AjouteWidget(FI_MesTaches, "FI_MesTaches")
      		TDB_General[nInd]..Visible = Vrai
      		TDB_General.DéplaceWidget(nInd, 3, 1)
      				
      		nInd = TDB_General.AjouteWidget(FI_MesTestsFonctionnels, "FI_MesTestsFonctionnels")
      		TDB_General[nInd]..Visible = Vrai
      		TDB_General.DéplaceWidget(nInd, 4, 1)
      
      		
      		nInd = TDB_General.AjouteWidget(FI_MesTestsUnitaires, "FI_MesTestsUnitaires")
      		TDB_General[nInd]..Visible = Vrai
      		TDB_General.DéplaceWidget(nInd, 3, 2)
      		
      		nInd = TDB_General.AjouteWidget(FI_MesBrouillons, "FI_MesBrouillons")
      		TDB_General[nInd]..Visible = Vrai
      		TDB_General.DéplaceWidget(nInd, 4, 2)
      
      		
      		nInd = TDB_General.AjouteWidget(FI_MesCritèresAControler, "FI_MesCritèresAControler")
      		TDB_General[nInd]..Visible = Vrai
      		TDB_General.DéplaceWidget(nInd, 1, 3)
      		
      		nInd = TDB_General.AjouteWidget(FI_CotationsAFaire, "FI_CotationsAFaire")
      		TDB_General[nInd]..Visible = Vrai
      		TDB_General.DéplaceWidget(nInd, 2, 3)
      				
      		nInd = TDB_General.AjouteWidget(FI_Taches_suspendues, "FI_Taches_suspendues")
      		TDB_General[nInd]..Visible = Vrai
      		TDB_General.DéplaceWidget(nInd, 3, 3)
      				
      		nInd = TDB_General.AjouteWidget(FI_MesMessages, "FI_MesMessages")
      		TDB_General[nInd]..Visible = Vrai
      		TDB_General.DéplaceWidget(nInd, 4, 3)
      		
      	// Mode manager
      	CAS 2
      		
      		nInd = TDB_General.AjouteWidget(FI_InfoPrj, "Info projet")
      		TDB_General[nInd]..Visible = Vrai
      		TDB_General.ChangeTailleWidget(nInd, 2, 2)
      		TDB_General.DéplaceWidget(nInd, 2, 1)
      		
      		nInd = TDB_General.AjouteWidget(FI_MesTestsFonctionnels, "Mes tests")
      		TDB_General[nInd]..Visible = Vrai
      		TDB_General.ChangeTailleWidget(nInd, 1, 2)
      		TDB_General.DéplaceWidget(nInd, 4, 1)
      		
      
      FIN
      
      
