     type_code : 12
     code : |1-
      procedure globale sNomRelationDeType(nType_P est un entier)<métier>:chaine
      
      sRetour est une chaîne
      
      selon nType_P
      	CAS cstRelAgr
      		sRetour = "Agrégation"
      	CAS cstRelAsso
      		sRetour = "Association"
      	CAS cstRelCompo
      		sRetour = "Composition"
      	CAS cstRelIncl
      		sRetour = "Inclusion"
      		
      	AUTRE CAS
      		ExceptionDéclenche(1, "Type de relation non prévu")
      FIN
      
      renvoyer sRetour
