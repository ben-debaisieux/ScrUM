      FIN
      //:m_sNomUsage = remplace(sNomTable_P, ::mg_sPrefixeTable , "", SansCasse)
      
      si sNomTable_P <> "" ALORS
      	:m_sLibelle = {sNomTable_P,indFichier}..Libellé
