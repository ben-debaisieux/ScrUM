      -
        code : |1+
         oSprint est un ClSprint dynamique
         sInfoPoids est une chaîne
         sInfoSprint est une chaîne
         
         
         si zoTache.p_nPoids <> 0 ALORS
         	sInfoPoids = zoTache.p_nPoids + " points"
         sinon
         	sInfoPoids = "(Non déterminé)"
         FIN
         si zoTache.p_nNum_Sprint_Realisation <> 0 ALORS
         	oSprint <- ClSprint.GetObj(zoTache.p_nNum_Sprint_Realisation)
         	sInfoSprint = oSprint.Get_IdUnique() + " (" + oSprint.sSprint_Date() + ")"
         sinon
         	sInfoSprint = "(Non déterminé)"
         FIN
         
         
         LIB_InfosTache		= ChaîneConstruit([
         	%3 - %4 - Poids : %1 - Sprint : %2
         	], sInfoPoids, ...
         	sInfoSprint, ...
         	zoTache.p_sCodeTache, ...
         	zoTache.sLibDeStatut())
         
         LIB_DescriptioTache	= zoTache.p_sDescription
        type : 327692
