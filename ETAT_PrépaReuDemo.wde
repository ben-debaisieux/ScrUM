    code_elements :
     type_code : 30
     p_codes :
      -
        code : |1+
         oSp est un ClSprint dynamique
         nNumSprintDemarrage est un entier = zoUS.Get_DemarrageTache()
         sInfoSprint est une chaîne
         
         si nNumSprintDemarrage = 0 ALORS
         	sInfoSprint = "Démarrage non planifié"
         sinon
         	oSp <- ClSprint.GetObj(nNumSprintDemarrage)
         	sInfoSprint = "Démarrage : " + oSp.sSprint_Date()
         FIN
         
         LIB_InfoUS			= zoUS.sNommination_US()
         LIB_Etat_US			= "Statut : " + ClUserStory.sNomDeEtat(zoUS.Get_EtatUS())
         LIB_Demarrage_US	= sInfoSprint
         LIB_CodeUS			= "(" + zoUS.p_sCodeUserStory + ")"
         LIB_ChargeUS		= "Charge : " + zoUS.User_Story_Poids()
         LIB_CmtrUS			= zoUS.p_sCMTR_Global
        type : 327692
      -
        code : |1+
         STR_Statut est une structure
         	sLib est une chaîne
         	aStatuts est un tableau d'entiers
         fin
         
         stStatutTmp est un STR_Statut
         oTacheTmp est un ClTache dynamique
         bRamure est un booléen
         aListStauts est un tableau de STR_Statut
         
         //Chargement des statuts à traiter
         stStatutTmp.sLib = "A valider"
         stStatutTmp.aStatuts = [ClTache.cstStatut_realise]
         TableauAjoute(aListStauts, stStatutTmp)
         
         stStatutTmp.sLib = "A réaliser"
         stStatutTmp.aStatuts = [ClTache.cstStatut_a_coter, ...
         			ClTache.cstStatut_a_faire, ...
         			ClTache.cstStatut_en_cours]
         TableauAjoute(aListStauts, stStatutTmp)
         
         stStatutTmp.sLib = "Suspendues"
         stStatutTmp.aStatuts = [ClTache.cstStatut_Suspendu]
         TableauAjoute(aListStauts, stStatutTmp)
         
         stStatutTmp.sLib = "Annulées"
         stStatutTmp.aStatuts = [ClTache.cstStatut_Annulee]
         TableauAjoute(aListStauts, stStatutTmp)
         
         //Impression des taches, par statuts
         pour tout stStatutTmp de aListStauts
         	bRamure = faux
         	POUR TOUT ELEMENT oTacheTmp DE zoUS.aoListeTaches()
         		
         		SI TableauCherche(stStatutTmp.aStatuts, tcLinéaire, oTacheTmp.p_nStatut) <> -1 ALORS
         			
         			// Si on est sur le 1er item de ce statut, on affiche le statut
         			si pas bRamure ALORS
         				Bloc_Etat.LIB_SansNom = stStatutTmp.sLib
         				iImprimeBloc(Bloc_Etat)
         				bRamure = Vrai
         			FIN
         			zoTache		<- oTacheTmp
         			iImprimeBloc(Bloc_Tache)
         			
         			
         			si oTacheTmp.p_sCMTR <> "" ALORS
         				Bloc_Cmtr.LIB_CmtrTache		= "Commentaire : " + RC + oTacheTmp.p_sCMTR
         				iImprimeBloc(Bloc_Cmtr)
         			FIN
         
         			
         			si oTacheTmp.p_nStatut = ClTache.cstStatut_Suspendu ALORS
         				Bloc_Cmtr.LIB_CmtrTache = "Motif de blocage : " +  oTacheTmp.p_sCMTR_Blocage
         				iImprimeBloc(Bloc_Cmtr)
         			FIN
         		FIN
         	FIN
         FIN
         
        type : 327693
