       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1+
            oNewMes est un ClDbMessage(ClZF_ObjDbLs.cstModeCreation)
            
            SELON zoEntite..Classe
            	CAS "ClTache"
            		oNewMes.p_oTache = ClDbTache.GetObj(zoEntite.ZF_Get_IdUnique())
            		
            	CAS "ClUserStory"
            		oNewMes.p_oUserStory = ClDbTache.GetObj(zoEntite.ZF_Get_IdUnique())
            	
            	AUTRE CAS
            		ExceptionDéclenche(1, ChaîneConstruit("Type de message non prévu pour l'entité ""%1"".", zoEntite..Classe))
            FIN
            SI Ouvre(FEN_Message_Saisie,oNewMes) ALORS
            	ZoneRepete_Message()
            FIN
           type : 18
