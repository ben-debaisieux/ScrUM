            oNewMes est un ClMessage
            
            SELON zoEntite..Classe
            	CAS "ClTache"
            		oNewMes.p_oTache = ClTache.GetObj(zoEntite.Get_IdUnique())
            		
            	CAS "ClUserStory"
            		oNewMes.p_oUserStory = ClTache.GetObj(zoEntite.Get_IdUnique())
            	
            	AUTRE CAS
            		ExceptionDéclenche(1, ChaîneConstruit("Type de message non prévu pour l'entité ""%1"".", zoEntite..Classe))
