      nLigne		est un entier
      bRepondre	est un booléen
      //iImageMessage est une image
      nTaille		est un entier
      nInd		est un entier
      nHauteur	est un entier = 20
      nNbrMess	est un entier
      sFormatDte	est une chaîne = "JJ/MM/AAAA HH:mm"
      
      ZoneRépétéeSupprimeTout(ZR_Messages_UserStory)
      
      pour tout element oMsg_Tmp de ClMessage.aoListeMessage(zoEntite)
      	nNbrMess++
      	
      	LIB_Utilisateur..X								= LIB_Utilisateur..XInitial
      	LIB_Date..X										= LIB_Date..XInitial
      	LIB_Commentaire..X								= LIB_Commentaire..XInitial
      	nTaille											= 1 + ChaîneOccurrence(oMsg_Tmp.p_sCMTR, RC)
      	nLigne											= ZoneRépétéeAjouteLigne(ZR_Messages_UserStory)
      	
      	ZR_Messages_UserStory[nLigne]..Hauteur			= ZR_Messages_UserStory[nLigne]..Hauteur + (nTaille * nHauteur)
      	
      	ZR_Messages_UserStory[nLigne].ATT_Utilisateur	= oMsg_Tmp.p_oUsr.p_sNom
      	ZR_Messages_UserStory[nLigne].ATT_Date			= DateVersChaîne(oMsg_Tmp.p_dhDteCreation, sFormatDte)
      	ZR_Messages_UserStory[nLigne].ATT_Message		= oMsg_Tmp.p_sCMTR
      	ZR_Messages_UserStory[nLigne].ATT_IDMessage		= oMsg_Tmp.Get_IdUnique()
      	ZR_Messages_UserStory[nLigne].ATT_XUtilisateur	= LIB_Utilisateur..XInitial
      	ZR_Messages_UserStory[nLigne].ATT_XDate			= LIB_Date..XInitial
      	ZR_Messages_UserStory[nLigne].ATT_XMessage		= LIB_Commentaire..XInitial
      	ZR_Messages_UserStory[nLigne].ATT_Fleche		= Faux
      	ZR_Messages_UserStory[nLigne].ATT_BTN_Repondre	= Faux
      	ZR_Messages_UserStory[nLigne].ATT_taille		= nTaille * nHauteur
      	ZR_Messages_UserStory[nLigne].ATT_Id_Parent		= oMsg_Tmp.Get_IdUnique()
      	
      	ZR_Messages_UserStory[nLigne].ATT_Image_Visible = Faux
      	
      	nInd											= 0
      	
      	pour tout element oMsgEnf_Tmp de oMsg_Tmp.p_aoMsgEnfant
      		
      		nNbrMess++
      		
      		nInd++
      		
      		SI nInd = TableauOccurrence(oMsg_Tmp.p_aoMsgEnfant) ALORS
      			bRepondre = Vrai
      		SINON
      			bRepondre = Faux
      		FIN
      		
      		nTaille											= 1 + ChaîneOccurrence(oMsgEnf_Tmp.p_sCMTR,RC)
      		nLigne											= ZoneRépétéeAjouteLigne(ZR_Messages_UserStory)
      		ZR_Messages_UserStory[nLigne]..Hauteur			= ZR_Messages_UserStory[nLigne]..Hauteur + (nTaille * 15)
      		
      		ZR_Messages_UserStory[nLigne].ATT_Utilisateur	= oMsgEnf_Tmp.p_oUsr.p_sNom
      		ZR_Messages_UserStory[nLigne].ATT_Date			= DateVersChaîne(oMsgEnf_Tmp.p_dhDteCreation, sFormatDte)
      		ZR_Messages_UserStory[nLigne].ATT_Message		= oMsgEnf_Tmp.p_sCMTR
      		ZR_Messages_UserStory[nLigne].ATT_IDMessage		= oMsgEnf_Tmp.Get_IdUnique()
      		ZR_Messages_UserStory[nLigne].ATT_XUtilisateur	= LIB_Utilisateur..XInitial + 30
      		ZR_Messages_UserStory[nLigne].ATT_XDate			= LIB_Date..XInitial + 30
      		ZR_Messages_UserStory[nLigne].ATT_XMessage		= LIB_Commentaire..XInitial + 30
      		ZR_Messages_UserStory[nLigne].ATT_Fleche		= Vrai
      		ZR_Messages_UserStory[nLigne].ATT_BTN_Repondre	= bRepondre
      		ZR_Messages_UserStory[nLigne].ATT_taille		= nTaille * nHauteur
      		ZR_Messages_UserStory[nLigne].ATT_Id_Parent		= oMsg_Tmp.Get_IdUnique()
      //
      //		SI aMessagesFils[i].iImage.Valide = Faux ALORS
      			ZR_Messages_UserStory[nLigne].ATT_Image_Visible = Faux
      //		SINON
      //			
      //			ZR_Messages_UserStory[nLigne].ATT_Image = aMessagesFils[i].iImage
      //			ZR_Messages_UserStory[nLigne].ATT_XImage = IMG_Image..XInitial + 30
      //			ZR_Messages_UserStory[nLigne].ATT_Image_Visible = Vrai
      //		FIN
      	FIN
      		
      	ZR_Messages_UserStory[nLigne].ATT_BTN_Repondre = Vrai
      	
      	
      FIN
      
      
      renvoyer nNbrMess
