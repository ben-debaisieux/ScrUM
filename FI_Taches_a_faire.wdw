  procedures :
   -
     name : Actualise
     procedure_id : 2205713445699068901
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Actualise ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      // 	bForceActu_p (booléen - valeur par défaut=0) : <indiquez ici le rôle de bForceActu_p>
      PROCEDURE Actualise()
      
      oTache est un ClTache dynamique
      nNumLigne est un entier
      
      TableSupprimeTout(TBL_A_faire)
      
      SI goApp.m_oUsrCnx.p_bTop_Equip_Informatique ALORS
      	
      	pour tout element oTache de ClTache.Taches_a_faire(ClTache.PasseEtActuelle)
      		
      		//Affichage des taches non attribuées, ou qui sont attribuées à l'utilisateur connecté.
      		si oTache.p_oUsr_Dev = null _ou_ oTache.p_oUsr_Dev.Get_IdUnique() = goapp.m_oUsrCnx.Get_IdUnique() et oTache.p_nStatut <> ClTache.cstStatut_Suspendu ALORS
      			
      			nNumLigne = TableAjouteLigne(TBL_A_faire)
      			TBL_A_faire[nNumLigne].COL_IdTache		= oTache.Get_IdUnique()
      			TBL_A_faire[nNumLigne].COL_Tache		= oTache.p_sDescription
      			TBL_A_faire[nNumLigne].COL_Tache..Bulle	= oTache.sBulleTache()
      			TBL_A_faire[nNumLigne].COL_Projet		= oTache.p_oUserStoryParent.p_oProjetParent.p_sNom
      			TBL_A_faire[nNumLigne].COL_SprintReal	= oTache.p_nNum_Sprint_Realisation
      			
      			//Mise en évidence des taches qui commencent à etres en retard.
      			selon (goapp.m_nNumeroDuSprintActuel - oTache.p_nNum_Sprint_Realisation)
      				cas 0
      					//Ras : on est pas en retard
      				cas 1
      					//En retard d'un sprint
      					TBL_A_faire[nNumLigne].COL_Tache..CouleurFond	= OrangeClair
      				cas > 1
      					//Fortement en retard
      					TBL_A_faire[nNumLigne].COL_Tache..CouleurFond	= OrangeClair
      					TBL_A_faire[nNumLigne].COL_Tache..PoliceGras	= Vrai
      					
      				AUTRE CAS
      					
      			FIN
      		FIN
      	fin
      fin
      
      TableTrie(TBL_A_faire,TBL_A_faire.COL_Projet..nom,TBL_A_faire.COL_SprintReal..Nom,TBL_A_faire.COL_Tache..Nom)
      
      
      nInd est un entier
      pour nInd = 1 _a_ TableOccurrence(TBL_A_faire)
      	TBL_A_faire[TableIndiceRupture(RUPT_HautRupture1,nInd)].LIB_NomProjet = TBL_A_faire[nInd].COL_Projet
      FIN
     type : 458752
