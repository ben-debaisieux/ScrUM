         -
           code : |1+
            SI TableOccurrence(TBL_A_faire) >= 1 ALORS
            	oTache est un ClDbTache <- ClDbTache.GetObj(TBL_A_faire.COL_IdTache)
            	SI OuiNon(Non, "Confirmez-vous le démarrage de la tache :" + RC + oTache.p_sDescription) ALORS
