  p_codes :
   -
     code : |1+
      PROCEDURE MaFenêtre()
      
      STR_TacheSuspendue est une structure
      	nIdTache est un entier
      	sNomProjet est une chaîne
      	sUS est une chaîne
      	sInfoUS est une chaîne
      	sTache est une chaîne
      	sCmtr est une chaîne
      	nSprintReal est un entier
      	nPoids est un entier
      	nNbDocs est un entier
      fin
      
      STR_UsrTache est une structure
      	oUsr est un ClUsr
      	aLstTache est un tableau d'entiers
      fin
      
      
      zaLstTaches est un tableau de STR_TacheSuspendue
      zaTachesDeUsr est un tableau associatif de STR_UsrTache
      
      
      ChargeTachesSuspendues()
   -
     code : |1+
      AffTaches()
      
