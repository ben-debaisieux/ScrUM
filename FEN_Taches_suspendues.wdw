  procedures :
   -
     name : AffTaches
     procedure_id : 1307858270814243914
     type_code : 14
     code : |1+
      procédure AffTaches()
      
      stTacheTmp	est un STR_TacheSuspendue
      aLst est un tableau d'entiers
      nId est un entier
      
      TBL_taches..AffichageActif = Faux
      
      TableSupprimeTout(TBL_taches)
      
      
      SI CBO_ChefDeProjet..ValeurMémorisée = 0 ALORS
      	POUR TOUT ELÉMENT stTacheTmp DE zaLstTaches
      		AddLigne(stTacheTmp)
      	FIN
      SINON
      	aLst = zaTachesDeUsr[CBO_ChefDeProjet..ValeurMémorisée].aLstTache
      	POUR TOUT nId DE aLst
      		AddLigne(zaLstTaches[nId])
      	FIN
      FIN
      
      TableTrie(TBL_taches, TBL_taches.COL_Projet..Nom,TBL_taches.COL_US..Nom,TBL_taches.COL_Tache..Nom)
      TableSelectPlus(TBL_taches,1)
      
      TBL_taches..AffichageActif = Vrai
      
      
      
      PROCÉDURE INTERNE AddLigne(stTache_P est un STR_TacheSuspendue)
      	nNumLigne est un entier
      	nNumLigne									= TableAjouteLigne(TBL_taches)
      	TBL_taches[nNumLigne].COL_idTache			= stTache_P.nIdTache
      	TBL_taches[nNumLigne].COL_Projet			= stTache_P.sNomProjet
      	TBL_taches[nNumLigne].COL_US				= stTache_P.sUS
      	TBL_taches[nNumLigne].COL_US..Bulle			= stTache_P.sInfoUS
      	TBL_taches[nNumLigne].COL_Tache				= stTache_P.sTache
      	TBL_taches[nNumLigne].COL_Cmtr				= stTache_P.sCmtr
      	TBL_taches[nNumLigne].COL_Sprint			= stTache_P.nSprintReal
      	TBL_taches[nNumLigne].COL_Poids				= stTache_P.nPoids
      	TBL_taches[nNumLigne].COL_Docs				= stTache_P.nNbDocs
      	
      	SI 1 <= stTacheTmp.nSprintReal <= goApp.m_nNumeroDuSprintActuel ALORS
      		TBL_taches[nNumLigne].COL_Sprint..CouleurFond = COL_Scrum.gnCouleur_OrangeVif
      	FIN
      FIN
      
     type : 458752
   -
     name : ChargeTachesSuspendues
     procedure_id : 1307858696016039885
     type_code : 14
     code : |1+
      procédure ChargeTachesSuspendues()
      
      stTacheTmp	est un STR_TacheSuspendue
      oPrj		est un ClProjet dynamique
      oUs			est un ClUserStory dynamique
      oUsr		est un ClUsr dynamique
      oTache_Tmp	est un ClTache dynamique
      nLigne		est un entier
      
      
      POUR TOUT ELÉMENT oPrj DE ClProjet.aoListeProjets()
      	POUR TOUT ELÉMENT oUs DE oPrj.aoListeUS()
      		POUR TOUT ELÉMENT oTache_Tmp DE oUs.aoListeTaches()
      			SI oTache_Tmp.p_nStatut = ClTache.cstStatut_Suspendu ALORS
      				
      				VariableRAZ(stTacheTmp)
      				stTacheTmp.nIdTache		= oTache_Tmp.Get_IdUnique()
      				stTacheTmp.sNomProjet	= oUs.p_oProjetParent.p_sNom
      				stTacheTmp.sUS			= oUs.p_sCodeUserStory +  " - " + oUs.sNommination_US()
      				stTacheTmp.sInfoUS		= oUs.p_sCMTR_Global
      				stTacheTmp.sTache		= oTache_Tmp.p_sCodeTache + " - " + oTache_Tmp.p_sDescription
      				stTacheTmp.sCmtr		= oTache_Tmp.p_sCMTR
      				stTacheTmp.nSprintReal	= oTache_Tmp.p_nNum_Sprint_Realisation
      				stTacheTmp.nPoids		= oTache_Tmp.p_nPoids
      				stTacheTmp.nNbDocs		= Dimension(oUs.asCheminsDocs())
      				nLigne = TableauAjouteLigne(zaLstTaches,stTacheTmp)
      				
      				add_tache_usr(oPrj.p_oUsr_ChefProjet, nLigne)
      				
      				POUR TOUT ELÉMENT oUsr DE oPrj.aoInvite_Liste()
      					add_tache_usr(oUsr, nLigne)
      				FIN
      				
      			FIN
      		FIN
      	FIN
      FIN
      
      
      	PROCÉDURE INTERNE add_tache_usr(oUsr_add est un ClUsr, nLigneTache)
      		
      		nIdUsr est un entier = oUsr_add.Get_IdUnique()
      		SI zaTachesDeUsr[nIdUsr]..Vide ALORS
      			zaTachesDeUsr[nIdUsr] = allouer un tableau de STR_UsrTache
      			zaTachesDeUsr[nIdUsr].oUsr = oUsr_add
      		FIN
      		
      		TableauAjoute(zaTachesDeUsr[nIdUsr].aLstTache, nLigneTache)
      		
      	FIN
     type : 458752
