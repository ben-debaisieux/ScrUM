     name : sRubSqlDeTypeHF
     procedure_id : 1474468380170634330
     type_code : 12
     code : |1+
      procedure sRubSqlDeTypeHF(stRub_p est un ClZF_GenCodeFic.STR_DefRub)<metier>:chaine
      
      sRetour est une chaîne
      selon stRub_p.nTypeRubHF
      	CAS hRubBinaire
      		sRetour = "BINARY"
      	CAS hRubBooléen
      		sRetour = "BIT"
      	CAS hRubCaractère
      		sRetour = "CHAR(" + stRub_p.nTaille + ")"
      	CAS hRubDate8
      		sRetour = "DATE"			//*
      	CAS hRubDateHeure
      		sRetour = "Datetime2"		//*
      	CAS hRubDurée
      		sRetour = "Datetime2"		//*
      	CAS hRubEntier1
      		sRetour = "smallint"
      	CAS hRubEntier2
      		sRetour = "smallint"
      	CAS hRubEntier4
      		sRetour = "int"
      	CAS hRubEntier8
      		sRetour = "bigint"
      	CAS hRubEntierNonSigné1
      		sRetour = "tinyint"
      	CAS hRubEntierNonSigné2
      		sRetour = "INT"
      	CAS hRubEntierNonSigné4
      		sRetour = "int"
      	CAS hRubEntierNonSigné8
      		sRetour = "bigint"
      	CAS hRubHeure
      		sRetour = "Time(0)"
      	CAS hRubIDAuto
      		sRetour= "bigint"
      	CAS hRubIDAuto4
      		sRetour = "int"
      	CAS hRubImage
      		sRetour = "IMAGE"
      	CAS hRubMémoBinaire
      		sRetour = "VARBINARY(max)"
      	CAS hRubMémoBinaire4
      		sRetour = "VARBINARY(max)"
      	CAS hRubMémoTexte
      		sRetour = "Text"
      	CAS hRubMémoUnicode
      		sRetour = "nText"
      	CAS hRubMonétaire
      		sRetour = "Money"
      	CAS hRubNumérique
      		sRetour = "DECIMAL ([%stRub_p.nTaille%], [%stRub_p.nScale%])"
      	CAS hRubRéel4
      		sRetour = "Real"
      	CAS hRubRéel8
      		sRetour = "Float"
      	CAS hRubRéelTurbo
      		sRetour = "Real"
      	CAS hRubTexte
      		sRetour = "Varchar(" + stRub_p.nTaille + ")"
      	CAS hRubTexteUnicode
      		sRetour = "nVarchar(" + stRub_p.nTaille + ")"
      	AUTRE CAS
      		ClZF_App.mg_oExSelonCasNonTraite.ZF_Declenche([stRub_p.nTypeRubHF])
      FIN
      
      renvoyer sRetour
