     name : sRubHFDeTypeSql
     procedure_id : 1614354722022641761
     type_code : 12
     code : |1+
      // Redéfinition de la méthode CLZF_GenData.sRubHFDeTypeSql
      PROCÉDURE  sRubHFDeTypeSql(sType_p est une chaîne)<métier>:entier
      
      
      nTypeRetour est un entier
      
      SELON Minuscule(sType_p)
      	CAS "bit" :
      		nTypeRetour	= hRubBooléen
      	CAS "date" :
      		nTypeRetour	= hRubDate8
      	CAS "datetime" :
      		nTypeRetour	= hRubDate8
      	CAS "decimal", "numeric" :
      		nTypeRetour	= hRubNumérique
      	CAS "int" :
      		nTypeRetour	= hRubEntier4
      	CAS "money" :
      		nTypeRetour	= hRubMonétaire
      	CAS "nchar" :
      		nTypeRetour	= hRubTexteUnicode
      	CAS "nvarchar" :
      		nTypeRetour	= hRubTexteUnicode
      	CAS "smallint" :
      		nTypeRetour	= hRubEntier2
      	CAS "smallmoney" :
      		nTypeRetour	= hRubMonétaire
      	CAS "time" :
      		nTypeRetour	= hRubHeure
      	CAS "tinyint" :
      		nTypeRetour	= hRubEntierNonSigné1
      	CAS "uniqueidentifier" :
      		nTypeRetour	= hRubUUID128
      	CAS "varbinary" :
      		nTypeRetour	= hRubBinaire
      	CAS "varchar" :
      		nTypeRetour	= hRubTexte
      	CAS "xml" :
      		nTypeRetour	= hRubMémoTexte
      	
      	CAS "null" :
      		nTypeRetour	= hRubBinaire		//Pour les cas non identifiés, on les mets en binaires
      		
      	AUTRE CAS
      		ClZF_App.mg_oExSelonCasNonTraite.ZF_Declenche([sType_p])
      FIN
      
      RENVOYER nTypeRetour
