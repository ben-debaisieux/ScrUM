     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // ChargeCbo ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE ChargeCbo()
      
      
      nNumSprintActuel est un entier = goApp.m_nNumeroDuSprintActuel
      nNumSprintSel est un entier = CBO_Sprint..ValeurMémorisée
      oTache_tmp est un ClTache dynamique
      nNumSprint est un entier
      anLstSprint est un tableau d'entiers
      
      TableauAjoute(anLstSprint,nNumSprintActuel)
      
      POUR TOUT ELEMENT oTache_tmp DE zaoTaches 
      	SI TableauCherche(anLstSprint,tcLinéaire,oTache_tmp.p_nNum_Sprint_Previsionnel) = -1 et oTache_tmp.p_nNum_Sprint_Previsionnel <> 0 ALORS
      		TableauAjoute(anLstSprint,oTache_tmp.p_nNum_Sprint_Previsionnel)
      	FIN
      FIN
      
      TableauTrie(anLstSprint)
      
      
      
      ListeSupprimeTout(CBO_Sprint)
      //Ajout du choix Tous les sprints
      ListeAjoute(CBO_Sprint,"<Tous>" + gLien(0))
      ListeAjoute(CBO_Sprint,"<Futur>" + gLien(zcst_sprint_futur))
      ListeAjoute(CBO_Sprint,"<Sans sprint>" + gLien(zcst_sprint_sans))
      POUR TOUT nNumSprint DE anLstSprint
      	ListeAjoute(CBO_Sprint, "Sprint " + nNumSprint + gLien(nNumSprint))
      FIN
      
      
      
      nIndiceSprintActuel est un entier
      nIndiceSprintActuel = ListeCherche(CBO_Sprint, nNumSprintSel)
      SI nIndiceSprintActuel = -1 ALORS
      	nIndiceSprintActuel = ListeCherche(CBO_Sprint, nNumSprintActuel)
      	SI nIndiceSprintActuel = -1 ALORS
      		ListeSelectPlus(CBO_Sprint,1)
      	SINON
      		ListeSelectPlus(CBO_Sprint,nIndiceSprintActuel)
      	FIN
      SINON
      	ListeSelectPlus(CBO_Sprint,nIndiceSprintActuel)
      FIN
      //ExécuteTraitement(CBO_Sprint,trtSélection)
      
      
      
      AppFilte()
