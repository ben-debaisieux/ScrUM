      -
        code : |1+
         oTache_tmp est un ClTache dynamique
         nIndTbl est un entier
         nInd est un entier
         bErreur est un booléen
         	oSpr est un ClSprint dynamique
         
         
         Pour toute ligne nInd de TBL_Taches
         	nIndTbl = TBL_Taches[nInd].COL_ID
         	oTache_tmp <- zaoTaches[nIndTbl]
         	
         	oTache_tmp.p_nPoids						= oTache_tmp.p_nPoids_Previsionnel
         	oTache_tmp.p_nNum_Sprint_Realisation	= oTache_tmp.p_nNum_Sprint_Previsionnel
         	quand exception dans
         		oTache_tmp.Enregistre()
         	FAIRE
         		erreur("Tache : " + oTache_tmp.p_sCodeTache + rc + ExceptionInfo(errMessage))
         		sortir
         	FIN
         FIN
         
         si pas bErreur ALORS
         	oSpr <- ClSprint.GetObj(goApp.m_nNumeroDuSprintActuel)
         	oSpr.p_bTop_Termine = vrai
         	
         	quand exception dans
         		oSpr.Enregistre()
         	FAIRE
         		erreur(ExceptionInfo(errMessage))
         		bErreur = vrai
         	FIN
         FIN
         	
         SI PAS bErreur ALORS
         	goApp.m_nNumeroDuSprintActuel++ 
         	oSpr <- ClSprint.GetObj(goApp.m_nNumeroDuSprintActuel + 1)
         	Info("Le Sprint " + NumériqueVersChaîne(oSpr.Get_IdUnique()) + " a bien été initialisé." + rc + "Dates : " + oSpr.sSprint_Date())
         	Ferme()
         FIN
