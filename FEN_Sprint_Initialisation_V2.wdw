      PROCEDURE GestAffLigne()
      
      
      SI TBL_Taches.COL_Poids <> TBL_Taches.COL_PoidsPrevisionnel ALORS
      	TBL_Taches[TBL_Taches].COL_Poids..CouleurFond	= JauneClair
      SINON
      	TBL_Taches[TBL_Taches].COL_Poids..CouleurFond	= iTransparent
      FIN
      SI TBL_Taches.COL_Sprint <> TBL_Taches.COL_SprintPrevisionnel ALORS
      	TBL_Taches[TBL_Taches].COL_Sprint..CouleurFond	= JauneClair
      SINON
      	TBL_Taches[TBL_Taches].COL_Sprint..CouleurFond	= iTransparent
      FIN
