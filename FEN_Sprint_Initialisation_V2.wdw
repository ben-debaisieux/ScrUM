     name : ListeTaches
     procedure_id : 2212460186108890168
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //ListeTaches ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      //	nNumSprint_p (entier) : <indiquez ici le rôle de nnumSprint>
      PROCEDURE ListeTaches()<UI>:vide
      
      nLigne		est un entier
      oTache_tmp	est un ClDbTache dynamique
      sGroupement	est une chaîne
      oSpReel		est un ClDbSprint dynamique
      oSpPrev		est un ClDbSprint dynamique
      nIdSel		est un entier = TBL_Taches.COL_IDTache
      nLigneSel	est un entier
      
      TableSupprimeTout(TBL_Taches)
      TBL_Taches..AffichageActif = faux
      
      
      POUR TOUT element oTache_tmp de zOTaches.aItems()
      	
      	si oTache_tmp.p_oUserStoryParent.p_oIDScr_Epic = null ALORS
      		sGroupement = ""
      	sinon
      		sGroupement = oTache_tmp.p_oUserStoryParent.p_oIDScr_Epic.p_sNom
      	FIN
      	
      	si oTache_tmp.p_nNum_Sprint_Realisation > 0 ALORS
      		oSpReel <- ClDbSprint.GetObj(oTache_tmp.p_nNum_Sprint_Realisation)
      	sinon
      		oSpReel = null
      	fin
      	
      	si oTache_tmp.p_nNum_Sprint_Previsionnel > 0 alors
      		oSpPrev <- ClDbSprint.GetObj(oTache_tmp.p_nNum_Sprint_Previsionnel)
      	sinon
      		oSpPrev = null
      	fin
      	
      	
      	nLigne										= TableAjouteLigne(TBL_Taches)
      	TBL_Taches[nLigne].COL_IDTache				= oTache_tmp.Get_IdUnique()
      	TBL_Taches[nLigne].COL_Code					= oTache_tmp.p_sCodeTache
      	
      	TBL_Taches[nLigne].COL_Grp					= sGroupement
      	TBL_Taches[nLigne].COL_Description			= oTache_tmp.p_sDescription
      	TBL_Taches[nLigne].COL_Description..Bulle	= oTache_tmp.sBulleTache()
      	TBL_Taches[nLigne].COL_Poids				= oTache_tmp.p_nPoids
      	TBL_Taches[nLigne].COL_PoidsPrevisionnel	= oTache_tmp.p_nPoids_Previsionnel
      	
      	COL_Charge_UI.col_tbl_sprint(TBL_Taches.COL_Sprint..NomComplet, nLigne, oSpReel)
      	COL_Charge_UI.col_tbl_sprint(TBL_Taches.COL_SprintPrevisionnel..NomComplet, nLigne, oSpPrev)
      	
      	TBL_Taches[nLigne].COL_Projet				= oTache_tmp.p_oUserStoryParent.p_oIDScr_Projet.p_sNom
      	TBL_Taches[nLigne].COL_Statut				= oTache_tmp.sLibDeStatut()
      	
      	selon oTache_tmp.p_nStatut
      		cas ClDbTache.cstStatut_Brouillon, ClDbTache.cstStatut_Annulee, ClDbTache.cstStatut_Suspendu
      			TBL_Taches[nLigne].COL_Description..Police..Italique	= vrai
      			TBL_Taches[nLigne].COL_Description..Couleur				= COL_Scrum.gnCouleur_Gris_TFonce
      		cas ClDbTache.cstStatut_a_coter
      			TBL_Taches[nLigne].COL_Poids..CouleurFond				= COL_Scrum.gnCouleur_JauneVif
      		AUTRE CAS
      			
      	FIN
      	
      	si nIdSel = oTache_tmp.Get_IdUnique() ALORS
      		nLigneSel = nLigne
      	FIN
      	
      FIN
      
      SI TableOccurrence(TBL_Taches) > 0 ALORS
      	TableSelectPlus(TBL_Taches, Max(1, nLigneSel))
      FIN
      
      TBL_Taches..AffichageActif = Vrai
      
      
      	
