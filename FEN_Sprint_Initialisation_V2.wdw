         	FAIRE
         		erreur("Tache : " + oTache_tmp.p_sCodeTache + rc + ExceptionInfo(errMessage))
         		sortir
         	FIN
         FIN
         
         si pas bErreur ALORS
         	oSpr <- ClSprint.GetObj(goApp.m_nNumeroDuSprintActuel)
         	oSpr.p_bTop_Termine = vrai
         	
         	quand exception dans
         		oSpr.Enregistre()
         	FAIRE
         		erreur(ExceptionInfo(errMessage))
         		bErreur = vrai
         	FIN
         FIN
         	
         SI PAS bErreur ALORS
         	goApp.m_nNumeroDuSprintActuel++ 
         	oSpr <- ClSprint.GetObj(goApp.m_nNumeroDuSprintActuel)
         	Info("Le Sprint " + NumériqueVersChaîne(oSpr.Get_IdUnique()) + " a bien été initialisé." + rc + "Dates : " + oSpr.sSprint_Date())
         	Ferme()
         FIN
