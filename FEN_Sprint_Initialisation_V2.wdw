   -
     name : PoidsTotal_sur_VelociteMoyenne
     procedure_id : 2212460186108955704
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // PoidsTotal_sur_VelociteMoyenne ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE PoidsTotal_sur_VelociteMoyenne()<ui>:Vide
      
      STR_Grp est une structure
      	nCouleur		est un entier
      	nChargeReele	est un entier
      	nChargePrevi	est un entier
      fin
      
      nPoidsTotalPrev		est un entier	= 0
      nPoidsTotalReel		est un entier	= 0
      nVelociteMoyenne	est un entier	= goApp.m_nVelociteMoyenne
      oTache_tmp			est un ClDbTache dynamique
      nSprintRef			est un entier
      sGrp				est une chaîne
      aNCoulGrp			est un tableau associatif de STR_Grp
      stInfoGrp			est un STR_Grp
      
      
      //Si on est sur 'tous les sprints', on prend le sprint en cours
      si CBO_Sprint..ValeurMémorisée <= 0 ALORS
      	nSprintRef = goApp.m_nNumeroDuSprintActuel
      sinon
      	nSprintRef = CBO_Sprint..ValeurMémorisée
      FIN
      
      //On cumule tous les poids prévisionnel du sprint retenu
      POUR TOUT ELEMENT oTache_tmp DE zoTaches.aItems()
      	
      	sGrp = oTache_tmp.p_oUserStoryParent.p_oIDScr_Projet.p_sGroupement
      	SI aNCoulGrp[sGrp]..Vide ALORS
      		variableraz(stInfoGrp)
      		stInfoGrp.nCouleur		= DonneCouleur()
      		aNCoulGrp[sGrp]			= allouer un STR_Grp
      	sinon
      		stInfoGrp				= aNCoulGrp[sGrp]
      	FIN
      	
      	si nSprintRef = oTache_tmp.p_nNum_Sprint_Previsionnel alors
      		nPoidsTotalPrev 		+= oTache_tmp.p_nPoids_Previsionnel
      		stInfoGrp.nChargePrevi	+= oTache_tmp.p_nPoids_Previsionnel
      	fin
      	
      	si nSprintRef = oTache_tmp.p_nNum_Sprint_Realisation alors
      		nPoidsTotalReel			+= oTache_tmp.p_nPoids
      		
      		stInfoGrp.nChargeReele	+= oTache_tmp.p_nPoids
      	fin
      	
      	aNCoulGrp[sGrp] = stInfoGrp
      FIN
      
      TableSupprimeTout(TBL_grp)
      nLigne est un entier
      pour tout stInfoGrp, sGrp de aNCoulGrp
      	nLigne									= TableAjouteLigne(TBL_grp)
      	TBL_grp.COL_Groupe[nLigne]				= sGrp
      	TBL_grp.COL_ChargeReel[nLigne]			= stInfoGrp.nChargeReele
      	TBL_grp.COL_ChargePrev[nLigne]			= stInfoGrp.nChargePrevi
      	si nVelociteMoyenne > 0 alors
      		TBL_grp.COL_pct_absReel[nLigne]		= stInfoGrp.nChargeReele / nVelociteMoyenne
      		TBL_grp.COL_pct_absPrev[nLigne]		= stInfoGrp.nChargePrevi / nVelociteMoyenne
      	fin
      	si nPoidsTotalReel > 0 alors
      		TBL_grp.COL_pct_saisieReel[nLigne]	= stInfoGrp.nChargeReele / nPoidsTotalReel
      		TBL_grp.COL_pct_saisiePrev[nLigne]	= stInfoGrp.nChargePrevi / nPoidsTotalPrev
      	fin
      FIN
      
      //On actualise l'affichage de la jauge
      LIB_Poids_Velocité_moyenne_prev..Couleur	= Noir
      LIB_Poids_Velocite_moyenne_reel..Couleur	= Noir
      
      JAUGE_charge_prev..CouleurJauge				= VertFoncé
      JAUGE_ChargeReel..CouleurJauge				= VertFoncé
      
      LIB_Poids_Velocité_moyenne_prev				= "Poids prev. / Vélocité Moyenne : [%nPoidsTotalPrev%]/[%nVelociteMoyenne%]"
      LIB_Poids_Velocite_moyenne_reel				= "Poids réel / Vélocité Moyenne : [%nPoidsTotalReel%]/[%nVelociteMoyenne%]"
      
      JAUGE_charge_prev							= nPoidsTotalPrev
      JAUGE_ChargeReel							= nPoidsTotalReel
      
      
      SI nPoidsTotalPrev > nVelociteMoyenne alors
      	LIB_Poids_Velocité_moyenne_prev..Couleur	= COL_Scrum.gnCouleur_Rougeclair
      	JAUGE_charge_prev..CouleurJauge				= COL_Scrum.gnCouleur_Rougeclair
      FIN
      SI nPoidsTotalReel > nVelociteMoyenne alors
      	LIB_Poids_Velocite_moyenne_reel..Couleur	= COL_Scrum.gnCouleur_Rougeclair
      	JAUGE_ChargeReel..CouleurJauge				= COL_Scrum.gnCouleur_Rougeclair
      FIN
      
      
      
      si nSprintRef = (goapp.m_nNumeroDuSprintActuel + 1) et SEL_TypeSprint = 1 ALORS
      	BTN_Valider..Visible = vrai
      sinon
      	BTN_Valider..Visible = faux
      FIN
     type : 458752
   -
     name : ChargeCbo
     procedure_id : 2212517180344993832
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // ChargeCbo ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE ChargeCbo()<UI>:Vide
      
      
      nNumSprintActuel	est un entier	= goApp.m_nNumeroDuSprintActuel
      nNumSprintSel		est un entier	= CBO_Sprint..ValeurMémorisée
      oTache_tmp			est un ClDbTache dynamique
      nNumSprint			est un entier
      anLstSprint			est un tableau d'entiers
      
      TableauAjoute(anLstSprint,nNumSprintActuel)
      
      POUR TOUT ELEMENT oTache_tmp DE zoTaches.aItems()
      	SI TableauCherche(anLstSprint,tcLinéaire,oTache_tmp.p_nNum_Sprint_Previsionnel) = -1 et oTache_tmp.p_nNum_Sprint_Previsionnel <> 0 ALORS
      		TableauAjoute(anLstSprint,oTache_tmp.p_nNum_Sprint_Previsionnel)
      	FIN
      FIN
      
      TableauTrie(anLstSprint)
      
      
      
      ListeSupprimeTout(CBO_Sprint)
      //Ajout du choix Tous les sprints
      ListeAjoute(CBO_Sprint,"<Tous>" + gLien(0))
      ListeAjoute(CBO_Sprint,"<Futur>" + gLien(zcst_sprint_futur))
      ListeAjoute(CBO_Sprint,"<Sans sprint>" + gLien(zcst_sprint_sans))
      POUR TOUT nNumSprint DE anLstSprint
      	ListeAjoute(CBO_Sprint, "Sprint " + nNumSprint + gLien(nNumSprint))
      FIN
      
      
      
      nIndiceSprintActuel est un entier
      nIndiceSprintActuel = ListeCherche(CBO_Sprint, nNumSprintSel)
      SI nIndiceSprintActuel = -1 ALORS
      	nIndiceSprintActuel = ListeCherche(CBO_Sprint, nNumSprintActuel)
      	SI nIndiceSprintActuel = -1 ALORS
      		ListeSelectPlus(CBO_Sprint,1)
      	SINON
      		ListeSelectPlus(CBO_Sprint,nIndiceSprintActuel)
      	FIN
      SINON
      	ListeSelectPlus(CBO_Sprint,nIndiceSprintActuel)
      FIN
      
     type : 458752
   -
     name : AppFiltre
     procedure_id : 2212520929857887175
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //AppFiltre ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE AppFiltre()<ui>:Vide
      
      sNomColSprintFiltre est une chaîne
      
      TBL_Taches..AffichageActif = faux
      
      TableDésactiveFiltre(TBL_Taches.COL_SprintPrevisionnel)
      TableDésactiveFiltre(TBL_Taches.COL_Sprint)
      
      selon SEL_TypeSprint
      	// Sp. prévi
      	CAS 1
      		sNomColSprintFiltre = TBL_Taches.COL_SprintPrevisionnel..NomComplet
      	// Sp réal
      	CAS 2
      		sNomColSprintFiltre = TBL_Taches.COL_Sprint..NomComplet
      FIN
      
      selon CBO_Sprint..ValeurMémorisée
      	cas 0
      	cas zcst_sprint_futur
      		TableActiveFiltre(sNomColSprintFiltre,filtreSupérieur,goApp.p_nNumeroDuSprintActuel)
      	cas zcst_sprint_sans
      		TableActiveFiltre(sNomColSprintFiltre,filtreEgal,"")
      	AUTRE CAS
      		TableActiveFiltre(sNomColSprintFiltre,filtreEgal,CBO_Sprint)
      FIN
      
      TableTrie(TBL_Taches,TBL_Taches.COL_Sprint..Nom,TBL_Taches.COL_Projet..Nom,TBL_Taches.COL_Code..Nom)
      
      
      PoidsTotal_sur_VelociteMoyenne()
      
      
      TBL_Taches..AffichageActif = vrai
     type : 458752
