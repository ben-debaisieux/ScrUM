      PROCEDURE AppFilte()
      
      
      selon CBO_Sprint..ValeurMémorisée
      	cas 0
      		TableDésactiveFiltre(TBL_Taches.COL_SprintPrevisionnel)
      	cas zcst_sprint_futur
      		TableActiveFiltre(TBL_Taches.COL_SprintPrevisionnel,filtreSupérieur,goApp.Sprint_Num_Actuel())
      	cas zcst_sprint_sans
      		TableActiveFiltre(TBL_Taches.COL_SprintPrevisionnel,filtreEgal,0)
      	AUTRE CAS
      		TableActiveFiltre(TBL_Taches.COL_SprintPrevisionnel,filtreEgal,CBO_Sprint)
      FIN
      
      TableTrie(TBL_Taches,TBL_Taches.COL_Sprint..Nom,TBL_Taches.COL_Projet..Nom)
      
      
