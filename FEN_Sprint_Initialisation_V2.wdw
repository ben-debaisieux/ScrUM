  -
    name : BTN_Valider
    identifier : 0x2eb43cd140c50c38
    internal_properties : 
    properties :
     x : 692
     y : 6
     width : 100
     height : 22
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         oTache_tmp	est un ClDbTache dynamique
         nInd		est un entier
         bErreur		est un booléen
         oSpr		est un ClDbSprint dynamique
         
         
         Pour toute ligne nInd de TBL_Taches
         	oTache_tmp <- ClDbTache.GetObj(TBL_Taches[nInd].COL_IDTache)
         	
         	oTache_tmp.p_nPoids						= oTache_tmp.p_nPoids_Previsionnel
         	oTache_tmp.p_nNum_Sprint_Realisation	= oTache_tmp.p_nNum_Sprint_Previsionnel
         	quand exception dans
         		oTache_tmp.ZF_Enregistre()
         	FAIRE
         		erreur("Tache : " + oTache_tmp.p_sCodeTache + rc + ExceptionInfo(errMessage))
         		sortir
         	FIN
         FIN
         
         si pas bErreur ALORS
         	oSpr <- ClDbSprint.GetObj(goApp.m_nNumeroDuSprintActuel)
         	oSpr.p_bTop_Termine = vrai
         	
         	quand exception dans
         		oSpr.ZF_Enregistre()
         	FAIRE
         		erreur(ExceptionInfo(errMessage))
         		bErreur = vrai
         	FIN
         FIN
         	
         SI PAS bErreur ALORS
         	goApp.m_nNumeroDuSprintActuel++ 
         	oSpr <- ClDbSprint.GetObj(goApp.m_nNumeroDuSprintActuel)
         	Info("Le Sprint " + NumériqueVersChaîne(oSpr.ZF_Get_IdUnique()) + " a bien été initialisé." + rc + "Dates : " + oSpr.sSprint_Date())
         	Ferme()
         FIN
