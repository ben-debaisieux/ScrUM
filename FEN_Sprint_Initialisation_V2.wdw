  procedures :
   -
     name : ListeTaches
     procedure_id : 2212460186108890168
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //ListeTaches ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      //	nNumSprint_p (entier) : <indiquez ici le rôle de nnumSprint>
      PROCEDURE ListeTaches()
      
      nLigne est un entier
      oTache_tmp est un ClTache dynamique
      nIndTbl est un entier
      
      TableSupprimeTout(TBL_Taches)
      TBL_Taches..AffichageActif = faux
      
      POUR TOUT element oTache_tmp, nIndTbl de zaOTaches 
      	nLigne = TableAjouteLigne(TBL_Taches)
      	TBL_Taches[nLigne].COL_ID					= nIndTbl
      	
      	//On affiche le code de la tache si elle en a un
      	Si oTache_tmp.p_sCodeTache <> "" alors
      		TBL_Taches[nLigne].COL_Code				= oTache_tmp.p_sCodeTache
      	sinon //Au cas ou elle n'a pas de code, on affiche son ID
      		TBL_Taches[nLigne].COL_Code				= oTache_tmp.Get_IdUnique()
      	fin
      	TBL_Taches[nLigne].COL_Description			= oTache_tmp.p_sDescription
      	TBL_Taches[nLigne].COL_Description..Bulle	= oTache_tmp.sBulleTache()
      	TBL_Taches[nLigne].COL_Poids				= oTache_tmp.p_nPoids
      	TBL_Taches[nLigne].COL_PoidsPrevisionnel	= oTache_tmp.p_nPoids_Previsionnel
      	TBL_Taches[nLigne].COL_Sprint				= oTache_tmp.p_nNum_Sprint_Realisation
      	TBL_Taches[nLigne].COL_SprintPrevisionnel	= oTache_tmp.p_nNum_Sprint_Previsionnel
      	TBL_Taches[nLigne].COL_Projet				= oTache_tmp.p_oUserStoryParent.p_oProjetParent.p_sNom
      	
      FIN
      TBL_Taches..AffichageActif = vrai
      
      
      
      ChargeCbo()
      	
     type : 458752
   -
     name : PoidsTotal_sur_VelociteMoyenne
     procedure_id : 2212460186108955704
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // PoidsTotal_sur_VelociteMoyenne ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE PoidsTotal_sur_VelociteMoyenne()
      
      nPoidsTotal est un entier = 0
      nVelociteMoyenne est un entier = goApp.m_nVelociteMoyenne
      oTache_tmp est un ClTache dynamique
      nSprintRef est un entier
      
      //Si on est sur 'tous les sprints', on prend le sprint en cours
      si CBO_Sprint..ValeurMémorisée <= 0 ALORS
      	nSprintRef = goApp.m_nNumeroDuSprintActuel + 1
      sinon
      	nSprintRef = CBO_Sprint..ValeurMémorisée
      FIN
      
      //On cumule tous les poids prévisionnel du sprint retenu
      POUR TOUT ELEMENT oTache_tmp DE zaoTaches
      	si nSprintRef = oTache_tmp.p_nNum_Sprint_Previsionnel alors
      		nPoidsTotal += oTache_tmp.p_nPoids_Previsionnel
      	fin
      FIN
      
      
      //On actualise l'affichage de la jauge
      LIB_Poids_Velocité_moyenne..Couleur = Noir
      JAUGE_Jauge..CouleurJauge = VertFoncé
      LIB_Poids_Velocité_moyenne = ChaîneConstruit("Sprint %3 - Poids / Vélocité Moyenne : %1/%2",...
      	nPoidsTotal, ...
      	nVelociteMoyenne, ...
      	nSprintRef)
      
      JAUGE_Jauge = nPoidsTotal
      
      SI nPoidsTotal > nVelociteMoyenne alors
      	LIB_Poids_Velocité_moyenne..Couleur = RougeClair
      	JAUGE_Jauge..CouleurJauge = RougeClair
      FIN
      
      
      si nSprintRef = (goapp.m_nNumeroDuSprintActuel + 1) ALORS
      	BTN_Valider..Visible = vrai
      sinon
      	BTN_Valider..Visible = faux
      FIN
     type : 458752
   -
     name : ChargeCbo
     procedure_id : 2212517180344993832
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // ChargeCbo ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE ChargeCbo()
      
      
      nNumSprintActuel est un entier = goApp.m_nNumeroDuSprintActuel
      nNumSprintSel est un entier = CBO_Sprint..ValeurMémorisée
      oTache_tmp est un ClTache dynamique
      nNumSprint est un entier
      anLstSprint est un tableau d'entiers
      
      TableauAjoute(anLstSprint,nNumSprintActuel)
      
      POUR TOUT ELEMENT oTache_tmp DE zaoTaches 
      	SI TableauCherche(anLstSprint,tcLinéaire,oTache_tmp.p_nNum_Sprint_Previsionnel) = -1 et oTache_tmp.p_nNum_Sprint_Previsionnel <> 0 ALORS
      		TableauAjoute(anLstSprint,oTache_tmp.p_nNum_Sprint_Previsionnel)
      	FIN
      FIN
      
      TableauTrie(anLstSprint)
      
      
      
      ListeSupprimeTout(CBO_Sprint)
      //Ajout du choix Tous les sprints
      ListeAjoute(CBO_Sprint,"<Tous>" + gLien(0))
      ListeAjoute(CBO_Sprint,"<Futur>" + gLien(zcst_sprint_futur))
      ListeAjoute(CBO_Sprint,"<Sans sprint>" + gLien(zcst_sprint_sans))
      POUR TOUT nNumSprint DE anLstSprint
      	ListeAjoute(CBO_Sprint, "Sprint " + nNumSprint + gLien(nNumSprint))
      FIN
      
      
      
      nIndiceSprintActuel est un entier
      nIndiceSprintActuel = ListeCherche(CBO_Sprint, nNumSprintSel)
      SI nIndiceSprintActuel = -1 ALORS
      	nIndiceSprintActuel = ListeCherche(CBO_Sprint, nNumSprintActuel)
      	SI nIndiceSprintActuel = -1 ALORS
      		ListeSelectPlus(CBO_Sprint,1)
      	SINON
      		ListeSelectPlus(CBO_Sprint,nIndiceSprintActuel)
      	FIN
      SINON
      	ListeSelectPlus(CBO_Sprint,nIndiceSprintActuel)
      FIN
      //ExécuteTraitement(CBO_Sprint,trtSélection)
      
      
      
      AppFilte()
     type : 458752
