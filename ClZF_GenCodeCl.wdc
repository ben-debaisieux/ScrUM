      procédure GenBlocCL_Propriete(sCode_R_p est un chaine, sCode_W_p est un chaine, sNom_p est un chaine):chaine
      
      sBlocR est une chaîne
      sBlocW est une chaîne
      
      si sCode_R_p = "" et sCode_W_p = "" ALORS
      	exceptiondéclenche(1,"Erreur de paramètre : une propriété est au moins en écriture ou en lecture.")
      FIN
      
      si sCode_R_p <> "" ALORS
      	sBlocR = [
              code : |1-
      [%sIndentSpace(sCode_R_p, 9)%]
              type : 1966080
      	]
      FIN
      
      SI sCode_R_p <> "" ALORS
      	sBlocW = [
              code : |1-
      [%sIndentSpace(sCode_w_p, 9)%]
              type : 2031616
      	]
      FIN
      
      sRetour est une chaîne = [
         -
           name : [%sNom_p%]
           identifier : [%GenProcIdent()%]
           type_code : 103
           p_codes :
            -
      [%sBlocR%]
            -
      [%sBlocw%]
           template_refs : []
      ]
      
      RENVOYER sRetour
