  procedures :
   -
     name : Actualise
     procedure_id : 2278218561562896903
     type_code : 14
     code : |1-
      procédure Actualise(LOCAL nSprint_p est un entier, ...
      	LOCAL nNbSprint_p est un entier, ...
      	LOCAL eSelecteur_p est un ClDbTache.ETypeCharge)<ui>:vide
      		
      
      stGraphe	est un col_graphes.STR_Charge		
      stPrj		est une col_graphes.STR_Charge_InfoPrj
      stSpr		est une col_graphes.STR_Charge_Spr
      nInd		est un entier
      aLstSprints est un tableau d'entiers
      
      
      
      stGraphe = col_graphes.Graphe_Charge_Sprints(nSprint_p, nNbSprint_p, eSelecteur_p, faux)
      
      
      grSupprimeSérie(GRF_Planification)
      grLibellé(GRF_Planification, grAfficheEtiquette, Vrai)
      
      
      
      
      nInd = 1
      
      //Affiche une ligne representant la vélocité moyenne actuelle
      // - type de la série
      grTypeSérie(GRF_Planification, nInd, grDroiteConstante, stGraphe.nVelocite)
      // - étiquette
      grEtiquetteSérie(GRF_Planification, nInd, "Vélocité moyenne")
      // - type de trait
      grTypeTraitSérie(GRF_Planification, nInd, TraitContinu)
      // - épaisseur du trait
      grEpaisseurTrait(GRF_Planification, nInd, 2)
      // - couleur
      grCouleurSérie(GRF_Planification, nInd,RougeClair)
      
      
      GRF_Planification..Titre = stGraphe.sTitre
      
      
      
      
      //Création des indices de série des sprints.
      nInd2 est un entier
      pour nInd2 = nSprint_p _a_ nSprint_p + nNbSprint_p
      	TableauAjoute(aLstSprints,nInd2)
      FIN
      
      POUR TOUT stPrj DE stGraphe.aStProjets
      	nInd++
      	//nom du projet pour le nom de l'étiquette 
      	grEtiquetteSérie(GRF_Planification, nInd, stPrj.sNom)
      	//donne une couleur à la série
      	grCouleurSérie(GRF_Planification, nInd , stPrj.nCouleur)
      	
      	//parcours les projets compris dans l'intervalle de sprints
      	POUR TOUT stSpr, sNumSprint DE stPrj.aSpr
      		//ajout des données dans le graphe
      		grAjouteDonnée(GRF_Planification, nInd, nIndiceSprint(sNumSprint), stSpr.nPts)
      	FIN
      FIN
      
      sSpr est une chaîne
      nInd = 0
      pour TOUT sSpr de stGraphe.aStListeSprints
      	nInd++
      	grEtiquetteCatégorie(GRF_Planification,nInd,sSpr)
      FIN
      
      
      
      grDessine(GRF_Planification)
      
      
      	procedure interne nIndiceSprint(sSprint_p est une chaine):entier
      		renvoyer max(0, TableauCherche(aLstSprints, tcLinéaire, sSprint_p))
      	FIN
