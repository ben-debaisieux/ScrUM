         //HAnnuleDéclaration(sdSource_Liste_Sprint)
         
         oSpr est un ClSprint
         aoSpr est un tableau de ClSprint
         nInd est un entier
         nIndiceSprintActuel est un entier
         
         
         
         ListeSupprimeTout(CBO_Sprint)
         
         aoSpr = ClSprint.aoListeSprint()
         POUR nInd = aoSpr..Occurrence a 1 pas -1
         	oSpr <- aoSpr[nInd]
         	ListeAjoute(CBO_Sprint, "Sprint " + oSpr.Get_IdUnique() + gLien(oSpr.Get_IdUnique()))
         FIN
         
         nIndiceSprintActuel = ListeCherche(CBO_Sprint,goApp.m_nNumeroDuSprintActuel)
         
         ListeSelectPlus(CBO_Sprint,nIndiceSprintActuel)
         
