      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure Th_GetMetriques()
      
      
      s est une SectionCritique
      
      tantque vrai
      	quand Exception dans
      		UTILISER s DANS
      			astMetrique = ClZF_ObjDbLs.mg_oApp.GetMetriques()
      			
      			
      			oObjDbTmp est un ClZF_ObjDbLs dynamique
      			sCle est une chaîne
      			pour tout oObjDbTmp, sCle de ClZF_App.mg_aoInstances
      				si pas oObjDbTmp.ZF_bVerifPointeur() ALORS
      					trace("Pb pointeur : [%sCle%] - [%oObjDbTmp.sRepresentation()%].")
      				FIN
      			FIN
      			
      		FIN
      	faire
      		//rien
      	FIN
      	
      	si ThreadArrêtDemandé() ALORS
      		ThreadDemandeArrêt("th1")
      	sinon
      		ThreadPause(cst_Tempo_cs)
      	FIN
      FIN
     type : 458752
