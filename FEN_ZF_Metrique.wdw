   -
     name : AffMetrique
     procedure_id : 1442574322444467060
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // AffMetrique ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure AffMetrique()
      stTmpMetrique est un ClZF_App.STR_metrique
      nInd est un entier
      
      
      grSupprimeSérie(GRF_Metriques)
      
      POUR TOUT stTmpMetrique, nInd de astMetrique
      	grEtiquetteCatégorie(GRF_Metriques,nInd, nInd / (100 / cst_Tempo_cs))
      	grAjouteDonnée(GRF_Metriques, 1, nInd, stTmpMetrique.nNbRequetes)
      	grAjouteDonnée(GRF_Metriques, 2, nInd, stTmpMetrique.nNbTransactions)
      	grAjouteDonnée(GRF_Metriques, 3, nInd, stTmpMetrique.nNbDeclarationObjBd)
      	grAjouteDonnée(GRF_Metriques, 4, nInd, stTmpMetrique.nNbDeclarationObjBd_Cache)
      	grAjouteDonnée(GRF_Metriques, 5, nInd, stTmpMetrique.nNbDeclarationObjBd_NonCache)
      	grAjouteDonnée(GRF_Metriques, 6, nInd, stTmpMetrique.nNbRecherches)
      	grAjouteDonnée(GRF_Metriques, 7, nInd, stTmpMetrique.nNbObjBd_Expire)
      FIN
      
      //grTitre(GRF_Projection,stRetour.sTitre)
      grLibellé(GRF_Metriques, grAfficheEtiquette, Vrai)
      
      grEtiquetteSérie(GRF_Metriques,1,"Nombre de requêtes")
      grEtiquetteSérie(GRF_Metriques,2,"Nombre de transactions")
      grEtiquetteSérie(GRF_Metriques,3,"Nombre d'objets déclarés")
      grEtiquetteSérie(GRF_Metriques,4,"Nombre d'objets déclarés - en cache")
      grEtiquetteSérie(GRF_Metriques,5,"Nombre d'objets déclarés - hors cache")
      grEtiquetteSérie(GRF_Metriques,6,"Nombre de recherches")
      grEtiquetteSérie(GRF_Metriques,7,"Nombre d'objets expirés")
      
      //grEtiquetteSérie(GRF_Projection,1,"Points Restants")
      //grCouleurSérie(GRF_Projection,1,BleuClair)
      
      nSerie est un entier
      pour nSerie = 1 a 6
      	grEpaisseurTrait(GRF_Metriques,nSerie,2)
      FIN
      
      //
      //grEtiquetteSérie(GRF_Projection,2,"Projection Avancement")
      //grCouleurSérie(GRF_Projection,2,RougeClair)
      //grEpaisseurTrait(GRF_Projection,2,2)
      
      grDessine(GRF_Metriques)
