   -
     name : alistRubAvecLiaisonPK
     procedure_id : 1600637567123839656
     type_code : 12
     code : |1-
      procédure publique alistRubAvecLiaisonPK() <métier>:tableau de chaînes
      
      
      aRetour			est un tableau de chaines
      
      POUR TOUTE CHAÎNE sLigneCle DE HListeLiaison(:m_sNomTable) SÉPARÉE PAR RC
      	
      	sTbl1			est une chaîne		= ExtraitChaîne(sLigneCle, 2,tab)
      	sRub1			est une chaîne		= ExtraitChaîne(sLigneCle, 3,tab)
      	sCardinalite1	est une chaîne		= ExtraitChaîne(sLigneCle, 4,tab)
      	sTbl2			est une chaîne		= ExtraitChaîne(sLigneCle, 5,tab)
      	sRub2			est une chaîne		= ExtraitChaîne(sLigneCle, 6,tab)
      	sCardinalite2	est une chaîne		= ExtraitChaîne(sLigneCle, 7,tab)
      	
      	
      	SI sCardinalite1 <> sCardinalite2 /*ET sTbl1 <> sTbl2*/ ALORS
      		
      		MemoRubPK(sTbl1,sCardinalite1,sRub1)
      		
      		MemoRubPK(sTbl2,sCardinalite2,sRub2)
      	FIN
      FIN
      
      renvoyer aRetour
      
      
      	procedure interne MemoRubPK(sTbl_p est une chaîne, sCardinalite_p est une chaine, sRub_p est une chaine):vide
      		
      		SI sTbl_p = :m_sNomTable ALORS
      			SI Droite(Minuscule(sCardinalite_p), 1) = "n" ALORS
      				si TableauCherche(aRetour,tccSansAccent+tccSansCasse+tccSansEspace+tccSansEspaceIntérieur+tccSansPonctuationNiEspace+tcLinéaire, sRub_p) = -1 ALORS
      					TableauAjoute(aRetour,sRub_p)
      				FIN
      			FIN
      		FIN
      	FIN
      	
