   -
     name : aTableDependances
     procedure_id : 1474825930687461673
     type_code : 12
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] aTableDependances ()
      //
      // Paramètres :
      //	Aucun
      
      // Valeur de retour :
      // 	tableau (chaîne ANSI) : <indiquez ici le rôle de la valeur de retour>
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure publique globale aTableDependances()<metier>:tableau de chaines
      
      taTbls	est un tableau associatif de ClZF_GenCodeFic dynamique
      oTblTmp	est un ClZF_GenCodeFic dynamique
      aRetour	est un tableau de chaînes
      sTbl	est une chaîne
      bMqt	est un booléen
      stRel	est un ClZF_GenCodeFic.STR_Rel
      
      
      //On récupère la définition de toutes les tables
      pour toute sTbl de ::aListeFicAna()
      	taTbls[sTbl] <- ClZF_GenCodeFic.oGetGenCodeFic(sTbl)
      FIN
      
      //Tant qu'il reste des tables à traiter...
      tantque taTbls..Occurrence > 0
      	
      	//Parcourt de toutes les entités restantes.
      	pour tout oTblTmp, sTbl de taTbls
      		bMqt = faux
      		
      		//On liste toutes les FK pour trouver les dépendances.
      		pour tout stRel de oTblTmp.m_aPK_de_FK
      			
      			//Pour chaque relations, on regarde si on a déjà traité la table parente.
      			si stRel.sTablePK <> sTbl _et_ ...
      					TableauCherche(aRetour, tcLinéaire, stRel.sTablePK) <= 0 _et_ 	//on recherche les tables traitées
      					pas taTbls[stRel.sTablePK]..vide ALORS							//on exclue la dépendance, si la table n'est pas dans les non traités (cas de tables exclues)
      				//Non : on a une dépendance non résolue
      				bMqt = vrai
      			FIN
      		FIN
      		
      		
      		//SI on a pas de dpéndance sur la table, on l'ajoute à la liste des tables traitée (et est retiré des tables à traiter)
      		si pas bMqt ALORS
      			TableauAjoute(aRetour, sTbl)
      			TableauSupprime(taTbls, sTbl)
      		FIN
      	FIN
      FIN
      
      
      renvoyer aRetour
     type : 458752
