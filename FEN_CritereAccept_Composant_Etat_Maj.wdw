         Scr_CritereAccept.CommentaireDemand			= sdSourceCritereAccept.CommentaireDemand
         
         
         HLitRecherche(Scr_CritereAccept, IDScr_CritereAccept, ID_CritereAccept,hIdentique+hLimiteParcours)
         si htrouve(Scr_CritereAccept) alors
         	
         	Scr_CritereAccept.IDScr_CritereAccept_Etat = CBO_Etat
         
         	
         	
