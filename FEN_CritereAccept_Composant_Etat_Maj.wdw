   -
     code : |1+
      PROCEDURE FEN_CritereAccept_Composant_Etat_Maj(local ID_CritereAccept est une entier)
   -
     code : |1-
      
      //requete pour avoir le nom et les ID des users
      sdSource_Etat est une source de données
      sReq_Etat est une chaîne = [
      select
       Scr_CritereAccept_Etat.Libelle, 
       Scr_CritereAccept_Etat.IDScr_CritereAccept_Etat
       from Scr_CritereAccept_Etat
       
      
      ]
      
      ListeSupprime(CBO_Etat)
      // la on les remplis
      SI goApp.Rsql(sdSource_Etat,sReq_Etat) ALORS
      	POUR TOUT sdSource_Etat
      		ListeAjoute(CBO_Etat,sdSource_Etat.Libelle + gLien(sdSource_Etat.IDScr_CritereAccept_Etat))
      	FIN
      	
      FIN
      
      sdEtatActuel est une source de données
      sReq_EtatActuel est une chaine = ChaîneConstruit([
      SELECT
      	 Scr_CritereAccept.IDScr_CritereAccept_Etat
      FROM
      	Scr_CritereAccept
      WHERE 
      	IDScr_CritereAccept = %1
      	
      ], ID_CritereAccept)
      
      si goApp.Rsql(sdEtatActuel, sReq_EtatActuel) alors
      	nEtat est un entier = sdEtatActuel.IDScr_CritereAccept_Etat
      
      	CBO_Etat = nEtat
      FIN
      HAnnuleDéclaration(sdEtatActuel)
      FEN_CritereAccept_Composant_Etat_Maj..ValeurRenvoyée = vrai
