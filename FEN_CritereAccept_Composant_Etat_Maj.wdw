         			Scr_CritereAccept.IDSrc_Usr_Valideur = goApp.m_oUsrCnx.ZF_Get_IdUnique()
         			
         		CAS 3 : // Désactivation		
         			Scr_CritereAccept.DateDesactivation = dDate
         			Scr_CritereAccept.CommentaireDesactivation += [RC] + SAI_Comm
         			Scr_CritereAccept.IDSrc_Usr_Desactivation = goApp.m_oUsrCnx.ZF_Get_IdUnique()
         			
         		AUTRE CAS
         			
         	FIN
         	
         	SI PAS HModifie(Scr_CritereAccept) ALORS
         		Info(ErreurInfo(errComplet))
         	sinon
         		hraz(Scr_Message)
