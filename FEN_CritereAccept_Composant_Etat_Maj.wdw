        code : |1+
         //récuperation date systeme
         dDate est une Date = DateSys()
         // controle champ non vide :
         
         si SAI_Comm = "" alors
         	Erreur("Champs Commentaire vide")
         	RepriseSaisie(SAI_Comm)
         FIN
         
         sdSourceCritereAccept est une source de données
         // requete recuperation data CritereAccept
         sReqetoileCritere est une chaine = ChaîneConstruit([
         	
         	SELECT
         	
         	IDScr_CritereAccept_Etat,
         	CommentaireDemand,
         	CommentaireDev,
         	IDSrc_Usr_Redacteur,
         	DateCreation,
         	IDSrc_Usr_Valideur,
         	DateValidation,
         	IDSrc_Usr_Desactivation,
         	DateDesactivation,
         	CommentaireDesactivation
         	
         	FROM
         	
         	Scr_CritereAccept
         	
         	WHERE
         	
         	IDScr_CritereAccept = %1
         	
         ], ID_CritereAccept)
         //execution
         SI goApp.Rsql(sdSourceCritereAccept, sReqetoileCritere) ALORS
         	Scr_CritereAccept.IDScr_CritereAccept_Etat = sdSourceCritereAccept.IDScr_CritereAccept_Etat
         	Scr_CritereAccept.DateDesactivation = sdSourceCritereAccept.DateDesactivation
         	Scr_CritereAccept.CommentaireDesactivation = sdSourceCritereAccept.CommentaireDesactivation
         	Scr_CritereAccept.CommentaireDev = sdSourceCritereAccept.CommentaireDev
         	Scr_CritereAccept.DateValidation = sdSourceCritereAccept.DateValidation
         	Scr_CritereAccept.CommentaireDemand = sdSourceCritereAccept.CommentaireDemand
         	Scr_CritereAccept.DateCreation = sdSourceCritereAccept.DateCreation
         	Scr_CritereAccept.IDSrc_Usr_Desactivation = sdSourceCritereAccept.IDSrc_Usr_Desactivation
         	Scr_CritereAccept.IDSrc_Usr_Redacteur = sdSourceCritereAccept.IDSrc_Usr_Redacteur
         	Scr_CritereAccept.CommentaireDemand = sdSourceCritereAccept.CommentaireDemand
         FIN
         
         HLitRecherche(Scr_CritereAccept, IDScr_CritereAccept, ID_CritereAccept,hIdentique+hLimiteParcours)
         si htrouve(Scr_CritereAccept) alors
         	
         	Scr_CritereAccept.IDScr_CritereAccept_Etat = CBO_Etat
         
         	
         	
         	selon CBO_Etat
         		CAS 1 : // Rédaction
         			Scr_CritereAccept.CommentaireDemand += [RC] + SAI_Comm
         			
         			
         		CAS 2 : // valider
         			Scr_CritereAccept.DateValidation = dDate
         			Scr_CritereAccept.CommentaireDemand += [RC] + SAI_Comm
         			Scr_CritereAccept.IDSrc_Usr_Valideur = goApp.m_oUsrCnx.Get_IdUnique()
         			
         		CAS 3 : // Désactivation		
         			Scr_CritereAccept.DateDesactivation = dDate
         			Scr_CritereAccept.CommentaireDesactivation += [RC] + SAI_Comm
         			Scr_CritereAccept.IDSrc_Usr_Desactivation = goApp.m_oUsrCnx.Get_IdUnique()
         			
         		AUTRE CAS
         			
         	FIN
         	
         	SI PAS HModifie(Scr_CritereAccept) ALORS
         		Info(ErreurInfo(errComplet))
         	sinon
         		hraz(Scr_Message)
         		Scr_Message.IDScr_Usr				= goApp.m_oUsrCnx.Get_IdUnique()
         		Scr_Message.Num_Sprint				= 0
         		Scr_Message.IDScr_UserStory			= Scr_CritereAccept.IDScr_UserStory
         		Scr_Message.IDScr_CritereAccept		= Scr_CritereAccept.IDScr_CritereAccept
         		Scr_Message.IDScr_Tache				= 0
         		Scr_Message.IDScr_Message_Parent	= 0	
         		Scr_Message.CMTR					= "changement d'état du critère d'acceptation : " + rc + Scr_CritereAccept.Titre + rc + SAI_Comm
         		Scr_Message.Top_Visible				= vrai
         		Scr_Message.DteCreation				= dDate
         		Scr_Message.Image					= ""	
         		
         		si pas hajoute(Scr_Message) alors
         			Info(ErreurInfo(errComplet))		
         		FIN
         	FIN
         	
         	Info("Critère Validé !")
         	
         fin
         
