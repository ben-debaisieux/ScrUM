  procedures :
   -
     name : ListeUserStories
     procedure_id : 2071396992392491436
     type_code : 14
     code : |1+
      // Résumé : Affiche la liste des US en test pour un utilisateur
      // Syntaxe :
      //ListeUserStories ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      //	nIDUsr_p (entier) : <indiquez ici le rôle de nIDUsr_p>
      //	nIDProjet_p (entier) : <indiquez ici le rôle de nIDProjet_p>
      PROCEDURE ListeUserStories()
      
      nLigne				est un entier
      sLstTaches			est une chaîne
      sGrp				est une chaîne
      oPrjs				est un ClColProjet
      oPrj				est un ClDbProjet dynamique
      oUs					est un ClDbUserStory dynamique
      oTache				est un ClDbTache dynamique
      //bPresTacheATester	est un booléen
      
      si CBO_Projet..ValeurMémorisée <> -1 alors
      	
      	si CBO_Projet..ValeurMémorisée = cstTousProjet ALORS
      		si goapp.m_oUsrCnx.p_bTop_Equip_Informatique ALORS
      			oPrjs = ClColProjet.aoListeProjets()
      		sinon
      			oPrjs = ClColProjet.aoListeProjetsDeUsr(goApp.m_oUsrCnx)
      //			oPrjs = goApp.m_oUsrCnx.aoMesProjets(ClUsr.usr_tous)
      		FIN
      		
      	sinon
      		oPrjs.addItemById(CBO_Projet..ValeurMémorisée)
      	FIN
      	
      	
      	TableSupprimeTout(TBL_Liste_User_Stories)
      	TBL_Liste_User_Stories..AffichageActif = Faux
      	pour tout element oPrj de oPrjs.aItems()
      		
      		POUR TOUT element oUs de ClColUserStory.oFromProjet(oPrj).aitems()	//oPrj.aoListeUS().aItems()
      			
      			
      			si bAffUS(oUs) alors
      //			si oUs.Get_EtatUS() = ClUserStory.ECompteNbUS.En_test_fonctionnel alors
      				nLigne = TableAjouteLigne(TBL_Liste_User_Stories)
      				
      				
      				sGrp = ""
      				si pas oUs.p_oIDScr_Epic = Null ALORS
      					sGrp = oUs.p_oIDScr_Epic.p_sNom
      				FIN
      				
      				
      				pour tout element oTache de ClColTache.oFromUserStory(oUs).aItems()
      					si pas oTache.p_nStatut dans (ClDbTache.cstStatut_realise, ClDbTache.cstStatut_Annulee) alors
      						sLstTaches += [rc] + oTache.p_sCodeTache + " - " + oTache.p_sDescription + " (" + oTache.sStatut() + ")"
      					FIN
      				FIN
      				
      				
      				TBL_Liste_User_Stories[nLigne].COL_ChefDeProjet		= ClZF_ObjDbLs.ZF_RepresentationSiNonNull(oUs.p_oIDScr_Usr_Valideur)
      				TBL_Liste_User_Stories[nLigne].COL_IDUserStory		= oUs.ZF_Get_IdUnique()
      				TBL_Liste_User_Stories[nLigne].COL_Grp				= sGrp
      				TBL_Liste_User_Stories[nLigne].COL_US				= oUs.sNommination_US()
      				TBL_Liste_User_Stories[nLigne].COL_Taches			= sLstTaches
      				TBL_Liste_User_Stories[nLigne].COL_Projet			= oUs.p_oIDScr_Projet.p_sNom
      				TBL_Liste_User_Stories[nLigne].COL_DteMiseEnTest	= oUs.p_dDateMiseADispo_EnTest
      			FIN
      		FIN
      	FIN
      	
      	TableTrie(TBL_Liste_User_Stories, "-" + TBL_Liste_User_Stories.COL_DteMiseEnTest..nom)
      	
      	TBL_Liste_User_Stories..AffichageActif = Vrai
      fin
      
      
     type : 458752
   -
     name : bAffUS
     procedure_id : 1422159346484888659
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] bAffUS ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	booléen : <indiquez ici le rôle de la valeur de retour>
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure bAffUS(oUS_p est un ClDbUserStory dynamique):booléen
      
      oTache est un ClDbTache dynamique
      bPresTacheATester est un booléen
      
      SI PAS oUS_p.p_bTermine ALORS
      	POUR TOUT ÉLÉMENT oTache DE ClColTache.oFromUserStory(oUS_p).aItems()
      		SI oTache.p_nNum_Sprint_Demarrage > 0 _ET_ ...
      				oTache.p_nNum_Sprint_Fin = Null _et_ ...
      				pas oTache.bEstValidee() _et_ ...
      				pas oTache.p_nStatut dans (ClDbTache.cstStatut_Annulee, ClDbTache.cstStatut_Suspendu) ALORS
      				
      			bPresTacheATester = Vrai
      			SORTIR
      		FIN
      	FIN
      FIN
      
      renvoyer bPresTacheATester
     type : 458752
