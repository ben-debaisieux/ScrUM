   -
     name : bMessageUs
     procedure_id : 2072060131045470211
     type_code : 14
     code : |1+
      // Résumé : Renvoie vrai si une US est lié a des messages
      // Syntaxe :
      //[ <Résultat> = ] bMessageUs (<nIDUserStory_p> est entier)
      //
      // Paramètres :
      //	nIDUserStory_p (entier) : <indiquez ici le rôle de nIDUserStory_p>
      // Valeur de retour :
      // 	booléen : // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE bMessageUs(LOCAL nIDUserStory_p est un entier):booléen
      
      bMessages est un booléen
      
      sdRechercheMessage est une source de donneés
      sReq est une chaine
      
      sReq = ChaîneConstruit([
      	SELECT COUNT(Scr_Message.IDScr_Message) AS nbMessage
      	FROM Scr_Message
      	WHERE Scr_Message.IDScr_UserStory = %1
      ], nIDUserStory_p)
      
      SI goApp.Rsql(sdRechercheMessage,sReq) ALORS
      	HLitPremier(sdRechercheMessage)
      	SI htrouve(sdRechercheMessage) alors
      		bMessages = (sdRechercheMessage.nbMessage > 0)	//Vrai si nb de message > 0 faux sinon
      	FIN
      FIN
      
      HAnnuleDéclaration(sdRechercheMessage)
      
      RENVOYER bMessages
     type : 458752
