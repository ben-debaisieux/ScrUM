      -
        code : |1+
         oPrj		est un ClDbProjet dynamique
         oProjets	est un ClColProjet
         nCompt		est un entier
         nComptTot	est un entier
         
         
         SI goApp.m_oUsrCnx.p_bTop_Equip_Informatique ALORS
         	oProjets <- ClColProjet.aoListeProjets(Faux)
         SINON
         	oProjets <- ClColProjet.aoListeProjetsDeUsr(goApp.m_oUsrCnx)
         FIN
         
         ListeSupprimeTout(CBO_Projet)
         POUR TOUT ÉLÉMENT oPrj DE oProjets.aItems()
         	nCompt = 0
         	
         	//On compte les US qui sont en test fonctionnel
         	POUR TOUT ÉLÉMENT oUs DE ClColUserStory.oFromProjet(oPrj).aitems()	//oPrj.aoListeUS().aItems()
         		SI bAffUS(oUs) ALORS
         			nCompt++
         			nComptTot++
         		FIN
         	FIN
         
         	si nCompt > 0 ALORS
         		ListeAjoute(CBO_Projet, oPrj.p_sNom + " (" + nCompt + ")"+ gLien(oPrj.ZF_Get_IdUnique()))
         	FIN
         FIN
         
         ListeTrie(CBO_Projet)
         
         ListeInsère(CBO_Projet,"<Tous> ([%nComptTot%])" + glien(cstTousProjet), 1)
         ListeSelectPlus(CBO_Projet, 1)
