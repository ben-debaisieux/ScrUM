     name : ListeUserStories
     procedure_id : 2071396992392491436
     type_code : 14
     code : |1+
      // Résumé : Affiche la liste des US en test pour un utilisateur
      // Syntaxe :
      //ListeUserStories ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      //	nIDUsr_p (entier) : <indiquez ici le rôle de nIDUsr_p>
      //	nIDProjet_p (entier) : <indiquez ici le rôle de nIDProjet_p>
      PROCEDURE ListeUserStories()
      
      nLigne est un entier
      //sLstTaches est une chaîne
      //nPoids est un entier
      aoPrj est un tableau ClProjet dynamique
      oPrj est un ClProjet dynamique
      oUs est un ClUserStory dynamique
      
      si CBO_Projet..ValeurMémorisée <> -1 alors
      	
      	si CBO_Projet..ValeurMémorisée = cstTousProjet ALORS
      		si goapp.m_oUsrCnx.p_bTop_Equip_Informatique ALORS
      			aoPrj = ClProjet.aoListeProjets()
      		sinon
      			aoPrj = goApp.m_oUsrCnx.aoMesProjets(ClUsr.usr_tous)
      		FIN
      		
      	sinon
      		TableauAjouteRéférence(aoPrj, ClProjet.GetObj(CBO_Projet..ValeurMémorisée))
      	FIN
      	
      	
      	TableSupprimeTout(TBL_Liste_User_Stories)
      	TBL_Liste_User_Stories..AffichageActif = Faux
      	pour tout element oPrj de aoPrj
      		
      		POUR TOUT element oUs de oPrj.aoListeUS()
      			si oUs.Get_EtatUS() = ClUserStory.ECompteNbUS.En_test_fonctionnel alors
      				nLigne = TableAjouteLigne(TBL_Liste_User_Stories)
      				
      				TBL_Liste_User_Stories[nLigne].COL_ChefDeProjet		= oUs.p_oUsrResponsable.p_sNom
      				TBL_Liste_User_Stories[nLigne].COL_IDUserStory		= oUs.Get_IdUnique()
      				TBL_Liste_User_Stories[nLigne].COL_EnTantQue		= oUs.p_sEntantQue
      				TBL_Liste_User_Stories[nLigne].COL_Besoin			= oUs.p_sJaiBesoinDe
      				TBL_Liste_User_Stories[nLigne].COL_AfinDe			= oUs.p_sAfinDe
      				TBL_Liste_User_Stories[nLigne].COL_Projet			= oUs.p_oProjetParent.p_sNom
      				TBL_Liste_User_Stories[nLigne].COL_DteMiseEnTest	= oUs.p_dDateMiseADispo_EnTest
      //				
      //				SI bMessageUs(oUs.Get_IdUnique()) ALORS
      //					TBL_Liste_User_Stories[nLigne].COL_Message = "message.png" 
      //				FIN
      //				
      //				
      //				oTache est un ClTache dynamique
      //				
      //				sLstTaches = ""
      //				nPoids = 0
      //				pour tout element oTache de oUs.aoListeTaches()
      //					sLstTaches += [", "] + oTache.p_sCodeTache
      //					nPoids += oTache.p_nPoids
      //				FIN
      				
      				//sReq = ChaîneConstruit([
      				//	SELECT
      				//		CodeTache,
      				//		Poids
      				//	
      				//	FROM Scr_Tache
      				//	
      				//	WHERE IDScr_UserStory = %1
      				//	
      				//], oUs.Get_IdUnique())
      				//
      				//SI goApp.Rsql(sdInfoTaches,sReq) ALORS
      				//	
      				//	sLstTaches = ""
      				//	nPoids = 0
      				//	
      				//	POUR TOUT sdInfoTaches
      				//		
      				//		sLstTaches += [", "] + sdInfoTaches.CodeTache
      				//		nPoids += sdInfoTaches.Poids
      				//		
      				//	FIN
      				//	
      				//FIN
      				//
      				//HAnnuleDéclaration(sdInfoTaches)
      //				
      //				TBL_Liste_User_Stories[nLigne].COL_LIsteTaches = sLstTaches
      //				TBL_Liste_User_Stories[nLigne].COL_Poids = nPoids
      				
      				
      //				nLigne++
      			FIN
      		FIN
      	FIN
      	
      	TableTrie(TBL_Liste_User_Stories, "-" + TBL_Liste_User_Stories.COL_DteMiseEnTest..nom)
      	
      	TBL_Liste_User_Stories..AffichageActif = Vrai
      fin
      
      
