   -
     name : AffUSs
     procedure_id : 1474668392670792642
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //AffUSs (<nLigneUs_p> est entier, <oUS_p> est ClDbUserStory)
      //
      // Paramètres :
      //	nLigneUs_p (entier) : <indiquez ici le rôle de nLigneUs>
      //	oUS_p (ClDbUserStory) : <indiquez ici le rôle de oUS>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure AffUSs(nLigneUs_p est un entier, oUS_p est un ClDbUserStory)
      
      bAlerte est un booléen
      nLigneTache est un entier
      sCommentaire est une chaîne
      
      
      
      SI oUS_p.p_sCMTR_Global <> "" ALORS
      	sCommentaire += [RC] + [
      				Commentaire global :
      				[%oUS_p.p_sCMTR_Global%]
      	]
      FIN
      SI oUS_p.p_sCMTR_RetourDeTest <> "" ALORS
      	sCommentaire += [RC] + [
      				Retour de test :
      				[%oUS_p.p_sCMTR_RetourDeTest%]
      	]
      FIN
      SI oUS_p.p_sCMTR_Mise_En_Prod <> "" ALORS
      	sCommentaire += [RC] + [
      				Infos de mise en prod :
      				[%oUS_p.p_sCMTR_Mise_En_Prod%]
      	]
      FIN
      
      TBLH_SelUS[nLigneUs_p].COL_Arbre		= oUS_p.sNommination_US()
      TBLH_SelUS[nLigneUs_p].COL_Arbre..Bulle	= sCommentaire
      TBLH_SelUS[nLigneUs_p].COL_IdUS			= oUS_p.ZF_Get_IdUnique()
      TBLH_SelUS[nLigneUs_p].COL_Cmtr			= oUS_p.p_sCMTR_Global
      TBLH_SelUS[nLigneUs_p].COL_Code			= oUS_p.p_sCodeUserStory
      TBLH_SelUS[nLigneUs_p].COL_Statut		= oUS_p.sLibelleDeEtat()
      
      //bAlerte									= Faux
      
      //Parcours de toutes le taches de l'US
      POUR TOUT ÉLÉMENT oTache DE ClColTache.aoFromUS(oUS_p).aItems()	//oUS.aoListeTaches().aItems()
      	
      	//				bAffTache = faux
      	//				
      	//				si oTache.p_nStatut pas dans (ClDbTache.cstStatut_realise, ClDbTache.cstStatut_Annulee) ALORS
      	//					bAffTache = Vrai
      	//				FIN
      	
      	
      	//Envoie des données de la tache
      	nLigneTache = TableAjouteFils(TBLH_SelUS, nLigneUs_p)
      	
      	AffTache(nLigneTache, oTache)
      	//				
      	//				sCommentaire = [
      	//					Commentaire :
      	//					[%oTache.p_sCMTR%]
      	//					]
      	//				TBLH_SelUS[nLigneTache].COL_Arbre			= oTache.p_sDescription
      	//				TBLH_SelUS[nLigneTache].COL_Arbre..Bulle	= sCommentaire
      	//				TBLH_SelUS[nLigneTache].COL_IdTache			= oTache.Get_IdUnique()
      	//				TBLH_SelUS[nLigneTache].COL_Cmtr			= oTache.p_sCMTR
      	//				TBLH_SelUS[nLigneTache].COL_Code			= oTache.p_sCodeTache
      	//				TBLH_SelUS[nLigneTache].COL_Statut			= oTache.sLibDeStatut()
      	//				
      	//				si oTache.p_nNum_Sprint_Realisation <> 0 alors
      	//					oSprint = ClDbSprint.GetObj(oTache.p_nNum_Sprint_Realisation)
      	//					TBLH_SelUS[nLigneTache].COL_Dates		= oSprint.sSprint_Date()
      	//				fin
      	//				
      	//				//Mise en évidence du texte recherché (si identifié sur la ligne)
      	//				VisuRech(nLigneTache, SAI_Recherche)
      	
      	//Mise en évidence des taches avec un événement à régler.
      	SI bTacheAlerte(oTache.p_nStatut) ALORS
      		bAlerte = Vrai
      	FIN
      FIN
      
      //Mise en évidence du texte recherché (si identifié sur la ligne)
      VisuRech(nLigneUs_p, SAI_Recherche)
      
      //Si on a une alerte et qu'on doit l'afficher ou si on doit tout afficher
      SI (bAlerte ET SEL_Affichage = 1) OU 
      	SEL_Affichage = 4 ALORS
      	//On laisse ouvert
      SINON
      	//On referme
      	TableEnroule(TBLH_SelUS,nLigneUs_p)
      FIN
