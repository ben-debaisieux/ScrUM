        code : |1-
         si TBLH_SelUS.COL_IdTache <> 0 alors
         	oTache est un ClDbTache <- ClDbTache.GetObj(TBLH_SelUS.COL_IdTache)
         	SI Ouvre(FEN_User_Story_Detail_v2, oTache.p_oUserStoryParent, oTache.Get_IdUnique()) ALORS
         		AffTache(TBLH_SelUS, oTache)
         //		Afficher()
         	FIN
         sinon
         	si TBLH_SelUS.COL_IdUS <> 0 alors
         		oUs est un ClDbUserStory <- ClDbUserStory.GetObj(TBLH_SelUS.COL_IdUS)
         		SI Ouvre(FEN_User_Story_Detail_v2, oUs) ALORS
         			AffUSs(TBLH_SelUS,oUs)
         			//Afficher()
         		FIN
         	fin
         fin
