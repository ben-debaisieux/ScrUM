      				Infos de mise en prod :
      				[%oUS.p_sCMTR_Mise_En_Prod%]
      			]
      		FIN
      		
      		//Ajoute de l'User story dans la liste
      		nLigneEpic_p								= TableAjouteFils(TBLH_SelUS, nLigneEpic_p)
      		TBLH_SelUS[nLigneEpic_p].COL_Arbre			= oUS.sNommination_US()
      		TBLH_SelUS[nLigneEpic_p].COL_Arbre..Bulle	= sCommentaire
      		TBLH_SelUS[nLigneEpic_p].COL_IdUS			= oUS.ZF_Get_IdUnique()
      		TBLH_SelUS[nLigneEpic_p].COL_Cmtr			= oUS.p_sCMTR_Global
      		TBLH_SelUS[nLigneEpic_p].COL_Code			= oUS.p_sCodeUserStory
      		TBLH_SelUS[nLigneEpic_p].COL_Statut			= oUS.sLibelleDeEtat()
      		
      		
      		AffTaches_old(nLigneEpic_p, ClColTache.aoFromUS(oUS))
      	FIN
      	
      	
      	SI SEL_Affichage DANS (2) ALORS
      		TableEnroule(TBLH_SelUS,nLigneEpic_p)
      	FIN
      FIN
