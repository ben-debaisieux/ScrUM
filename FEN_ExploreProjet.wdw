      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure AffTaches_old(nLigneUs_p est un entier, oColTaches est un ClColTache)<ui>:vide
      
      bAlerte est un booléen
      bAffTache est un booléen
      sCommentaire est une chaîne
      nLigneTache est un entier
      oSprint est un ClDbSprint dynamique
      
      //Parcours de toutes le taches de l'US
      POUR TOUT ÉLÉMENT oTache DE oColTaches.aItems()	//oUS.aoListeTaches().aItems()
      	
      	bAffTache = Faux
      	
      	SI oTache.p_nStatut PAS DANS (ClDbTache.cstStatut_realise, ClDbTache.cstStatut_Annulee) ALORS
      		bAffTache = Vrai
      	FIN
      	
      	sCommentaire = [
      		Commentaire :
      		[%oTache.p_sCMTR%]
      	]
      	
      	//Envoie des données de la tache
      	nLigneTache									= TableAjouteFils(TBLH_SelUS, nLigneUs_p)
      	TBLH_SelUS[nLigneTache].COL_Arbre			= oTache.p_sDescription
      	TBLH_SelUS[nLigneTache].COL_Arbre..Bulle	= sCommentaire
      	TBLH_SelUS[nLigneTache].COL_IdTache			= oTache.Get_IdUnique()
      	TBLH_SelUS[nLigneTache].COL_Cmtr			= oTache.p_sCMTR
      	TBLH_SelUS[nLigneTache].COL_Code			= oTache.p_sCodeTache
      	TBLH_SelUS[nLigneTache].COL_Statut			= oTache.sLibDeStatut()
      	
      	SI oTache.p_nNum_Sprint_Realisation <> 0 ALORS
      		oSprint									<- ClDbSprint.GetObj(oTache.p_nNum_Sprint_Realisation)
      		TBLH_SelUS[nLigneTache].COL_Dates		= oSprint.sSprint_Date()
      	FIN
      	
      	//Mise en évidence du texte recherché (si identifié sur la ligne)
      	VisuRech(nLigneTache, SAI_Recherche)
      	
      	//Mise en évidence des taches avec un événement à régler.
      	SI oTache.p_nStatut DANS (ClDbTache.cstStatut_a_coter, ClDbTache.cstStatut_Suspendu) ALORS
      		TBLH_SelUS[nLigneTache].COL_Cmtr..CouleurFond	= COL_Scrum.gnCouleur_Orange_pale
      		bAlerte											= Vrai
      	FIN
      FIN
      
      //Mise en évidence du texte recherché (si identifié sur la ligne)
      VisuRech(nLigneUs_p, SAI_Recherche)
      
      //Si on a une alerte et qu'on doit l'afficher ou si on doit tout afficher
      SI (bAlerte ET SEL_Affichage = 1) OU 
      	SEL_Affichage = 4 ALORS
      	//On laisse ouvert
      SINON
      	//On referme
      	TableEnroule(TBLH_SelUS,nLigneUs_p)
      FIN
