   -
     name : AffTache
     procedure_id : 1474666477114408213
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //AffTache (<nLigneTache> est entier, <oTache> est ClDbTache)
      //
      // Paramètres :
      //	nLigneTache (entier) : <indiquez ici le rôle de nLigneTache>
      //	oTache (ClDbTache) : <indiquez ici le rôle de oTache>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure AffTache(nLigneTache est un entier, oTache est un ClDbTache)
      
      oSprint est un ClDbSprint dynamique
      sCommentaire est une chaîne
      
      sCommentaire = [
      	Commentaire :
      	[%oTache.p_sCMTR%]
      ]
      TBLH_SelUS[nLigneTache].COL_Arbre			= oTache.p_sDescription
      TBLH_SelUS[nLigneTache].COL_Arbre..Bulle	= sCommentaire
      TBLH_SelUS[nLigneTache].COL_IdTache			= oTache.Get_IdUnique()
      TBLH_SelUS[nLigneTache].COL_Cmtr			= oTache.p_sCMTR
      TBLH_SelUS[nLigneTache].COL_Code			= oTache.p_sCodeTache
      TBLH_SelUS[nLigneTache].COL_Statut			= oTache.sLibDeStatut()
      
      SI oTache.p_nNum_Sprint_Realisation <> 0 ALORS
      	oSprint									= ClDbSprint.GetObj(oTache.p_nNum_Sprint_Realisation)
      	TBLH_SelUS[nLigneTache].COL_Dates		= oSprint.sSprint_Date()
      FIN
      
      //Mise en évidence du texte recherché (si identifié sur la ligne)
      VisuRech(nLigneTache, SAI_Recherche)
      	
      //Mise en évidence des taches avec un événement à régler.
      SI bTacheAlerte(oTache.p_nStatut) ALORS
      	TBLH_SelUS[nLigneTache].COL_Cmtr..CouleurFond	= COL_Scrum.gnCouleur_Orange_pale
      FIN
