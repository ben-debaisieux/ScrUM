   -
     name : Afficher1
     procedure_id : 1474659540696438893
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Afficher ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure Afficher1() <UI>:vide
      
      oUS				est un ClDbUserStory dynamique
      oTache			est un ClDbTache dynamique
      oEpic			est un ClDbEpic dynamique
      oSprint			est un ClDbSprint dynamique
      nLigneEpic		est un entier
      nLigneUs		est un entier
      nLigneTache		est un entier
      bAlerte			est un booléen
      aOLstEpics		est un tableau de ClDbEpic
      sCommentaire	est une chaîne
      bAffUS			est un booléen
      bAffTache		est un booléen
      
      
      TBLH_SelUS.AffichageActif = faux
      TableSupprimeTout(TBLH_SelUS)
      znPremLigneAff = 1
      
      aOLstEpics <- ClColEpic.from_projet(zoPrj_P).aItems()
      TableauInsère(aOLstEpics, 1, Null)
      
      POUR TOUT ÉLÉMENT oEpic DE aOLstEpics
      	
      	//Ajout du projet dans la liste
      	nLigneEpic = TableAjouteLigne(TBLH_SelUS)
      	SI oEpic = Null ALORS
      		
      		TBLH_SelUS[nLigneEpic].COL_Arbre	= zoPrj_P.p_sNom
      		
      		SI Dimension(aOLstEpics) > 1 ALORS
      			TBLH_SelUS[nLigneEpic].COL_Arbre	+= " (Sans groupement)"
      		FIN
      	SINON
      		TBLH_SelUS[nLigneEpic].COL_Arbre	= oEpic.p_sNom
      	FIN
      	TBLH_SelUS[nLigneEpic]..CouleurFond	= COL_Scrum.gnCouleur_Gris_TClair
      	
      	
      	//Liste de toutes les user stories
      	POUR TOUT ÉLÉMENT oUS DE ClColUserStory.oFromProjet(zoPrj_P).aitems()	//zoPrj_P.aoListeUS().aItems()
      		bAffUS = faux
      		
      		//Si on est sur la bonne epic ou si on est 'sans epic'
      		SI oUS.p_oEpic = oEpic OU (oUS.p_oEpic = Null ET oEpic = Null) ALORS
      			
      			//Doit on tout afficher ?
      			si INT_SansNom1 ALORS
      				//Oui
      				bAffUS = Vrai
      			sinon
      				//Non, on affiche que ce qui est en cours.
      				si oUS.Get_EtatUS() pas dans (ClDbUserStory.ECompteNbUS.Terminee, ClDbUserStory.ECompteNbUS.Annulée) ALORS
      					bAffUS = Vrai
      				FIN
      			FIN
      			
      		FIN
      		
      		SI bAffUS ALORS
      			
      			sCommentaire = ""
      			si oUS.p_sCMTR_Global <> "" ALORS
      				sCommentaire += [rc] + [
      				Commentaire global :
      				[%oUS.p_sCMTR_Global%]
      				]
      			FIN
      			si oUS.p_sCMTR_RetourDeTest <> "" ALORS
      				sCommentaire += [RC] + [
      				Retour de test :
      				[%oUS.p_sCMTR_RetourDeTest%]
      				]
      			FIN
      			si oUS.p_sCMTR_Mise_En_Prod <> "" ALORS
      				sCommentaire += [RC] + [
      				Infos de mise en prod :
      				[%oUS.p_sCMTR_Mise_En_Prod%]
      				]
      			FIN
      			
      			//Ajoute de l'User story dans la liste
      			nLigneUs = TableAjouteFils(TBLH_SelUS, nLigneEpic)
      			TBLH_SelUS[nLigneUs].COL_Arbre			= oUS.sNommination_US()
      			TBLH_SelUS[nLigneUs].COL_Arbre..Bulle	= sCommentaire
      			TBLH_SelUS[nLigneUs].COL_IdUS			= oUS.ZF_Get_IdUnique()
      			TBLH_SelUS[nLigneUs].COL_Cmtr			= oUS.p_sCMTR_Global
      			TBLH_SelUS[nLigneUs].COL_Code			= oUS.p_sCodeUserStory
      			TBLH_SelUS[nLigneUs].COL_Statut			= oUS.sLibelleDeEtat()
      			
      			bAlerte = Faux
      			
      			//Parcours de toutes le taches de l'US
      			POUR TOUT ÉLÉMENT oTache DE ClColTache.aoFromUS(oUS).aItems()	//oUS.aoListeTaches().aItems()
      				
      				bAffTache = faux
      				
      				si oTache.p_nStatut pas dans (ClDbTache.cstStatut_realise, ClDbTache.cstStatut_Annulee) ALORS
      					bAffTache = Vrai
      				FIN
      				
      				sCommentaire = [
      					Commentaire :
      					[%oTache.p_sCMTR%]
      					]
      				
      				//Envoie des données de la tache
      				nLigneTache = TableAjouteFils(TBLH_SelUS, nLigneUs)
      				TBLH_SelUS[nLigneTache].COL_Arbre			= oTache.p_sDescription
      				TBLH_SelUS[nLigneTache].COL_Arbre..Bulle	= sCommentaire
      				TBLH_SelUS[nLigneTache].COL_IdTache			= oTache.Get_IdUnique()
      				TBLH_SelUS[nLigneTache].COL_Cmtr			= oTache.p_sCMTR
      				TBLH_SelUS[nLigneTache].COL_Code			= oTache.p_sCodeTache
      				TBLH_SelUS[nLigneTache].COL_Statut			= oTache.sLibDeStatut()
      				
      				si oTache.p_nNum_Sprint_Realisation <> 0 alors
      					oSprint = ClDbSprint.GetObj(oTache.p_nNum_Sprint_Realisation)
      					TBLH_SelUS[nLigneTache].COL_Dates		= oSprint.sSprint_Date()
      				fin
      				
      				//Mise en évidence du texte recherché (si identifié sur la ligne)
      				VisuRech(nLigneTache, SAI_Recherche)
      				
      				//Mise en évidence des taches avec un événement à régler.
      				SI oTache.p_nStatut DANS (ClDbTache.cstStatut_a_coter, ClDbTache.cstStatut_Suspendu) ALORS
      					TBLH_SelUS[nLigneTache].COL_Cmtr..CouleurFond = COL_Scrum.gnCouleur_Orange_pale
      					bAlerte = Vrai
      				FIN
      			FIN
      			
      			//Mise en évidence du texte recherché (si identifié sur la ligne)
      			VisuRech(nLigneUs, SAI_Recherche)
      			
      			//Si on a une alerte et qu'on doit l'afficher ou si on doit tout afficher
      			SI (bAlerte ET SEL_Affichage = 1) OU 
      					SEL_Affichage = 4 ALORS
      				//On laisse ouvert
      			SINON
      				//On referme
      				TableEnroule(TBLH_SelUS,nLigneUs)
      			FIN
      		FIN
      		
      		
      		SI SEL_Affichage DANS (2) ALORS
      			TableEnroule(TBLH_SelUS,nLigneEpic)
      		FIN
      	FIN
      FIN
      
      si znPremLigneAff > 1 ALORS
      	TablePosition(TBLH_SelUS,znPremLigneAff, taVisible)
      FIN
      TBLH_SelUS.AffichageActif = vrai
      
      
     type : 458752
   -
     name : VisuRech
     procedure_id : 1474659626595936245
     type_code : 14
     code : |1+
      procedure VisuRech(nLigne_P est un entier, sValRecherche_p est une chaîne):vide
      
      bTrouve est un booléen
      
      //Si on a un texte de recherche, on le met en évidence dans la cellule où il est trouvé
      si sValRecherche_p <> "" ALORS
      	si ChaîneOccurrence(TBLH_SelUS.COL_Arbre[nLigne_P],sValRecherche_p,SansCasse) > 0 ALORS
      		TBLH_SelUS.COL_Arbre[nLigne_P]..CouleurFond = COL_Scrum.gnCouleur_OrangeVif
      		bTrouve = vrai
      	FIN
      	si ChaîneOccurrence(TBLH_SelUS.COL_Cmtr[nLigne_P],sValRecherche_p,SansCasse) > 0 ALORS
      		TBLH_SelUS.COL_Cmtr[nLigne_P]..CouleurFond = COL_Scrum.gnCouleur_OrangeVif
      		bTrouve = Vrai
      	FIN
      	si ChaîneOccurrence(TBLH_SelUS.COL_Code[nLigne_P],sValRecherche_p,SansCasse) > 0 ALORS
      		TBLH_SelUS.COL_Code[nLigne_P]..CouleurFond = COL_Scrum.gnCouleur_OrangeVif
      		bTrouve = Vrai
      	FIN
      FIN
      
      si bTrouve et znPremLigneAff = 1 ALORS
      	znPremLigneAff = nLigne_P
      FIN
     type : 458752
