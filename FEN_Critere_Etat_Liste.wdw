  -
    name : BTN_SupprimerCetEtat
    identifier : 0x2e1db2a707dc76ba
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         
         //On cherche L’état utilisé dans la table criteres d'acceptation.
         
         HLitRecherche(Scr_CritereAccept, IDScr_CritereAccept_Etat, TBL_Scr_CritereAccept_Etat.COL_IDScr_CritereAccept_Etat, hIdentique+hLimiteParcours)
         si HTrouve(Scr_CritereAccept) alors
         	info("Cet État est utilisé dans les critère d'acceptation, Suppression impossible")
         	retour
         sinon
         	//Suppression État
         
         	TitreSuivant("Supprimer État")
         	
         	SELON Dialogue(<§?1E1C857E01174BEA§>)
         		// &Supprimer
         		CAS 1
         			
         			HLitRecherche(Scr_CritereAccept_Etat, IDScr_CritereAccept_Etat, COL_IDScr_CritereAccept_Etat, hIdentique+hLimiteParcours)
         			si HTrouve(Scr_CritereAccept_Etat) = vrai alors
         				
         				si pas HSupprime(Scr_CritereAccept_Etat) alors
         					Erreur(HErreurInfo(hErrComplet))
         				FIN
         				
         				ChargementListeEtat()
         			FIN								
         		CAS 2
         	FIN	
         	
         FIN
        type : 18
      -
        type : 161
        enabled : false
