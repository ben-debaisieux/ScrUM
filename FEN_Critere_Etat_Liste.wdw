      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE ChargementListeEtat()
      
      Sd_Source_Liste_Etats_Critere est une requête SQL = 
      [
      	SELECT Libelle, IDScr_CritereAccept_Etat FROM Scr_CritereAccept_Etat
      ] 
      
      si HExécuteRequête(Sd_Source_Liste_Etats_Critere) = Vrai alors
      	
      	//vidage de la table
      	
      	TableSupprimeTout(TBL_Scr_CritereAccept_Etat)
      	
      	//Chargement de la table avec les résultats de la requête.
      	POUR TOUT	Sd_Source_Liste_Etats_Critere
      		TableAjouteLigne(TBL_Scr_CritereAccept_Etat, Sd_Source_Liste_Etats_Critere.IDScr_CritereAccept_Etat, Sd_Source_Liste_Etats_Critere.Libelle)
      	FIN
      	
      sinon
      	//Affichage Message Erreur
      	
      	
      fin
      
      // Libération Requete serveur
      HAnnuleDéclaration(Sd_Source_Liste_Etats_Critere)
