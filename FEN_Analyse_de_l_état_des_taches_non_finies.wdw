    type : 35
    controls : []
    code_elements :
     type_code : 89
     p_codes :
      -
        code : |1+
         oPrj est un ClDbProjet dynamique
         oUS est un ClDbUserStory dynamique
         oTache est un ClDbTache dynamique
         
         nLignePrj est un entier
         nLigneUS est un entier
         nLigneTahe est un entier
         
         TBLH_Taches_non_finies..AffichageActif = faux
         
         pour tout element oPrj de ClColProjet.aoListeProjets().aItems()
         	
         	si znIdProjet_P dans (0, oPrj.ZF_Get_IdUnique()) alors
         		
         		nLignePrj = TableAjouteLigne(TBLH_Taches_non_finies)
         		TBLH_Taches_non_finies[nLignePrj].COL_Taches	= oPrj.p_sNom
         		
         		POUR TOUT ELEMENT oUS DE ClColUserStory.oFromProjet(oPrj).aitems()	//oPrj.aoListeUS().aItems()
         			nLigneUS = TableAjouteFils(TBLH_Taches_non_finies,nLignePrj)
         			TBLH_Taches_non_finies[nLigneUS].COL_Taches	= oUS.p_sCodeUserStory + " - " + oUS.sNommination_US()
         			
         			pour tout element oTache de ClColTaches.aoFromUS(oUS).aItems()	//oUS.aoListeTaches().aItems()
         				si 0 < oTache.p_nNum_Sprint_Demarrage <= goapp.m_nNumeroDuSprintActuel et ...
         						pas oTache.p_bAnnule ET ...
         						PAS oTache.p_bBrouillon ET ...
         						oTache.p_nNum_Sprint_Fin = 0 ALORS
         					
         					nLigneTahe = TableAjouteFils(TBLH_Taches_non_finies,nLigneUS)
         					TBLH_Taches_non_finies[nLigneTahe].COL_Taches	= oTache.p_sCodeTache + " - " + oTache.p_sDescription
         					TBLH_Taches_non_finies[nLigneTahe].COL_Poids	= oTache.p_nPoids
         					
         					TBLH_Taches_non_finies[nLigneUS].COL_Poids		+= oTache.p_nPoids
         					TBLH_Taches_non_finies[nLignePrj].COL_Poids		+= oTache.p_nPoids
         					SAI_Total										+= oTache.p_nPoids
         				FIN
         			FIN
         			
         			//Si l'US n'a pas de tache en test, on supprime la ligne
         			SI TBLH_Taches_non_finies[nLigneUS].COL_Poids = 0 ALORS
         				TableSupprime(TBLH_Taches_non_finies, nLigneUS)
         			SINON
         				SI znIdProjet_P <> 0 ALORS
         					TableEnroule(TBLH_Taches_non_finies, nLigneUS)
         				FIN
         			FIN
         		FIN
         		
         		//Si le projet n'a pas de tache en test, on supprime la ligne
         		SI TBLH_Taches_non_finies[nLignePrj].COL_Poids = 0 ALORS
         			TableSupprime(TBLH_Taches_non_finies, nLignePrj)
         		FIN
         	FIN
         FIN
         
         si znIdProjet_P = 0 ALORS
         	TableEnrouleTout(TBLH_Taches_non_finies)
         	SAI_Objectif = goApp.m_nVelociteMoyenne * cstObjectifChargeCumule
         sinon
         	SAI_Objectif = goApp.m_nVelociteMoyenne * cstObjectifChargeCumuleTies
         FIN
         
         TableTrie(TBLH_Taches_non_finies, "-" + TBLH_Taches_non_finies.COL_Poids..nom)
         TableSelectPlus(TBLH_Taches_non_finies, 1)
         
         TBLH_Taches_non_finies..AffichageActif = vrai
         
