  procedures :
   -
     name : Actualise
     procedure_id : 1607207754769593631
     type_code : 14
     code : |1+
      procédure Actualise() <UI>:vide
      
      sLib	est une chaine
      nVal	est un entier
      nSerie	est un entier
      nCouleur est un entier
      
      grSupprimeTout(GRF_Avancement)
      grType(GRF_Avancement,grDonut)
      grTitre(GRF_Avancement, "Avancement sprint """ + goapp.m_nNumeroDuSprintActuel + """")
      
      pour tout nVal, sLib de Graph_EtatSprint()
      	nSerie++
      	
      	selon sLib
      //		CAS ClDbTache::sLibDeStatut(ClDbTache::cstStatut_Brouillon)
      //			nCouleur = COL_Scrum.gnCouleur_Gris_TClair
      		CAS ClDbTache::sLibDeStatut(ClDbTache::cstStatut_a_coter)
      			nCouleur = COL_Scrum.gnCouleur_OrangeVif
      		CAS ClDbTache::sLibDeStatut(ClDbTache::cstStatut_a_faire)
      			nCouleur = COL_Scrum.gnCouleur_BleuVif
      		CAS ClDbTache::sLibDeStatut(ClDbTache::cstStatut_en_cours)
      			nCouleur = COL_Scrum.gnCouleur_Vert_pale
      			
      		CAS ClDbTache::sLibDeStatut(ClDbTache::cstStatut_realise) + rc + ClDbUserStory::sNomDeEtat(ClDbUserStory.ECompteNbUS.EnCours)
      			nCouleur = COL_Scrum.gnCouleur_BleuPale
      	
      		CAS ClDbTache::sLibDeStatut(ClDbTache::cstStatut_realise) + rc + ClDbUserStory::sNomDeEtat(ClDbUserStory.ECompteNbUS.En_test_fonctionnel)
      			nCouleur = COL_Scrum.gnCouleur_Orange_pale
      	
      		CAS ClDbTache::sLibDeStatut(ClDbTache::cstStatut_realise) + rc + ClDbUserStory::sNomDeEtat(ClDbUserStory.ECompteNbUS.Terminee)
      			nCouleur = COL_Scrum.gnCouleur_VertVif
      			
      		CAS ClDbTache::sLibDeStatut(ClDbTache::cstStatut_Annulee)
      			nCouleur = COL_Scrum.gnCouleur_Gris
      		CAS ClDbTache::sLibDeStatut(ClDbTache::cstStatut_Suspendu)
      			nCouleur = COL_Scrum.gnCouleur_JauneVif
      		autres cas 
      			nCouleur = COL_Scrum.gnCouleur_Gris_TFonce
      			trace(sLib)
      	FIN
      	
      	grEtiquetteCatégorie(GRF_Avancement,nSerie, sLib)
      	grCouleurCatégorie(GRF_Avancement,nSerie, nCouleur)
      	grAjouteDonnée(GRF_Avancement,1,nSerie, nVal)
      FIN
      
      grDessine(GRF_Avancement)
      
      
      
      
