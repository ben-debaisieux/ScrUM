      PROCEDURE FEN_Sprint_Retrospective(LOCAL nSprint_P est un entier = 0)
      
      
      zoSprint est un ClSprint dynamique
      
      
      //A t on un N° de sprint spécifié ?
      si nSprint_P = 0 ALORS
      	//Non
      	zoSprint <- ClSprint.GetObj(goApp.m_nNumeroDuSprintActuel)
      sinon
      	//Oui
      	zoSprint <- ClSprint.GetObj(nSprint_P)
      FIN
