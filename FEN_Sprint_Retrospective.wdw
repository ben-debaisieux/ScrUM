   -
     name : Liste_Tache_Realisee
     procedure_id : 2081358224144955802
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Liste_Tache_Realisee ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      //	nSprint_p (entier) : <indiquez ici le rôle de nSprint_p>
      PROCEDURE Liste_Tache_Realisee()
      
      nLigne		est un entier
      oTache		est un ClDbTache dynamique
      oColTache	est un ClcolTache
      sReq		est une chaîne
      
      TBL_TachesRealise..AffichageActif = Faux
      TableSupprimeTout(TBL_TachesRealise)
      
      sReq = [
      	SELECT 
      		Scr_Tache.*
      	FROM Scr_Tache
      	WHERE Scr_Tache.Num_Sprint_Fin = [%zoSprint.ZF_Get_IdUnique()%]
      		and Scr_Tache.Top_Brouillon = 0
      		and Scr_Tache.Top_Annule = 0	
      ]
      oColTache.ZF_LoadSQL(sReq)
      
      pour TOUT element oTache de oColTache.aItems() 
      	nLigne										= TableAjouteLigne(TBL_TachesRealise)
      	TBL_TachesRealise[nLigne].COL_ID				= oTache.Get_IdUnique()
      	TBL_TachesRealise[nLigne].COL_Code				= oTache.p_sCodeTache
      	TBL_TachesRealise[nLigne].COL_Description		= oTache.p_sDescription
      	TBL_TachesRealise[nLigne].COL_Sprint_Initial	= oTache.p_nNum_Sprint_Demarrage
      	TBL_TachesRealise[nLigne].COL_Poids			= oTache.p_nPoids		
      FIN
      
