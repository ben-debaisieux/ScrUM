     name : Liste_Tache_Realisee
     procedure_id : 2081358224144955802
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Liste_Tache_Realisee ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      //	nSprint_p (entier) : <indiquez ici le rôle de nSprint_p>
      PROCEDURE Liste_Tache_Realisee()
      
      nLigne est un entier
      
      sdRechercheTachesRealisees est une source de Données
      sReq est une chaîne
      
      TBL_TachesRealise..AffichageActif = Faux
      TableSupprimeTout(TBL_TachesRealise)
      
      sReq = ChaîneConstruit([
      	SELECT 
      		Scr_Tache.IDScr_Tache,
      		Scr_Tache.CodeTache,
      		Scr_Tache.Description,
      		Scr_Tache.Num_Sprint_Demarrage,
      		Scr_Tache.Poids
      	FROM Scr_Tache
      	WHERE Scr_Tache.Num_Sprint_Fin = %1
      		and Scr_Tache.Top_Brouillon = 0
      		and Scr_Tache.Top_Annule = 0	
      ], zoSprint.Get_IdUnique())
      
      SI goApp.Rsql(sdRechercheTachesRealisees,sReq) ALORS
      	
      	pour TOUT sdRechercheTachesRealisees 
      		nLigne = TableAjouteLigne(TBL_TachesRealise)
      		TBL_TachesRealise[nLigne].COL_ID = sdRechercheTachesRealisees.IDScr_Tache
      		TBL_TachesRealise[nLigne].COL_Code = sdRechercheTachesRealisees.CodeTache
      		TBL_TachesRealise[nLigne].COL_Description = sdRechercheTachesRealisees.Description
      		TBL_TachesRealise[nLigne].COL_Sprint_Initial = sdRechercheTachesRealisees.Num_Sprint_Demarrage
      		TBL_TachesRealise[nLigne].COL_Poids = sdRechercheTachesRealisees.Poids		
      	FIN
      	
      FIN
      
      HAnnuleDéclaration(sdRechercheTachesRealisees)
      
      TBL_TachesRealise..AffichageActif = Vrai
     type : 458752
   -
     name : Retrospective
     procedure_id : 2081358224145021338
     type_code : 14
     code : |1+
      // Résumé : Affiche dans le tableau de la rétrospective les differents champs lié au sprint
      // Syntaxe :
      //Retrospective ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      //	nIDSprint_p (entier) : <indiquez ici le rôle de nIDSprint>
      PROCEDURE Retrospective()
      
      nLigne est un entier 
      
      sdRechercheCommentaire est une source de Données
      sReq est une chaîne
      
      
      ZR_Retrospective..AffichageActif = Faux
      TableSupprimeTout(ZR_Retrospective)
      
      
      sReq = ChaîneConstruit([
      	SELECT
      		Scr_Type_Cmtr.IDScr_Type_Cmtr,
      		Scr_Type_Cmtr.Type_Cmtr,
      		Scr_Type_Cmtr.Top_Par_Defaut,
      		
      		TEMP_Cmtr.IDScr_Commentaire,
      		TEMP_Cmtr.CMTR
      			
      	FROM Scr_Type_Cmtr
      		LEFT OUTER JOIN (
      			SELECT
      				Scr_Commentaire.IDScr_Commentaire,
      				Scr_Commentaire.IDScr_Type_Cmtr,
      				Scr_Commentaire.CMTR
      			
      			FROM Scr_Commentaire
      			
      			WHERE Scr_Commentaire.Num_Sprint = %1
      		) TEMP_Cmtr ON (TEMP_Cmtr.IDScr_Type_Cmtr = Scr_Type_Cmtr.IDScr_Type_Cmtr)
      	
      ], zoSprint.Get_IdUnique())
      
      nligcbo est un entier
      
      Si goApp.Rsql(sdRechercheCommentaire,sReq) ALORS
      	
      	
      	pour TOUT sdRechercheCommentaire 
      	
      		si sdRechercheCommentaire.Top_Par_Defaut = vrai _OU_ sdRechercheCommentaire.IDScr_Commentaire <> 0 alors
      						
      			nLigne = TableAjouteLigne(ZR_Retrospective)
      		
      			ZR_Retrospective[nLigne].COL_Commentaire.SAI_ID = sdRechercheCommentaire.IDScr_Commentaire
      			
      			nligcbo = listecherche(CBO_Type_Cmtr_ref,sdRechercheCommentaire.IDScr_Type_Cmtr,vrai,1)
      			ZR_Retrospective.COL_Commentaire[nLigne].CBO_Type_Cmtr = CBO_Type_Cmtr_Ref[nligcbo]..ValeurAffichée
      			
      			ZR_Retrospective[nLigne].COL_Commentaire.SAI_Commentaire = sdRechercheCommentaire.CMTR
      			ZR_Retrospective[nLigne].COL_Commentaire.INT_Defaut = 1
      			
      			//Les commentaires déja remplis sont grisés
      			si ZR_Retrospective[nLigne].COL_Commentaire.SAI_ID <> 0 alors
      				ZR_Retrospective[nLigne].COL_Commentaire.SAI_Commentaire..grisé = vrai
      			fin
      		fin
      	FIN
      FIN
      
      HAnnuleDéclaration(sdRechercheCommentaire)
      
      ZR_Retrospective..AffichageActif = Vrai
