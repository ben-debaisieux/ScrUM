    name : BTN_Stats
    identifier : 0x222514fb12258aed
    internal_properties : 
    properties :
     x : 6
     y : 292
     width : 100
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         
         oPrj				est un ClDbProjet dynamique
         oUS					est un ClDbUserStory dynamique
         oTache				est un ClDbTache dynamique
         
         
         nNbACoter est un entier
         nNbACoterEtDemarre est un entier
         nNbSuspendu est un entier
         nNbEnRetard est un entier
         
         nNbUS_EnTest est un entier
         
         
         POUR TOUT ELÉMENT oPrj DE ClColProjet.aoListeProjets().aItems()
         	POUR TOUT ELÉMENT oUS DE ClColUserStory.oFromProjet(oPrj).aitems()	//oPrj.aoListeUS(Vrai).aItems()
         		POUR TOUT ELÉMENT oTache DE ClColTache.aoFromUS(oUS).aItems()	//oUS.aoListeTaches().aItems()
         			selon oTache.p_nStatut 
         				
         				cas ClDbTache.cstStatut_Suspendu
         					nNbSuspendu++
         					
         				cas ClDbTache.cstStatut_a_coter
         					nNbACoter++
         					
         					si 1 <= oTache.p_nNum_Sprint_Realisation < goApp.Sprint_Num_Actuel() ALORS
         						nNbACoterEtDemarre++
         					FIN
         
         				CAS ClDbTache.cstStatut_a_faire
         				
         					SI 1 <= oTache.p_nNum_Sprint_Realisation < goApp.Sprint_Num_Actuel() ALORS
         						nNbEnRetard++
         					FIN
         			AUTRE CAS
         				
         			FIN
         			
         
         		FIN
         		
         		selon oUS.Get_EtatUS()
         			cas ClDbUserStory.ECompteNbUS.En_test_fonctionnel
         				nNbUS_EnTest++
         			AUTRE CAS
         				
         		FIN
         	FIN
         FIN
         
         
         
         trace("Taches : ")
         trace("A coter : " + nNbACoter)
         trace("A coter et démarrée : " + nNbACoterEtDemarre)
         trace("Suspendue : " + nNbSuspendu)
         trace("En retard : " + nNbEnRetard)
         
         Trace("User story : ")
         Trace("En test : " + nNbUS_EnTest)
         
        type : 18
