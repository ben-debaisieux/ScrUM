 controls :
  -
    name : BTN_Enregistrer
    identifier : 0x2ce2784704e2799a
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         nLigneTypeCom est un entier
         bRetrospectiveValide est un booléen = vrai
         
         // Parcours de contrôles
         POUR i = 1 _a_ TableOccurrence(ZR_Retrospective)
         	si ZR_Retrospective[i].COL_Commentaire.INT_Defaut = 1 ET ZR_Retrospective[i].COL_Commentaire.SAI_Commentaire = "" alors
         		erreur("Vous devez remplir les champs par défaut pour enregistrer la rétrospective.")
         		bRetrospectiveValide = faux
         		Sortir
         	FIN
         fin
         
         si bRetrospectiveValide alors
         	
         	Pour i = 1 _a_ TableOccurrence(ZR_Retrospective)
         		
         		HLitRecherche(Scr_Commentaire,IDScr_Commentaire,ZR_Retrospective[i].COL_Commentaire.SAI_ID,hIdentique+hLimiteParcours)
         		
         		//Si le commentaire existe déja, on le modifie
         		si Htrouve(Scr_Commentaire) alors
         			Scr_Commentaire.CMTR = ZR_Retrospective[i].COL_Commentaire.SAI_Commentaire
         			
         			si pas HModifie(Scr_Commentaire) alors
         				Erreur(ErreurInfo(errComplet))
         			FIN
         			
         		sinon
         			//Si le champ Commentaire n'est pas rempli, il ne faux pas l'enregistrer
         			si ZR_Retrospective[i].COL_Commentaire.SAI_Commentaire <> "" alors
         				
         				HRAZ(Scr_Commentaire)
         				Scr_Commentaire.Num_Sprint		= zoSprint.Get_IdUnique()
         				Scr_Commentaire.CMTR			= ZR_Retrospective[i].COL_Commentaire.SAI_Commentaire
         				
         				gLienActive(CBO_Type_Cmtr_Ref,faux)
         				nLigneTypeCom = ListeCherche(CBO_Type_Cmtr_Ref, ZR_Retrospective[i].COL_Commentaire.CBO_Type_Cmtr)
         				gLienActive(CBO_Type_Cmtr_Ref,vrai)
         				
         				Scr_Commentaire.IDScr_Type_Cmtr	= CBO_Type_Cmtr_Ref[nLigneTypeCom]
         				
         				SI PAS Hajoute(Scr_Commentaire) ALORS
         					Erreur(ErreurInfo(errComplet))
         				FIN
         			FIN
         		fin
         	FIN
         	
         	
         	zoSprint.p_bTop_Termine = vrai
         
         	quand exception dans
         		zoSprint.Enregistre()
         	FAIRE
         		erreur(ExceptionInfo(errMessage))
         	sinon
         		
         		ClSprint.Calcul_Velocite_Moyenne()
         		
         		//Recalcul du n° de sprint
         		ClSprint.Sprint_Num_Actuel()
         		Info("Rétrospective enregistrée.")
         		Ferme()
         	FIN
         	
         fin
        type : 18
      -
        type : 161
        enabled : false
