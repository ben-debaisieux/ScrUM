         -
           code : |1+
            oTache est un ClTache dynamique
            nIdTace est un entier = TBL_taches.COL_idTache
            bModif est un booléen
            
            si nIdTace > 0 ALORS
            	oTache <- ClTache.GetObj(nIdTace)
            	
            	quand Exception dans
            		oTache.p_nPoids_Previsionnel = TBL_taches.COL_Poids_Previ
            		bModif = oTache.bEstModifie()
            		si bModif ALORS
            			oTache.Enregistre()
            		FIN
            	FAIRE
            		info("Echec de l'enregistrement :" + rc + ExceptionInfo(errMessage))
            	sinon
            		si bModif ALORS
            			ToastAffiche("Enregistrement réussi",toastLong)
            		FIN
            	FIN
            FIN
