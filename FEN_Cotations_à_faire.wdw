  procedures :
   -
     name : AffTaches
     procedure_id : 1320832074228800525
     type_code : 14
     code : |1+
      procédure AffTaches()
      
      
      
      oPrj		est un ClProjet dynamique
      oUs			est un ClUserStory dynamique
      oTache_Tmp	est un ClTache dynamique
      nNumLigne	est un entier
      sGrp		est une chaîne
      
      
      TBL_taches..AffichageActif = Faux
      
      TableSupprimeTout(TBL_taches)
      
      
      POUR TOUT ELÉMENT oPrj DE ClProjet.aoListeProjets()
      	POUR TOUT ELÉMENT oUs DE oPrj.aoListeUS()
      		
      		POUR TOUT ELÉMENT oTache_Tmp DE oUs.aoListeTaches()
      			
      //			Trace("Tache : " + oTache_Tmp.p_sCodeTache)
      //			Trace("Aff" + oTache_Tmp:p_nPoids + "-" + oTache_Tmp:p_nPoids_Previsionnel + "-" + oTache_Tmp:p_bAnnule + "-" + oTache_Tmp.p_bBrouillon)
      			
      			SI oTache_Tmp.bCotationAFaire() ALORS
      				
      				si oUs.p_oEpic = Null ALORS
      					sGrp = ""
      				sinon
      					sGrp = rc + oUs.p_oEpic.p_sNom
      				FIN
      				
      				
      				nNumLigne									= TableAjouteLigne(TBL_taches)
      				TBL_taches[nNumLigne].COL_idTache			= oTache_Tmp.Get_IdUnique()
      				TBL_taches[nNumLigne].COL_Projet			= oUs.p_oProjetParent.p_sNom + sGrp
      				TBL_taches[nNumLigne].COL_US				= oUs.p_sCodeUserStory +  " - " + oUs.sNommination_US()
      				TBL_taches[nNumLigne].COL_US..Bulle			= oUs.p_sCMTR_Global
      				TBL_taches[nNumLigne].COL_Tache				= oTache_Tmp.p_sCodeTache + " - " + oTache_Tmp.p_sDescription
      				TBL_taches[nNumLigne].COL_Tache..Bulle		= oTache_Tmp.p_sCMTR
      				TBL_taches[nNumLigne].COL_Cmtr				= oTache_Tmp.p_sCMTR
      				TBL_taches[nNumLigne].COL_Sprint			= oTache_Tmp.p_nNum_Sprint_Realisation
      				TBL_taches[nNumLigne].COL_Statut			= oTache_Tmp.sLibDeStatut()
      				TBL_taches[nNumLigne].COL_Poids_Previ		= oTache_Tmp.p_nPoids_Previsionnel
      				TBL_taches[nNumLigne].COL_Docs				= Dimension(oUs.asCheminsDocs())
      				
      				SI oTache_Tmp.p_nNum_Sprint_Realisation <= goApp.m_nNumeroDuSprintActuel ET oTache_Tmp.p_nNum_Sprint_Realisation <> 0 ALORS
      					TBL_taches[nNumLigne].COL_Sprint..CouleurFond = COL_Scrum.gnCouleur_OrangeVif
      				FIN
      			FIN
      		FIN
      	FIN
      FIN
      
      TableTrie(TBL_taches, TBL_taches.COL_Projet..Nom,TBL_taches.COL_US..Nom,TBL_taches.COL_Tache..Nom)
      
      
      TableSelectPlus(TBL_taches,1)
      
      TBL_taches..AffichageActif = Vrai
      
