     type_code : 14
     code : |1+
      PROCÉDURE Actualise()
      
      
      
      oPrjTmp		est un ClProjet dynamique
      aoProjets	est un tableau de ClProjet dynamique
      
      
      SI goApp.m_oUsrCnx.p_bTop_Equip_Informatique ALORS
      	aoProjets <- ClProjet.aoListeProjets(Faux)
      SINON
      	aoProjets <- goApp.m_oUsrCnx.aoMesProjets(ClUsr.usr_tous)
      FIN
      
      
      ListeSupprimeTout(CBO_Projet)
      
      
      POUR TOUT ELEMENT oPrjTmp DE aoProjets
      	ListeAjoute(CBO_Projet, oPrjTmp.p_sNom + gLien(oPrjTmp.Get_IdUnique()))	
      FIN
      
      
      ListeTrie(CBO_Projet)
      ListeInsère(CBO_Projet, "<Choisir...>" + gLien(0), 1)	
      ListeSelectPlus(CBO_Projet, 1)
      
      
      grSupprimeTout(GRF_AvancementPrj)
      
