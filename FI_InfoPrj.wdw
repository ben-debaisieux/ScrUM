     code : |1+
      PROCÉDURE Actualise()<UI>:Vide
      
      
      
      aoProjets	est un ClColProjet
      
      
      SI goApp.m_oUsrCnx.p_bTop_Equip_Informatique ALORS
      	aoProjets <- ClColProjet.aoListeProjets(Faux)
      SINON
      	aoProjets <- ClColProjet.aoListeProjetsDeUsr(goApp.m_oUsrCnx)
      FIN
      
      
      ListeSupprimeTout(CBO_Projet)
      
      aoProjets.FiltreDirection(Faux).ZF_ChargeCbo(CBO_Projet..Nom)
      
      ListeTrie(CBO_Projet)
      ListeInsère(CBO_Projet, "<Choisir...>" + gLien(0), 1)	
      ListeSelectPlus(CBO_Projet, 1)
      
      
      grSupprimeTout(GRF_AvancementPrj)
