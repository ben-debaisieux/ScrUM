     -
       name : BTN_SansNom1
       identifier : 0x252e41950591b956
       internal_properties : 
       properties :
        x : 291
        y : 2
        width : 106
        height : 24
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1-
            oPrj				est un ClDbProjet(ClZF_ObjDbLs.cstModeCreation)
            aListe_Categorie	est un tableau de chaine
            nID_Groupement		est un entier
            asComplément		est un tableau associatif de chaînes
            stPowerPoint		est un STR_POWERPOINT
            aIMG_Graph est un tableau de chaine
            
            GRF_AvancementPrj..Visible = faux
            
            oPrj = ClDbProjet.GetObj(CBO_Projet)
            
            
            SI CBO_Projet <> 0 ALORS
            	asComplément[cst_cbo_tous]	= "<Tous>"
            	aListe_Categorie			= ClColEpic.aliste_charge_From_Projet(oPrj,asComplément)
            	
            	POUR nIndice = 1 _À_ TableauOccurrence(aListe_Categorie)
            		
            		oEpic est un ClDbEpic dynamique
            		
            		nID_Groupement = val(ExtraitChaîne(aListe_Categorie[nIndice],1,"=",DepuisDébut))
            		
            		SI nID_Groupement <> cst_cbo_tous ALORS
            			oEpic <- ClDbEpic.GetObj(nID_Groupement)
            		FIN
            		
            //		SI CBO_Projet <> 0 ALORS
            			COL_Graphes.InfoProjet(GRF_AvancementPrj, oPrj, oEpic)
            //		FIN
            		
            		sFic		est une chaîne	= fRepTemp() + "\Graphe[%DonneIdentifiant()%].bmp"
            		grSauveBMP(GRF_AvancementPrj, sFic)
            		
            		TableauAjoute(aIMG_Graph,sFic)
            		// Ajout image dans une slide
            		stPowerPoint = PowerPoint_Ajout_Une_Slide(stPowerPoint,"","",sFic)
            		
            		// récupération US
            		SI CBO_Projet <> 0 _et_ nID_Groupement <> cst_cbo_tous ALORS
            			sTitre		est une chaine
            			sContenu	est une chaine
            			
            			sTitre = "Avancement du projet " + oPrj.p_sNom
            			sTitre += [RC] + oEpic.p_sNom
            			
            			POUR TOUT ÉLÉMENT oUs DE ClColUserStory.oFromProjet(oPrj, Vrai).aItems()
            				bAffUs est un booléen = vrai
            				
            				//L'US a t elle un groupement ?
            				SI oUs.p_oIDScr_Epic = Null ALORS
            					//Non
            					bAffUs = Faux
            				SINON
            					//Oui
            					
            					//Est ce le bon ?
            					SI oUs.p_oIDScr_Epic.ZF_Get_IdUnique() <> nID_Groupement ALORS
            						//Non
            						bAffUs = Faux
            					sinon
            						//est ce que l'US est terminée ?
            						si oUs.p_bTermine ALORS
            							//Oui : on la masque
            							bAffUs = Faux
            						FIN
            					FIN
            				FIN
            				
            				//Faut il afficher l'US ?
            				SI bAffUs ALORS
            					sContenu += [RC] +  oUs.p_sCodeUserStory + " - " + oUs.sLibelleDeEtat() + " - " + remplace(oUs.sNommination_US(), rc, " / ")
            //					sContenu += [RC] +  oUs.p_sCodeUserStory + " " + oUs.p_sAfinDe
            				FIN
            			FIN
            			stPowerPoint = PowerPoint_Ajout_Une_Slide(stPowerPoint,sTitre,sContenu,"")
            		FIN
            	FIN
            FIN
            
            OLE_PowerPoint_Création(stPowerPoint)
            
            // dernier traitement après ajout au powerpoint
            POUR nIndice = 1 _À_ TableauOccurrence(aIMG_Graph)
            	fSupprime(aIMG_Graph[nIndice])
            FIN
            
            // chargement graphique en fonction du groupement choisi
            oEpic est un ClDbEpic dynamique
            SI CBO_Groupement <> cst_cbo_tous ALORS
            	oEpic = ClDbEpic.GetObj(CBO_Groupement)
            FIN
            
            SI CBO_Projet <> 0 ALORS
            	COL_Graphes.InfoProjet(GRF_AvancementPrj..NomComplet, ClDbProjet.GetObj(CBO_Projet), oEpic)
            FIN
            GRF_AvancementPrj..Visible = Vrai
