        p_codes :
         -
           type : 35
           enabled : false
         -
           code : |1-
            si CBO_Projet <> 0 alors
            	oPrjTmp		est un ClProjet dynamique <- ClProjet.GetObj(CBO_Projet)
            	
            	//Récupération des valeurs :
            	nFinies est un entier = oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.Terminee)
            	nATester est un entier = oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.En_test_fonctionnel)
            	nAnnulee est un entier = oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.Annulée)
            	nAFaire est un entier = oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.Toutes) - (nFinies + nATester + nAnnulee)
            	
            	
            	//Affichage des infos dans le graphe :
            	grSupprimeTout(GRF_AvancementPrj)
            	
            	grSMNAjouteDonnée(GRF_AvancementPrj, zcst_Developpe + TAB + zcst_Teste, nFinies)
            	grSMNCouleurPortion(GRF_AvancementPrj,zcst_Developpe, COL_Scrum.gnCouleur_VertVif)
            	grSMNCouleurPortion(GRF_AvancementPrj,zcst_Developpe + TAB + zcst_Teste, COL_Scrum.gnCouleur_BleuPale)
            	
            	grSMNAjouteDonnée(GRF_AvancementPrj, zcst_Developpe + TAB + zcst_Non_Teste, nATester)
            	grSMNCouleurPortion(GRF_AvancementPrj,zcst_Developpe + TAB + zcst_Non_Teste, COL_Scrum.gnCouleur_RougeOrange)
            	
            	grSMNAjouteDonnée(GRF_AvancementPrj, zcst_A_Faire, nAFaire)
            	grSMNCouleurPortion(GRF_AvancementPrj,zcst_A_Faire, COL_Scrum.gnCouleur_JauneVif)
            	
            	grSMNAjouteDonnée(GRF_AvancementPrj, zcst_Annule, nAnnulee)
            	grSMNCouleurPortion(GRF_AvancementPrj,zcst_Annule, COL_Scrum.gnCouleur_Gris)
            	
            	grDessine(GRF_AvancementPrj)
            fin
