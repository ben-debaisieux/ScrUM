    name : BTN_Enregister_les_sprints
    identifier : 0x2e21a8c40373c72d
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         IdFichierXLS est un xlsDocument
         Xlsnbligne_ est un entier
         i est un entier
         sFichier est une chaîne
         
         nPremiereligne est un entier = 2
         
         //Declatration Structure
         stSprint est un str_Sprint
         
         nCol_numSprint est un entier = 1
         nCol_Date_Debut est un entier = 2
         nCol_Date_Fin est un entier = 3
         
         duDuréeSprint est une durée 
         
         
         // Ouvre le sélecteur de fichiers
          
         sFichier = "L:\Service informatique\Service Développement\Normes et procédures de dev\SCRUM.xlsx"
         //sFichier = "L:\Service informatique\Service Développement\Test projet Scrum\Modele SCRUM.xlsx"
         
         
         SI sFichier <> "" ALORS
         	//identifiant du fichier
         	IdFichierXLS = xlsOuvre(sFichier,xlsEcriture)
         	
         	SI ErreurDétectée = Faux ALORS //interception d'une erreur
         		
         		SI xlsFeuilleEnCours(IdFichierXLS, 4) ALORS	//on ouvre la 4eme feuille du fichier
         			
         			Xlsnbligne_ =xlsNbLigne(IdFichierXLS,Vrai) //nombre de ligne à parcourir
         			
         			POUR i = nPremiereligne _À_ Xlsnbligne_
         				
         				//si la ligne n'est pas vide on enregistre les données
         				SI xlsDonnée(IdFichierXLS,i,nCol_Date_Debut) <> "" alors
         					stSprint.nNum_Sprint = xlsDonnée(IdFichierXLS,i, nCol_numSprint)
         					stSprint.dDateDebut = ChaîneVersDate(xlsDonnée(IdFichierXLS,i,nCol_Date_Debut))
         					stSprint.dDateFin = ChaîneVersDate(xlsDonnée(IdFichierXLS,i,nCol_Date_Fin))
         					
         					//calcul de la durée du sprint
         					duDuréeSprint = stSprint.dDatefin - stSprint.dDateDebut
         					stSprint.nNbreSemaines = Arrondi(duDuréeSprint..Jour / 7)
         					
         					COL_Scrum.Sprint_Ajout_deprecated(stSprint) //procédure globale d'ajout du sprint dans la base
         				FIN
         			FIN
         			
         			xlsFerme(IdFichierXLS) // fermeture du fichier
         
         		SINON
         			Erreur(ErreurInfo(errComplet))
         		FIN
         	SINON
         		Erreur(ErreurInfo(errComplet))
         	FIN	
         FIN
         
         
         
         
