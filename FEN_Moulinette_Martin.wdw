      PROCEDURE GetDescriptionById_CritereAccept(local ID_Crit est un entier)
      
      //declaration requete
      sDescriptionRetour est une chaîne
      sdDescription est une source de données
      
      
      GetDescription est une chaine = ChaîneConstruit([
      select Description from Scr_CritereAccept_Composant where IDScr_CritereAccept = %1
      ], ID_Crit)
      
      si goApp.Rsql(sdDescription, GetDescription)
      	HLitPremier(sdDescription) // on lit le premier resultat
      	si Htrouve(sdDescription) ALORS 
      		sDescriptionRetour = sdDescription.Description // si on le trouve, on l'affecte a la variable
      	FIN
      FIN
      HAnnuleDéclaration(sdDescription)
      
      renvoyer sDescriptionRetour //on renvoie la description
