    name : BTN_Projet_terminés
    identifier : 0x2e21ab5403b1c94f
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         aUserStory est un tableau de STR_UserStorie
         
         sdRechercheProjetTermine est une source de données
         sReq est une chaîne
         
         sReq = [
         	SELECT 
         		Scr_UserStory.IDScr_Projet,
         		COUNT(Scr_UserStory.IDScr_UserStory) as NbUSTermine
         	FROM
         		Scr_UserStory
         	WHERE 
         		Scr_UserStory.Top_Termine = 1
         	GROUP BY
         		Scr_UserStory.IDScr_Projet
         			
         ]
         
         SI goApp.Rsql(sdRechercheProjetTermine,sReq) alors
         	pour TOUT sdRechercheProjetTermine
         		aUserStory = COL_Scrum.User_Stories_Tableau_deprecated(sdRechercheProjetTermine.IDScr_Projet)
         		Si TableauOccurrence(aUserStory) = sdRechercheProjetTermine.NbUSTermine ALORS
         			HLitRecherche(Scr_Projet,IDScr_Projet,sdRechercheProjetTermine.IDScr_Projet,hIdentique+hLimiteParcours)
         			si Htrouve(Scr_Projet) alors
         				Scr_Projet.Top_Termine = 1
         				si pas HModifie(Scr_Projet) ALORS
         					Erreur(ErreurInfo(errComplet))
         				FIN
         			sinon
         				Erreur(ErreurInfo(errComplet))
         			FIN
         		FIN
         	FIN
         FIN
         
         HAnnuleDéclaration(sdRechercheProjetTermine)
