      -
        code : |1+
         sdIDUS est une source de données
         sReqGetIDUS est une chaine = ChaîneConstruit([
         
         SELECT
         	Scr_UserStory.CodeUserStory,
         	Scr_UserStory.IDScr_UserStory,
         	Scr_UserStory.EntantQue,
         	Scr_UserStory.JaiBesoinDe,
         	Scr_UserStory.AfinDe
         FROM	
         	Scr_UserStory	
         WHERE
         	Scr_UserStory.IDScr_Projet = %1
         	
         
         ], CBO_ProjectList..ValeurMémorisée)
         // on se sert de la combo d'avant pour la requete
         ListeSupprime(CBO_IDUS)
         SI goApp.Rsql(sdIDUS, sReqGetIDUS) ALORS
         	POUR TOUT sdIDUS
         		// on revoie l'ID user story
         		ListeAjoute(CBO_IDUS, sdIDUS.CodeUserStory +" :  "+ sdIDUS.EntantQue+" "+sdIDUS.JaiBesoinDe+" "+sdIDUS.AfinDe + gLien(sdIDUS.IDScr_UserStory))
         	FIN
         FIN
