  procedures :
   -
     name : Visualisation_sprints
     procedure_id : 2082921287298736954
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Visualisation_sprints (<nSprints> est entier, <nSemaines> est entier)
      //
      // Paramètres :
      //	nSprints (entier) : <indiquez ici le rôle de nNombre_Sprints>
      //	nSemaines (entier) : <indiquez ici le rôle de nSemaines>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Visualisation_sprints(LOCAL nSprints est un entier, LOCAL nSemaines est un entier)
      
      
      i						est un entier
      nLigne					est un entier
      dDateFinDernierSprint	est une date
      nNumDernierSprint		est un entier
      duDureeSprint			est une duree
      
      nNum_Sprint				est un entier
      dDateDebut				est une date
      dDateFin				est une date
      
      //Pour que les sprints commencent le lundi apres le week end
      duWeekEnd				est une Durée
      
      
      duWeekEnd..EnJours = 3
      
      //Recuperation de la date du dernier sprint
      HLitDernier(Scr_Sprint)
      si htrouve(Scr_Sprint) ALORS
      	dDateFinDernierSprint	= Scr_Sprint.DateFin 
      	nNumDernierSprint		= Scr_Sprint.Num_Sprint
      sinon
      	erreur(ErreurInfo(errComplet))
      FIN
      
      //reinitialisation de la table
      TableSupprimeTout(TBL_Visualisation_Sprint)
      TBL_Visualisation_Sprint..AffichageActif = faux
      
      //boucle sur le nombre de sprint a ajouter
      pour i = 1 _a_ nSprints 
      	
      	//remplissage de la structure
      	nNum_Sprint											= nNumDernierSprint + 1
      	dDateDebut											= dDateFinDernierSprint + duWeekEnd
      	
      	//calcul de la duree du sprint
      	duDureeSprint..EnJours								= (nSemaines - 1) * 7 + 4 //on ne compte pas le we sur la derniere semaine
      	dDateFin											= dDateDebut + duDureeSprint
      	
      	//Les variables de derniers sprint deviennent ce sprint
      	dDateFinDernierSprint								= dDateFin 
      	nNumDernierSprint									= nNum_Sprint
      	
      	//Affichage dans la table
      	nLigne												= TableAjouteLigne(TBL_Visualisation_Sprint)
      	TBL_Visualisation_Sprint[nLigne].COL_Num			= nNum_Sprint
      	TBL_Visualisation_Sprint[nLigne].COL_Date_de_début	= dDateDebut
      	TBL_Visualisation_Sprint[nLigne].COL_Date_de_fin	= dDateFin
      	
      FIN
      
      TBL_Visualisation_Sprint..AffichageActif = vrai
     type : 458752
