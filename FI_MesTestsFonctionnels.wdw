  procedures :
   -
     name : Actualise
     procedure_id : 2207207178848303414
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Actualise ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Actualise()
      
      oUs			est un ClDbUserStory dynamique
      nNumLigne	est un entier
      sBulle		est une chaîne
      
      
      TBL_Infos..AffichageActif = faux
      
      TableSupprimeTout(TBL_Infos)
      
      pour tout element oUs de goApp.m_oUsrCnx.aoMesUS_En_Test(ClDbUsr.test_fonctionnel).aItems()
      	
      	sBulle = oUs.p_sCodeUserStory + rc + ...
      		oUs.sNommination_US() + rc + rc + ...
      		"------------------------------" + rc + rc + ...
      		ChaîneConstruit("Testable, à partir du %1, sur l'environnement de ""%2"".", ...
      			DateVersChaîne(oUs.p_dDateMiseADispo_EnTest), ...
      			oUs.sNomEnvironnementTest())
      	
      	
      	//Y a t'il déja eu un retour de test ?
      	si oUs.p_sCMTR_RetourDeTest <> "" ALORS
      		//Oui, on affiche le dernier commentaire.
      		sBulle += rc + rc + "Dernier commentaire de retour de test :" + rc + oUs.p_sCMTR_RetourDeTest
      	FIN
      	
      	
      	nNumLigne = ajoutelignesection(oUs.ZF_Get_IdUnique(), oUs.sNommination_US(), oUs.p_oIDScr_Projet.p_sNom, sBulle)
      fin
      
      
      fi_mestestsfonctionnels.regroupeelts()
      
      //TableTrie(TBL_MesTests,TBL_MesTests.COL_Projet..Nom,TBL_MesTests.COL_IdUS..nom)
      //
      //
      //nInd est un entier
      //POUR nInd = 1 _À_ TableOccurrence(TBL_MesTests)
      //	TBL_Infos[TableIndiceRupture(RUPT_HautRupture1,nInd)].LIB_NomProjet = TBL_MesTests[nInd].COL_Projet
      //FIN
      
      TBL_Infos..AffichageActif = vrai
     type : 458752
   -
     name : AjouteLigneSection
     internal_properties : BwAAAAcAAADgUNxLC/rmRy0QD3eLlR6D56JAs4Vj7dFf52EtuIaW5vepU1CaruS0nPI96EuuC/Bj
     procedure_id : 1585064655800853980
     type_code : 14
     code : |1+
      procédure AjouteLigneSection(nId_p est un entier, sInfo_p est une chaîne, sGrp_p est une chaîne, sBulle_p est une chaîne) <UI>:entier
      
      nRetour est un entier
      nRetour = TableAjouteLigne(TBL_Infos)
      
      TBL_Infos[nRetour].COL_Id			= nId_p
      TBL_Infos[nRetour].COL_Infos		= sInfo_p
      TBL_Infos[nRetour].COL_Infos..Bulle	= sBulle_p
      TBL_Infos[nRetour].COL_Groupement	= sGrp_p
      
      
      renvoyer nRetour
      
     type : 458752
   -
     name : RegroupeElts
     internal_properties : BwAAAAcAAADgUNxLC/rmRy0QD3eLlR6D56JAs4Vj7dFf52EtuIaW5vepU1CaruS0nPI96EuuC/Bj
     procedure_id : 1585064655800919610
     type_code : 14
     code : |1+
      procédure RegroupeElts(sTriForce_p est une chaîne = "") <UI>:vide
      
      sTriAppl est une chaîne
      
      si sTriForce_p = "" ALORS
      	sTriAppl = TBL_Infos.COL_Groupement..Nom + tab + TBL_Infos.COL_Id..Nom
      sinon
      	sTriAppl = sTriForce_p
      FIN
      
      SI TableOccurrence(TBL_Infos) > 0 ALORS
      	TableTrie(TBL_Infos, sTriAppl)
      	
      	POUR nInd = 1 _À_ TableOccurrence(TBL_Infos)
      		TBL_Infos[TableIndiceRupture(RUPT_HautRupture1,nInd)].LIB_Section = TBL_Infos[nInd].COL_Groupement
      	FIN
      	TableSelectPlus(TBL_Infos,1)
      FIN
     type : 458752
