            type_code : 14
            code : |1+
             procédure AjouteLigneSection(nId_p est un entier, sInfo_p est une chaîne, sGrp_p est une chaîne, sBulle_p est une chaîne) <UI>:entier
             
             nRetour est un entier
             nRetour = TableAjouteLigne(TBL_Infos)
             
             TBL_Infos[nRetour].COL_Id			= nId_p
             TBL_Infos[nRetour].COL_Infos		= sInfo_p
             TBL_Infos[nRetour].COL_Infos..Bulle	= sBulle_p
             TBL_Infos[nRetour].COL_Groupement	= sGrp_p
             
             
             renvoyer nRetour
             
            type : 458752
          -
            name : RegroupeElts
            procedure_id : 1585064655800067548
            type_code : 14
            code : |1+
             procédure RegroupeElts() <UI>:vide
             
             
             SI TableOccurrence(TBL_Infos) > 0 ALORS
             	POUR nInd = 1 _À_ TableOccurrence(TBL_Infos)
             		TBL_Infos[TableIndiceRupture(RUPT_HautRupture1,nInd)].LIB_NomProjet = TBL_Infos[nInd].COL_Groupement
             	FIN
             	TableSelectPlus(TBL_Infos,1)
             FIN
             
             si TableOccurrence(TBL_Infos) > 0 ALORS
             	TableTrie(TBL_Infos,TBL_Infos.COL_Groupement..Nom, TBL_Infos.COL_Id..Nom)
             	TableSelectPlus(TBL_Infos, 1)
             FIN
            type : 458752
