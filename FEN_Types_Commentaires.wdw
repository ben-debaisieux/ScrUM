   -
     name : Ajout_Modif_Type
     procedure_id : 2082471277804163862
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Ajout_Modif_Type ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Ajout_Modif_Type()
      
      Si COL_ID = 0 alors //C'est un ajout
      	
      	HRAZ(Scr_Retro_Commentaire_Type)
      	
      	Scr_Retro_Commentaire_Type.Type_Cmtr = COL_Type
      	Scr_Retro_Commentaire_Type.Top_Par_Defaut = COL_Par_Defaut
      	
      	si pas Hajoute(Scr_Retro_Commentaire) alors
      		erreur(erreurinfo(errComplet))
      	FIN
      	
      sinon //On modifie
      	
      	HLitRecherche(Scr_Retro_Commentaire_Type,IDScr_Retro_Commentaire_Type,COL_ID,hIdentique+hLimiteParcours)
      	
      	si htrouve(Scr_Retro_Commentaire_Type) ALORS
      		
      		Scr_Retro_Commentaire_Type.Type_Cmtr = COL_Type
      		Scr_Retro_Commentaire_Type.Top_Par_Defaut = COL_Par_Defaut
      		
      		si pas HModifie(Scr_Retro_Commentaire_Type) alors
      			Erreur(ErreurInfo(errComplet))
      		FIN
      		
      	sinon
      		Erreur(ErreurInfo(errComplet))
      	FIN
      	
      FIN
