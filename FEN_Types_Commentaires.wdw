   -
     name : Liste_Types_Commentaires
     procedure_id : 2082469061600654526
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Liste_Types_Commentaires ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Liste_Types_Commentaires()
      
      sdRecherecheTypesCommentaires est une source de Données
      sReq est une chaîne
      
      nLigne est un entier
      
      
      
      sReq = [
      	SELECT
      		Scr_Type_Cmtr.IDScr_Type_Cmtr,
      		Scr_Type_Cmtr.Type_Cmtr,
      		Scr_Type_Cmtr.Top_Par_Defaut
      	
      	FROM Scr_Type_Cmtr
      		
      ]
      
      SI goApp.Rsql(sdRecherecheTypesCommentaires,sReq) ALORS
      	TableSupprimeTout(TBL_Types_Commentaires)
      	TBL_Types_Commentaires..AffichageActif = faux
      	pour TOUT sdRecherecheTypesCommentaires 
      		
      		nLigne = TableAjouteLigne(TBL_Types_Commentaires)
      		
      		TBL_Types_Commentaires[nLigne].COL_ID = sdRecherecheTypesCommentaires.IDScr_Type_Cmtr
      		TBL_Types_Commentaires[nLigne].COL_Type = sdRecherecheTypesCommentaires.Type_Cmtr
      		TBL_Types_Commentaires[nLigne].COL_Par_Defaut = sdRecherecheTypesCommentaires.Top_Par_Defaut
      		
      	FIN
      	TBL_Types_Commentaires..AffichageActif = vrai
      FIN
      
      HAnnuleDéclaration(sdRecherecheTypesCommentaires)
     type : 458752
   -
     name : Ajout_Modif_Type
     procedure_id : 2082471277804163862
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Ajout_Modif_Type ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE Ajout_Modif_Type()
      
      Si COL_ID = 0 alors //C'est un ajout
      	
      	HRAZ(Scr_Type_Cmtr)
      	
      	Scr_Type_Cmtr.Type_Cmtr = COL_Type
      	Scr_Type_Cmtr.Top_Par_Defaut = COL_Par_Defaut
      	
      	si pas Hajoute(Scr_Type_Cmtr) alors
      		erreur(erreurinfo(errComplet))
      	FIN
      	
      sinon //On modifie
      	
      	HLitRecherche(Scr_Type_Cmtr,IDScr_Type_Cmtr,COL_ID,hIdentique+hLimiteParcours)
      	
      	si htrouve(Scr_Type_Cmtr) ALORS
      		
      		Scr_Type_Cmtr.Type_Cmtr = COL_Type
      		Scr_Type_Cmtr.Top_Par_Defaut = COL_Par_Defaut
      		
      		si pas HModifie(Scr_Type_Cmtr) alors
      			Erreur(ErreurInfo(errComplet))
      		FIN
      		
      	sinon
      		Erreur(ErreurInfo(errComplet))
      	FIN
      	
      FIN
     type : 458752
