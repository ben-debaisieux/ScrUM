      		TableauAjoute(aTabCol3, sdSource_ComposantTypes_Col3.Description)
      		TableauAjoute(aListeID3, sdSource_ComposantTypes_Col3.IDScr_CritereAccept_Composant)
      	FIN
      FIN
      
      
      
      
      POUR i = 1 _a_ TableauOccurrence(aTabCol1)
      	// on ajoute une ligne dans la table autant de fois qu'il y a d'occurences avec les deux tableau contenant la liste des etant donné et de leurs ID
      	TableAjouteLigne(TBL_EtantDonne, aTabCol1[i], aListeID[i])				
      FIN
      
      // on evite habilement une erreur d'index 
      SI TableauOccurrence(aTabCol2) > 0 ALORS
      	TableAjouteLigne(TBL_AumomentDe, aTabCol2[1], aListeID2[1])
      FIN
      
      POUR nIndex = 1 _a_ TableauOccurrence(aTabCol3)
      	// on ajoute une ligne dans la table autant de fois qu'il y a d'occurences avec les deux tableau contenant la liste des jattend que et de leurs ID
      	TableAjouteLigne(TBL_JattendQue, aTabCol3[nIndex], aListeID3[nIndex])
      FIN
      
      HAnnuleDéclaration(sdSource_ComposantTypes)
      HAnnuleDéclaration(sdSource_ComposantTypes_Col2)
      HAnnuleDéclaration(sdSource_ComposantTypes_Col3)
