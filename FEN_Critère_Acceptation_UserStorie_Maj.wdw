    identifier : 0x2e296b69069663e3
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         bCritre_Maj_Ok est un booleen = faux
         // Récupère la date système
         dDate est une Date = DateSys()
         
         //Contrôle des valeurs
         
         SI CBO_Rédacteur = 0 ALORS
         	Info("Champ Rédacteur Vide")
         	RepriseSaisie(CBO_Rédacteur)
         SINON SI SAI_TitreUS = "" ALORS
         	Info("Titre vide")
         	RepriseSaisie(SAI_TitreUS)
         FIN
         // controle des valeurs
         //SI (TBL_AumomentDe[1].COL_Moment <> "" ET TBL_EtantDonne[1].COL_EtantDonneQue <> "" ET TBL_JattendQue[1].COL_EtantDonneQue <> "") OU (TBL_AumomentDe[1].COL_Moment = "" ET TBL_EtantDonne[1].COL_EtantDonneQue = "" ET TBL_JattendQue[1].COL_EtantDonneQue = "") ALORS
         
         //sdGetIDus est une source de données
         //sReqIDus est une chaîne = ChaîneConstruit([
         //select Scr_UserStory.IDScr_UserStory from Scr_UserStory where Scr_UserStory.CodeUserStory = '%1'
         //
         //], zoUs.m_sCodeUserStory)
         
         //goApp.Rsql(sdGetIDus, sReqIDus)
         //si ID_Critere_Acceptation = 0 alors// HRAZ(Scr_CritereAccept)
         
         EcranVersSource(mafenetre,"zoCritere")
         
         zoCritere.p_oUsrRedacteur		= ClUsr.GetObj(CBO_Rédacteur)
         zoCritere.p_oUsrValideur		= ClUsr.GetObj(CBO_Valideur)
         zoCritere.p_oUseDesactivateur	= ClUsr.GetObj(CBO_Desactiveur)
         
         
         
         oCritCompo_Tmp est un ClCritereAccept_Composant dynamique
         nInd est un entier
         POUR nInd = 1 _A_ TableOccurrence(TBL_EtantDonne)
         	//ajout des composants
         	oCritCompo_Tmp <- ClCritereAccept_Composant.GetObj(TBL_EtantDonne[nInd].COL_IDComposant)
         	si TBL_EtantDonne[nInd].COL_EtantDonneQue <> "" ALORS
         		//Le composant est il nouveau ?
         		si oCritCompo_Tmp.bModeAjout() ALORS
         			//Ajout du composant dans le critère
         			zoCritere.AjouteComposant(ClCritereAccept_Composant.cst_EtantDonneQue, TBL_EtantDonne[nInd].COL_EtantDonneQue)
         		sinon
         			//Le composant existe et est référencé par le critère : on le met à jour
         			oCritCompo_Tmp.m_sDescription	= TBL_EtantDonne[nInd].COL_EtantDonneQue
         			oCritCompo_Tmp.m_nTypeCompo		= ClCritereAccept_Composant.cst_EtantDonneQue
         		FIN
         	sinon
         		//Le composant est vidé : on le supprime.
         		oCritCompo_Tmp.Supprimer()
         	FIN
         FIN
         
         oCritCompo_Tmp <- ClCritereAccept_Composant.GetObj(TBL_AumomentDe[1].COL_ID_Moment)
         si TBL_AumomentDe[1].COL_Moment <> "" ALORS
         	//Le composant est il nouveau ?
         	SI oCritCompo_Tmp.bModeAjout() ALORS
         		//Ajout du composant dans le critère
         		zoCritere.AjouteComposant(ClCritereAccept_Composant.cst_AuMomentDe, TBL_AumomentDe[1].COL_Moment)
         	SINON
         		oCritCompo_Tmp.m_sDescription	= TBL_AumomentDe[1].COL_Moment
         		oCritCompo_Tmp.m_nTypeCompo		= ClCritereAccept_Composant.cst_AuMomentDe
         	fin
         sinon
         	//Le composant est vidé : on le supprime.
         	oCritCompo_Tmp.Supprimer()
         FIN
         
         POUR nInd = 1  _A_ TableOccurrence(TBL_JattendQue)
         	//ajout des composants
         	oCritCompo_Tmp <- ClCritereAccept_Composant.GetObj(TBL_JattendQue[nInd].COL_ID_Composant)
         	si TBL_JattendQue[nInd].COL_EtantDonneQue <> "" ALORS
         		//Le composant est il nouveau ?
         		SI oCritCompo_Tmp.bModeAjout() ALORS
         			//Ajout du composant dans le critère
         			zoCritere.AjouteComposant(ClCritereAccept_Composant.cst_JattendQue, TBL_JattendQue[nInd].COL_EtantDonneQue)
         		SINON
         			oCritCompo_Tmp.m_sDescription	= TBL_JattendQue[nInd].COL_EtantDonneQue
         			oCritCompo_Tmp.m_nTypeCompo		= ClCritereAccept_Composant.cst_JattendQue
         		fin
         	sinon
         		//Le composant est vidé : on le supprime.
         		oCritCompo_Tmp.Supprimer()
         	FIN
         FIN
         	
         
         quand Exception dans
         	zoCritere.Enregistre()
         faire
         	erreur(ExceptionInfo())
         sinon
         	FEN_Critère_Acceptation_UserStorie_Maj..ValeurRenvoyée = Vrai 
         	ToastAffiche("Le critère d'acceptation a été enregistré.")
         	Ferme()
         fin
         	
