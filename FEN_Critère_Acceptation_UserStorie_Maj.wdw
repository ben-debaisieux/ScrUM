      // recuperation des parametre nécéssaire a la fonction
      PROCEDURE FEN_Critère_Acceptation_UserStorie_Maj(local ID_Critere_Acceptation est un entier, local nID_UserStory_p est un entier = 0)
      
      mafenetre..ValeurRenvoyée = Faux
      
      
      //zoUs est un ClUserStory <- ClUserStory.GetObj(nID_UserStory_p)
      zoCritere est un ClCritereAccept <- ClCritereAccept.GetObj(ID_Critere_Acceptation)
      
      
      si nID_UserStory_p = 0 et ID_Critere_Acceptation = 0 ALORS
      	ExceptionDéclenche(1,"Une user story est obligatoire pour créer un critère d'acceptation.")
      sinon
      	si ID_Critere_Acceptation = 0 ALORS
      		zoCritere.p_oUserStoryParente = ClUserStory.GetObj(nID_UserStory_p)
      	FIN
      FIN
      
      
      clusr.Combo_Charge_Usr(MaFenêtre..nom, CBO_Rédacteur..nom,ClUsr.usr_tous)
      
      ClUsr.Combo_Charge_Usr(MaFenêtre..Nom, CBO_Valideur..Nom,ClUsr.usr_tous)
      ListeInsère(CBO_Valideur,"<Aucun>" + glien(0))
      
      clusr.Combo_Charge_Usr(MaFenêtre..nom, CBO_Desactiveur..nom,ClUsr.usr_tous)
      ListeInsère(CBO_Desactiveur,"<Aucun>" + gLien(0))
      
      ClCritereAccept.Combo_Charge_Etats(MaFenêtre..Nom,CBO_Etat..nom)
   -
     code : |1+
      // cache champs inutiles
      si goApp.m_oUsrCnx.p_bTop_Equip_Informatique = Faux alors
      	SAI_CommentaireDes..EnSaisie	= faux
      	SAI_CommDev..EnSaisie			= faux
      	SAI_CritAssoc..Visible			= faux
      FIN
      
      // on cherche l'etat en fonction du l'id critere accept, s'il est a 1 ou 2 alors le commentaire désactivation est inactif
      SI pas zoCritere.p_nEtat DANS(ClCritereAccept.cst_Critere_Abandonné, ClCritereAccept.cst_Critere_Doublon) ALORS
      	SAI_CommentaireDes..EnSaisie	= Faux
      	CBO_Desactiveur..EnSaisie		= Faux
      	SAI_DateDes..EnSaisie			= Faux
      FIN
      
      
      SI zoCritere.bModeAjout() ALORS
      	zoCritere.p_oUsrRedacteur	= goApp.m_oUsrCnx
      	zoCritere.m_dDateCreation	= DateSys()
      FIN
      
      
      //--------------------------------------------------------------------------
      
      
      SourceVersEcran(MaFenêtre,"zoCritere")
      
      SAI_NumCritere		= zoCritere.Get_IdUnique()
      
      CBO_Rédacteur		= zoCritere.p_oUsrRedacteur.Get_IdUnique()
      SI zoCritere.p_oUsrValideur <> Null ALORS
      	CBO_Valideur	= zoCritere.p_oUsrValideur.Get_IdUnique()
      FIN
      SI zoCritere.p_oUseDesactivateur <> Null ALORS
      	CBO_Desactiveur	= zoCritere.p_oUseDesactivateur.Get_IdUnique()
      FIN
      
      
      SAI_Titre			= zoCritere.p_oUserStoryParente.sNommination_US()
      
      
      
      oCompoCrit_tmp est un ClCritereAccept_Composant dynamique
      
      POUR tout element oCompoCrit_tmp de zoCritere.Get_aoEtantDonneQue()
      	TableAjouteLigne(TBL_EtantDonne, oCompoCrit_tmp.m_sDescription, oCompoCrit_tmp.Get_IdUnique())				
      FIN
      TableAjouteLigne(TBL_EtantDonne, "", "")
      
      
      POUR tout element oCompoCrit_tmp de zoCritere.Get_aoAuMomentDe()
      	TableAjouteLigne(TBL_AumomentDe, oCompoCrit_tmp.m_sDescription, oCompoCrit_tmp.Get_IdUnique())				
      FIN
      TableAjouteLigne(TBL_AumomentDe, "", "")
      
      
      POUR TOUT ELEMENT oCompoCrit_tmp DE zoCritere.Get_aoJAttendsQue()
      	TableAjouteLigne(TBL_JattendQue, oCompoCrit_tmp.m_sDescription, oCompoCrit_tmp.Get_IdUnique())				
      FIN
      TableAjouteLigne(TBL_JattendQue, "", "")
