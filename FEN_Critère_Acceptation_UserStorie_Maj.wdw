      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE RefreshTable()
      
      sdSource_ComposantTypes est une source de données
      
      // requetes pour charger les composants
      sRequestComposant est une chaine = ChaîneConstruit([
      	
      	SELECT
      		Scr_CritereAccept_Composant.Description,
      		
      		Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant_Type,
      		Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant
      	FROM Scr_CritereAccept_Composant
      		
      	WHERE Scr_CritereAccept_Composant.IDScr_CritereAccept = '%1' 
      	and Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant_Type = '%2'
      	
      	order by 
      	
      	Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant
      	
      	
      ], ID_Critere_Acceptation, cstComposant_Crit_Accept_EtantDonneQue)
      //-------------------------------------------------------------------------------------
      
      sdSource_ComposantTypes_Col2 est une source de données
      
      sRequestComposant_Col2 est une chaine = ChaîneConstruit([
      	
      	SELECT
      		Scr_CritereAccept_Composant.Description, 
      		Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant_Type,
      		Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant
      		
      	FROM Scr_CritereAccept_Composant
      	
      	WHERE Scr_CritereAccept_Composant.IDScr_CritereAccept = '%1' 
      	and Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant_Type = '%2'
      	
      	order by 
      	
      	Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant
      	
      	
      ], ID_Critere_Acceptation, cstComposant_Crit_Accept_AuMomentDe)
      //-----------------------------------------------------------------------------------------------------------------	
      
      sdSource_ComposantTypes_Col3 est une source de données
      
      sRequestComposant_Col3 est une chaine = ChaîneConstruit([
      	
      	SELECT
      		Scr_CritereAccept_Composant.Description,
      		Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant,
      		Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant_Type
      		
      	FROM Scr_CritereAccept_Composant
      	
      	WHERE Scr_CritereAccept_Composant.IDScr_CritereAccept = '%1' 
      	and Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant_Type = '%2'
      	
      	order by 
      	
      	Scr_CritereAccept_Composant.IDScr_CritereAccept_Composant
      	
      	
      ], ID_Critere_Acceptation, cstComposant_Crit_Accept_JAttendQue)
      
      //-----------------------------------------------------------------------------------------------------------------
      
      // declaration tableau, ces tableau vont contenir une liste de Etant donné, au moment, j'attend que
      aTabCol1 est un tableau de chaînes
      aTabCol2 est un tableau de chaînes
      aTabCol3 est un tableau de chaînes
      // declaration tableau, ces tableau vont contenir une liste d'ID composant
      aListeID est un tableau d'entier
      aListeID2 est un tableau d'entier
      aListeID3 est un tableau d'entier
      
      
      //ça affiche les CritereComposants dans le tableau
      SI goApp.Rsql(sdSource_ComposantTypes, sRequestComposant) = Vrai ALORS
      	POUR TOUT sdSource_ComposantTypes
      		// on remplit le tableau avec les étant donné que
      		TableauAjoute(aTabCol1, sdSource_ComposantTypes.Description)
      		// on remplit les ID etant donné
      		TableauAjoute(aListeID, sdSource_ComposantTypes.IDScr_CritereAccept_Composant)
      	FIN
      FIN		
      
      SI goApp.Rsql(sdSource_ComposantTypes_Col2, sRequestComposant_Col2)= Vrai ALORS
      	POUR TOUT sdSource_ComposantTypes_Col2
      		// pareil
      		TableauAjoute(aTabCol2, sdSource_ComposantTypes_Col2.Description)
      		TableauAjoute(aListeID2, sdSource_ComposantTypes_Col2.IDScr_CritereAccept_Composant)
      		
      	FIN
      FIN
      
      SI goApp.Rsql(sdSource_ComposantTypes_Col3, sRequestComposant_Col3)= Vrai ALORS
      	POUR TOUT sdSource_ComposantTypes_Col3 
      		//pareil
      		TableauAjoute(aTabCol3, sdSource_ComposantTypes_Col3.Description)
      		TableauAjoute(aListeID3, sdSource_ComposantTypes_Col3.IDScr_CritereAccept_Composant)
      	FIN
      FIN
      
      
      
      
      POUR i = 1 _a_ TableauOccurrence(aTabCol1)
      	// on ajoute une ligne dans la table autant de fois qu'il y a d'occurences avec les deux tableau contenant la liste des etant donné et de leurs ID
      	TableAjouteLigne(TBL_EtantDonne, aTabCol1[i], aListeID[i])				
      FIN
      
      // on evite habilement une erreur d'index 
      SI TableauOccurrence(aTabCol2) > 0 ALORS
      	TableAjouteLigne(TBL_AumomentDe, aTabCol2[TableauOccurrence(aTabCol2)], aListeID2[TableauOccurrence(aTabCol2)])
      FIN
      
      POUR nIndex = 1 _a_ TableauOccurrence(aTabCol3)
      	// on ajoute une ligne dans la table autant de fois qu'il y a d'occurences avec les deux tableau contenant la liste des jattend que et de leurs ID
      	TableAjouteLigne(TBL_JattendQue, aTabCol3[nIndex], aListeID3[nIndex])
      FIN
      
      HAnnuleDéclaration(sdSource_ComposantTypes)
      HAnnuleDéclaration(sdSource_ComposantTypes_Col2)
      HAnnuleDéclaration(sdSource_ComposantTypes_Col3)
