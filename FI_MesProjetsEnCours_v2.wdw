           code : |1+
            aOProcess est un tableau de ClSchem dynamique
            oProcess_tmp est un ClSchem dynamique
            
            SI TableOccurrence(TBL_MesProjets) >= 1 ALORS
            	//Récupération des process du projet
            	oPrj est un ClDbProjet <- ClDbProjet.GetObj(TBL_MesProjets.COL_IdProjet)
            	aOProcess = ClColProcess_Metier.aoProcessMetiers(oPrj)
            	
            	//en a t on au moins un ?
            	si Dimension(aOProcess) = 0 ALORS
            		OPT_Process..Visible = faux
            	sinon
            		OPT_Process..Visible = vrai
            		
            		//Suppression de tous les sous items
            		TANTQUE EnumèreMenu(OPT_Process,1) <> ""
            			MenuSupprime(EnumèreMenu(OPT_Process,1))
            		FIN
            		
            		//Ajout d'un item par process du projet
            		pour tout element oProcess_tmp de aOProcess
            			MenuAjouteOption(OPT_Process, ...
            				OPT_Process..nom + "_" + oProcess_tmp.ZF_Get_IdUnique(), ...
            				oProcess_tmp.m_sLibelle, ...
            				AfficheProcess, ...
            				oProcess_tmp.ZF_Get_IdUnique())
            		FIN
            	FIN
            	
            	si goApp.m_oUsrCnx.p_bTop_Equip_Informatique ALORS
            		OPT_Vision_d_ensemble..Visible = Vrai
            	sinon
            		OPT_Vision_d_ensemble..Visible = faux
            	FIN
            fin
            
