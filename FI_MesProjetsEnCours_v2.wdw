      //AnalyseurDébut()
      
      //On liste tous les états qu'on veut afficher, dans le bon ordre
      TableauAjoute(aEtats, ClDbUserStory.ECompteNbUS.A_faire)
      TableauAjoute(aEtats, ClDbUserStory.ECompteNbUS.EnCours)
      TableauAjoute(aEtats, ClDbUserStory.ECompteNbUS.En_test_fonctionnel)
      TableauAjoute(aEtats, ClDbUserStory.ECompteNbUS.Terminee)
      TableauAjoute(aEtats, ClDbUserStory.ECompteNbUS.Annulée)
      
      TableSupprimeTout(TBL_MesProjets)
      
      TBL_MesProjets..AffichageActif	= Faux
      
      
      
      
      SI goApp.m_oUsrCnx.p_bTop_Equip_Informatique ALORS
      	oProjets <- ClColProjet.aoListeProjets(Faux)
      SINON
      	oProjets <- ClColProjet.aoListeProjetsDeUsr(goApp.m_oUsrCnx)
      FIN
      
      //Pré chargement des situations
      aSituationsParProjet est un tableau associatif de ClZF_ObjDbLs dynamique <- ClColProjet_Situation.aoFromSprintEtColPrj(oSprint, oProjets).ZF_VersTableauAssociatif("p_oIDScr_Projet_ID")
      
      //Mise en cache des US à traiter par la récupération des taches
      oColUS_Tmp est un ClColUserStory dynamique
      oColUS_Res est un ClColUserStory ()
      
      pour tout oColUS_Tmp de ClColUserStory.oColsFromProjets(oProjets)
      	//Cumul de toutes les US dans une seule collection
      	oColUS_Res.ZF_Join(oColUS_Tmp)
      FIN
      ClColTache.oColsFromUs(oColUS_Res)
      
      
      POUR TOUT ELEMENT oPrjTmp DE oProjets.aItems()
      	
      	bAffiche = Vrai
      	
      	//Calcul de l'avancement du développement des taches.
      	SI oPrjTmp.Get_StatUS(ClDbUserStory.ECompteNbUS.Toutes) <> 0 ALORS
      		rPctDev = (...
      			oPrjTmp.Get_StatUS(ClDbUserStory.ECompteNbUS.En_test_fonctionnel) + ...
      			oPrjTmp.Get_StatUS(ClDbUserStory.ECompteNbUS.Terminee) ...
      			) / (oPrjTmp.Get_StatUS(ClDbUserStory.ECompteNbUS.Toutes) + ...
      			oPrjTmp.Get_StatUS(ClDbUserStory.ECompteNbUS.Annulée))
      			
      	SINON
      		bAffiche = Faux
      	FIN
      	
      	//Calcul de l'avancement des tests, des taches réalisées.
      	SI oPrjTmp.Get_StatUS(ClDbUserStory.ECompteNbUS.En_test_fonctionnel) + ...
      			oPrjTmp.Get_StatUS(ClDbUserStory.ECompteNbUS.Terminee) <> 0 ALORS
      		
      			
      		rPctTest = (...
      			oPrjTmp.Get_StatUS(ClDbUserStory.ECompteNbUS.Terminee) ...
      			) / (oPrjTmp.Get_StatUS(ClDbUserStory.ECompteNbUS.En_test_fonctionnel) + ...
      			oPrjTmp.Get_StatUS(ClDbUserStory.ECompteNbUS.Terminee))
      	SINON
      		rPctTest = 0
      	FIN
      	
      	SI bAffiche ALORS
      		sBulle = ""
      		//Récupération de la situation associée au projet (s'il y en a une)
      		oSituation <- aSituationsParProjet[oPrjTmp.ZF_Get_IdUnique()]
      		
      		POUR TOUT ELEMENT eEtats, nPos DE aEtats
      			sBulle += [RC] + ClDbUserStory.sNomDeEtat(eEtats) + " : " + oPrjTmp.Get_StatUS(eEtats)
      		FIN
      		
      		//Si le projet a un commentaire de situation et que l'utilisateur est un dev...
      		SI goApp.m_oUsrCnx.p_bTop_Equip_Informatique _et_ oSituation <> Null _ET_ oSituation.p_sCMTR <> "" ALORS
      			//Affichage du commentaire dans la bulle
      			sBulle += [RC] + "--------------------------------" + RC + oSituation.p_sCMTR
      		FIN
      		
      		SI oPrjTmp.p_bTop_Direction ALORS
      			sType	= "Direction"
      		SINON
      			sType	= "Projet"
      		FIN
      		
      		nNumLigne = TableAjouteLigne(TBL_MesProjets)
      		TBL_MesProjets[nNumLigne].COL_IdProjet			= oPrjTmp.ZF_Get_IdUnique()
      		TBL_MesProjets[nNumLigne].COL_Projet			= oPrjTmp.p_sNom
      		
      		
      		SI oSituation <> null _et_ oSituation.p_bTop_Prioritaire ALORS
      			//On met un picto particulier sur les projets prioritaires
      			TBL_MesProjets[nNumLigne].COL_Projet		+= "  " + gImage(IMG_SansNom1)
      		FIN
      		
      		TBL_MesProjets[nNumLigne].COL_PctDev			= rPctDev
      		TBL_MesProjets[nNumLigne].COL_PctTest			= rPctTest
      		TBL_MesProjets[nNumLigne].COL_Type				= sType
      		
      		TBL_MesProjets[nNumLigne].COL_Projet..Bulle		= sBulle
      		TBL_MesProjets[nNumLigne].COL_PctDev..Bulle		= sBulle
      		TBL_MesProjets[nNumLigne].COL_PctTest..Bulle	= sBulle
      	FIN
      FIN
      
      TableTrie(TBL_MesProjets, "-" + TBL_MesProjets.COL_Type..Nom, "-" + TBL_MesProjets.COL_PctDev..Nom,"-" + TBL_MesProjets.COL_PctTest..Nom, TBL_MesProjets.COL_Projet..Nom)
      
      
      
      nInd est un entier
      POUR nInd = 1 _À_ TableOccurrence(TBL_MesProjets)
      	TBL_MesProjets[TableIndiceRupture(RUPT_HautRupture1,nInd)].LIB_NomProjet = TBL_MesProjets[nInd].COL_Type
      FIN
      
      SI TBL_MesProjets > 0 ALORS
      	TableSelectPlus(TBL_MesProjets,1)
      FIN
      
      TBL_MesProjets..AffichageActif	= Vrai
      
      
      //analyseurfin()
