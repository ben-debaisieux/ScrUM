     -
       name : OPT_Copier_le_plan_de_test_vierge
       identifier : 0x251e7c8e237f3edc
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1+
            SI TableOccurrence(TBL_Infos) >= 1 ALORS
            	oPrj			est un ClDbProjet	<- ClDbProjet.GetObj(TBL_Infos.COL_Id)
            	oUS				est un ClDbUserStory dynamique
            	oTache			est un ClDbTache dynamique
            	oEpic			est un ClDbEpic dynamique
            	aOLstEpics		est un tableau de ClDbEpic dynamique
            	sCommentaire	est une chaîne
            	sRetour			est une chaîne
            	
            	
            	aOLstEpics <- ClColEpic.oFromprojet(oPrj).aItems()
            	
            //	sRetour += oPrj.p_sNom
            //	AddRC(1)
            	
            	POUR TOUT ÉLÉMENT oEpic DE aOLstEpics
            		
            		AddRC(2)
            		SI oEpic = Null ALORS
            			sRetour += "(Sans groupement)"
            		SINON
            			sRetour += oEpic.p_sNom
            		FIN
            		sRetour += repete("#",100)
            		
            		//Liste de toutes les user stories
            		POUR TOUT ÉLÉMENT oUS DE ClColUserStory.oFromProjet(oPrj).aItems()
            			
            			
            			//Si on est sur la bonne epic ou si on est 'sans epic'
            			SI oUS.p_oIDScr_Epic = oEpic OU (oUS.p_oIDScr_Epic = Null ET oEpic = Null) ALORS
            				
            				
            				AddRC()
            				
            				
            				SI oUS.p_sCMTR_Global <> "" ALORS
            					sCommentaire += [RC] + [
            						Commentaire global :
            						[%oUS.p_sCMTR_Global%]
            						]
            				FIN
            				SI oUS.p_sCMTR_RetourDeTest <> "" ALORS
            					sCommentaire += [RC] + [
            						Retour de test :
            						[%oUS.p_sCMTR_RetourDeTest%]
            						]
            				FIN
            				SI oUS.p_sCMTR_Mise_En_Prod <> "" ALORS
            					sCommentaire += [RC] + [
            						Infos de mise en prod :
            						[%oUS.p_sCMTR_Mise_En_Prod%]
            						]
            				FIN
            				
            				sRetour += TAB + sAddInfo(oUS.sNommination_US())
            				si sCommentaire <> "" alors
            					sRetour += " - " + sAddInfo(sCommentaire)
            				FIN
            				sRetour += tab + TAB + TAB + oUS.p_sCodeUserStory
            				
            				si oUS.p_oIDScr_Usr_Valideur <> Null ALORS
            					sRetour += TAB + oUS.p_oIDScr_Usr_Valideur.p_sNom
            				FIN
            				
            				//Parcours de toutes le taches de l'US
            				POUR TOUT ÉLÉMENT oTache DE ClColTache.oFromUserStory(oUS).aItems()
            					
            					AddRC()
            					sRetour += TAB + TAB + sAddInfo(oTache.p_sDescription)
            					si oTache.p_sCMTR <> "" alors
            						sRetour += sAddInfo(" - " + oTache.p_sCMTR)
            					fin
            					sRetour += TAB + TAB + oTache.p_sCodeTache
            					
            					
            					sEtat est une chaine
            					selon oTache.p_nStatut
            //						CAS ClDbTache.cstStatut_a_coter, ClDbTache.cstStatut_a_faire
            //							sEtat = "A faire"
            //						CAS ClDbTache.cstStatut_en_cours
            //							sEtat = "En cours de dev"
            						CAS ClDbTache.cstStatut_realise
            							sEtat = "(Déjà réalisé)"
            						CAS ClDbTache.cstStatut_Annulee
            							sEtat = "(tache annulée)"
            //						CAS ClDbTache.cstStatut_Suspendu
            //							sEtat = "Besoin à redéfinir"
            						autres cas
            							//sEtat = "Etat tache : " + oTache.sLibDeStatut()
            					fin
            					sRetour += TAB + TAB + TAB + TAB + sEtat
            					
            				FIN
            				AddRC(5)
            			fin
            		FIN
            	FIN
            
            	procedure interne sAddInfo(local sInfo est une chaîne = ""):chaine
            		renvoyer remplace(sInfo, [RC, Caract(10), Caract(13)], " ")
            	FIN
            	
            	procedure interne AddRC(local nNbRc est un entier = 1):vide
            		sRetour += repete(rc, nNbRc)
            	FIN
            	
            	VersPressePapier(sRetour)
            	ToastAffiche("Plan copié dans le presse papier")
            FIN
            
           type : 32
     -
       name : OPT_Export_projet
       identifier : 0x25cc90e780325f16
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1-
            oPrj			est un ClDbProjet <- ClDbProjet.GetObj(TBL_Infos.COL_Id)
            oEpic			est un ClDbEpic dynamique
            oVers			est un ClDbVersion dynamique
            oColVers		est un ClcolVersion dynamique <- ClColVersion.oFromProjet(oPrj)
            oUs				est un ClDbUserStory dynamique
            aListeReleases	est un tableau de variants
            aListeUS		est un tableau de variants
            aListeEpics		est un tableau de variants
            aListeTheme		est un tableau de variants
            
            soit vExport = null
            vExport.nomProjet = oPrj.p_sNom
            
            
            si oColVers.ZF_nTaille() = 0 alors
            	//Si on a pas de versions, on découpe, par jalon (ancien fonctionnement)
            	POUR TOUT oUs DE ClColUserStory.oFromProjet(oPrj).oFiltreSansEpic().aItems()
            		TableauAjoute(aListeUS, ChargeUS(oUs))
            	FIN
            	vExport.usAAffecter = aListeUS
            	
            	POUR TOUT oEpic DE ClColEpic.oFromprojet(oPrj).aItems()
            		soit vRelease = null
            		
            		TableauSupprimeTout(aListeUS)
            		
            		vRelease.nomRelease = oEpic.p_sNom
            		
            		POUR TOUT oUs DE ClColUserStory.oFrom_Epic(oEpic).aItems()
            			TableauAjoute(aListeUS, ChargeUS(oUs))
            		FIN
            		
            		vRelease.UserStories					= aListeUS
            		TableauAjoute(aListeReleases, vRelease)
            	FIN
            sinon
            	
            	pour tout oVers de oColVers.aItems()
            		soit vRelease = null
            		vRelease.nomRelease = oVers.p_sNom
            		TableauAjoute(aListeReleases, vRelease)
            	FIN
            	
            	soit vTheme = Null
            	POUR TOUT oEpic DE ClColEpic.oFromProjet(oPrj).aItems()
            		nPosSeparateur est un entier
            		nPosSeparateur = Position(oEpic.p_sNom," > ")
            		
            		si nPosSeparateur <= 0 ALORS
            			//C'est un thème
            			
            			si pas vTheme = null alors
            				TableauSupprimeTout(aListeEpics)
            				tableauajoute(aListeTheme, vTheme)
            			FIN
            			
            			vTheme = null
            			
            			vTheme.nomTheme = oEpic.p_sNom
            			
            		sinon
            			//C'est un épic
            			soit vEpic = null
            			vEpic.nomEpic = oEpic.p_sNom[[nPosSeparateur + 3 a ]]
            			
            			TableauSupprimeTout(aListeUS)
            			
            			POUR TOUT oUs DE ClColUserStory.oFrom_Epic(oEpic).aItems()
            				TableauAjoute(aListeUS, ChargeUS(oUs))
            			FIN
            			
            			si aListeUS..Occurrence > 0 ALORS
            				vEpic.userStories	= aListeUS
            			FIN
            			
            			TableauAjoute(aListeEpics,vEpic)
            		FIN
            		
            		vTheme.epics = aListeEpics
            		
            	FIN
            	TableauAjoute(aListeTheme, vTheme)
            FIN
            
            
            
            vExport.releases = aListeReleases
            vExport.themes = aListeTheme
            VersPressePapier(VariantVersJSON(vExport,psdMiseEnForme))
            ToastAffiche("Projet copié")
            
            
            
            	procedure interne ChargeUS(oUs_p est un ClDbUserStory)
            		
            		vUS est un variant
            		vUS.code			= oUs_p.p_sCodeUserStory
            		vUS.commentaire		= oUs_p.p_sCMTR_Global
            		vUS.entantQue		= oUs_p.p_sEntantQue
            		vUS.jaiBesoinDe		= oUs_p.p_sJaiBesoinDe
            		vUS.afinDe			= oUs_p.p_sAfinDe
            		vUS.charge			= oUs_p.stInfoCharge().nChargePrev
            		vUS.businessValue	= oUs_p.p_nValeurBusiness
            		vUS.release			= ClZF_ObjDbLs.ZF_RepresentationSiNonNull(oUs_p.p_oIDScr_Version)
            		renvoyer vUS
            	FIN
           type : 32
