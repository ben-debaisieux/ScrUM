   -
     name : Actualise
     internal_properties : BQAAAAUAAACS4s5FFfzwLRS8Zz9FLTapqSTYJSsXt9WA+rzoQwkFZ0jYcq/532P35oxH5kmgGYKV
     procedure_id : 1205236688785931523
     type_code : 14
     code : |1+
      PROCÉDURE Actualise()
      
      oPrjTmp		est un ClProjet dynamique
      nNumLigne	est un entier
      aEtats		est un tableau de ClUserStory.ECompteNbUS
      eEtats		est un ClUserStory.ECompteNbUS
      nPos		est un entier
      rPctDev		est un réel
      rPctTest	est un réel
      aoProjets	est un tableau de ClProjet dynamique
      sBulle		est une chaîne
      sType		est une chaîne
      bAffiche est un booléen
      
      
      
      
      //On liste tous les états qu'on veut afficher, dans l' bon ordre
      TableauAjoute(aEtats, ClUserStory.ECompteNbUS.A_faire)
      TableauAjoute(aEtats, ClUserStory.ECompteNbUS.EnCours)
      TableauAjoute(aEtats, ClUserStory.ECompteNbUS.En_test_fonctionnel)
      TableauAjoute(aEtats, ClUserStory.ECompteNbUS.Terminee)
      TableauAjoute(aEtats, ClUserStory.ECompteNbUS.Annulée)
      
      TableSupprimeTout(TBL_MesProjets)
      
      TBL_MesProjets..AffichageActif	= Faux
      
      SI goApp.m_oUsrCnx.p_bTop_Equip_Informatique ALORS
      	aoProjets <- ClProjet.aoListeProjets(Faux)
      SINON
      	aoProjets <- goApp.m_oUsrCnx.aoMesProjets(ClUsr.usr_tous)
      FIN
      
      
      POUR TOUT ELEMENT oPrjTmp DE aoProjets
      	
      	bAffiche = Vrai
      	
      	//Calcul de l'avancement du développement des taches.
      	SI oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.Toutes) <> 0 ALORS
      		rPctDev = (...
      			oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.En_test_fonctionnel) + ...
      			oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.Terminee) + ...
      			oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.Annulée) ...
      			) / oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.Toutes)
      			
      	SINON
      		//rPctDev = 0
      		bAffiche = Faux
      	FIN
      	
      	//Calcul de l'avancement des tests, des taches réalisées.
      	SI oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.En_test_fonctionnel) + ...
      			oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.Terminee) <> 0 ALORS
      		
      			
      		rPctTest = (...
      			oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.Terminee) ...
      			) / (oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.En_test_fonctionnel) + ...
      			oPrjTmp.Get_StatUS(ClUserStory.ECompteNbUS.Terminee))
      	SINON
      		rPctTest = 0
      	FIN
      	
      	SI bAffiche ALORS
      		sBulle = ""
      		
      		POUR TOUT ELEMENT eEtats, nPos DE aEtats
      			sBulle += [RC] + ClUserStory.sNomDeEtat(eEtats) + " : " + oPrjTmp.Get_StatUS(eEtats)
      		FIN
      		
      		//Si le projet a un commentaire de situation et que l'utilisateur est un dev...
      		SI oPrjTmp.p_sCmtrsituation <> "" ET goApp.m_oUsrCnx.p_bTop_Equip_Informatique ALORS
      			//Affichage du commentaire dans la bulle
      			sBulle += [RC] + "--------------------------------" + RC + oPrjTmp.p_sCmtrsituation
      		FIN
      		
      		SI oPrjTmp.p_bTop_Direction ALORS
      			sType	= "Direction"
      		SINON
      			sType	= "Projet"
      		FIN
      		
      		nNumLigne = TableAjouteLigne(TBL_MesProjets)
      		TBL_MesProjets[nNumLigne].COL_IdProjet			= oPrjTmp.Get_IdUnique()
      		TBL_MesProjets[nNumLigne].COL_Projet			= oPrjTmp.p_sNom
      		
      		SI oPrjTmp.p_bTop_Prioritaire ALORS
      			//On met un picto particulier sur les projets prioritaires
      			TBL_MesProjets[nNumLigne].COL_Projet		+= "  " + gImage(IMG_SansNom1)
      		FIN
      		
      		TBL_MesProjets[nNumLigne].COL_PctDev			= rPctDev
      		TBL_MesProjets[nNumLigne].COL_PctTest			= rPctTest
      		TBL_MesProjets[nNumLigne].COL_Type				= sType
      		
      		TBL_MesProjets[nNumLigne].COL_Projet..Bulle		= sBulle
      		TBL_MesProjets[nNumLigne].COL_PctDev..Bulle		= sBulle
      		TBL_MesProjets[nNumLigne].COL_PctTest..Bulle	= sBulle
      	FIN
      FIN
      
      TableTrie(TBL_MesProjets, "-" + TBL_MesProjets.COL_Type..Nom, "-" + TBL_MesProjets.COL_PctDev..Nom,"-" + TBL_MesProjets.COL_PctTest..Nom, TBL_MesProjets.COL_Projet..Nom)
      
      
      
      nInd est un entier
      POUR nInd = 1 _À_ TableOccurrence(TBL_MesProjets)
      	TBL_MesProjets[TableIndiceRupture(RUPT_HautRupture1,nInd)].LIB_NomProjet = TBL_MesProjets[nInd].COL_Type
      FIN
      
      SI TBL_MesProjets > 0 ALORS
      	TableSelectPlus(TBL_MesProjets,1)
      FIN
      
      TBL_MesProjets..AffichageActif	= Vrai
     type : 458752
