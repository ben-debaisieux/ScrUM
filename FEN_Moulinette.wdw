               			POUR i = nPremiereligne _À_ Xlsnbligne_
               				
               				SI xlsDonnée(IdFichierXLS,i,nCol_nom) <> "" ALORS
               					
               //					//Recherche de l'ID de l'utilisateur
               //					HLitRecherche(Scr_Usr,Nom,xlsDonnée(IdFichierXLS,i,nCol_nom),hIdentique+hLimiteParcours)
               //					si Htrouve(Scr_Usr)
