               jauge(nInc(),nPosMax)
               ExécuteTraitement(BTN_Maj_etats,trtClic)
               
               jauge(nInc(),nPosMax)
               ExécuteTraitement(BTN_Import_messages,trtClic)
               
               
               jauge()
               
               //Purge de tous les éléments en mémoire
               clobjdb.LibereInstancesMemo()
