     procedure_id : 1462497928446580900
     type_code : 12
     code : |1+
      procedure prive Gen_Proc_Actualise()<metier> : chaine
      
      sNomProc est une chaîne = "Actualise"
      sRetour est une chaîne
      
      sRetour = [
      procédure [%sNomProc%](nIdSel_p est un entier = 0):vide
      
      oTmp est un [%:m_oClasseGen.sNomClasseGen()%] dynamique
      nLigne			est un entier
      nMemoLigneSel	est un entier
      
      [%:m_sNomChampsTbl%]..AffichageActif = faux
      
      TableSupprimeTout([%:m_sNomChampsTbl%])
      
      pour tout ELEMENT oTmp de [%:m_sNomColGlobale%].aItems()
      	nLigne = TableAjouteLigne([%:m_sNomChampsTbl%])
      	
      	si oTmp.ZF_Get_IdUnique() = nIdSel_p ALORS
      		nMemoLigneSel = nLigne
      	FIN
      	
      [%:m_sChargementTbl%]
      FIN
      
      si nMemoLigneSel > 0 ALORS
      	TableSelectPlus([%:m_sNomChampsTbl%],nMemoLigneSel)
      FIN
      
      [%:m_sNomChampsTbl%]..AffichageActif = vrai
      
      ]
      
      renvoyer :GenBloc_ProcedureLoc(sRetour, sNomProc)
      
      
      /*
      // a faire : factoriser la création de procédure locale<§cs:10§>
      renvoyer [<§cs:10§>
         -<§cs:10§>
           name : Actualise<§cs:10§>
           procedure_id : [%:GenProcIdent()%]<§cs:10§>
           type_code : 14<§cs:10§>
           code : |1-<§cs:10§>
            // Résumé : <indiquez ici ce que fait la procédure><§cs:10§>
            // Syntaxe :<§cs:10§>
            // Actualise ()<§cs:10§>
            //<§cs:10§>
            // Paramètres :<§cs:10§>
            //	Aucun<§cs:10§>
            // Valeur de retour :<§cs:10§>
            // 	Aucune<§cs:10§>
            //<§cs:10§>
            // Exemple :<§cs:10§>
            // <Indiquez ici un exemple d'utilisation><§cs:10§>
            //<§cs:10§>
            procédure Actualise(nIdSel_p est un entier = 0):vide<§cs:10§>
            <§cs:10§>
            oTmp est un [%:m_oClasseGen.sNomClasseGen()%] dynamique<§cs:10§>
            nLigne			est un entier<§cs:10§>
            nMemoLigneSel	est un entier<§cs:10§>
            <§cs:10§>
            [%:m_sNomChampsTbl%]..AffichageActif = faux<§cs:10§>
            <§cs:10§>
            TableSupprimeTout([%:m_sNomChampsTbl%])<§cs:10§>
            <§cs:10§>
            pour tout ELEMENT oTmp de [%:m_sNomColGlobale%].aItems()<§cs:10§>
            	nLigne = TableAjouteLigne([%:m_sNomChampsTbl%])<§cs:10§>
            	<§cs:10§>
            	si oTmp.ZF_Get_IdUnique() = nIdSel_p ALORS<§cs:10§>
            		nMemoLigneSel = nLigne<§cs:10§>
            	FIN<§cs:10§>
            	<§cs:10§>
      [%:sIndentSpace(:m_sChargementTbl, 6)%]<§cs:10§>
            FIN<§cs:10§>
            <§cs:10§>
            si nMemoLigneSel > 0 ALORS<§cs:10§>
            	TableSelectPlus([%:m_sNomChampsTbl%],nMemoLigneSel)<§cs:10§>
            FIN<§cs:10§>
            <§cs:10§>
            [%:m_sNomChampsTbl%]..AffichageActif = vrai<§cs:10§>
           type : 458752<§cs:10§>
      ]<§cs:10§>
      */
     type : 458752
   -
     name : Gen_btns_action_Ajout
     procedure_id : 1462972569519617792
     type_code : 12
     code : |1-
      procedure prive Gen_btns_action_Ajout():chaine
      
      
      oTmpFic est un ClZF_GenFenFic(:m_oClasseGen)
      
      
      renvoyer [
      nLigne est un entier
      oDb est un [%:m_oClasseGen.sNomClasseGen()%]
      
      nLigne = Ouvre([%oTmpFic.sNomFen()%], oDb)
      SI nLigne <> 0 ALORS
      [%TAB%][%:m_sNomColGlobale%].ZF_addItem(oDb)
      [%TAB%]Actualise(nLigne)
      FIN
      ]
     type : 458752
   -
     name : Gen_btns_action_Modif
     procedure_id : 1462973278189321096
     type_code : 12
     code : |1+
      procedure prive Gen_btns_action_Modif():chaine
      
      
      oTmpFic est un ClZF_GenFenFic(:m_oClasseGen)
      
      
      RENVOYER [
      nLigne est un entier
      
      SI TableOccurrence([%:m_sNomChampsTbl%]) > 0 ALORS
      [%TAB%]oDb est un [%:m_oClasseGen.sNomClasseGen()%] <- [%:m_oClasseGen.sNomClasseGen()%].GetObj([%:m_sNomChampsTbl%].COL_ID)
      [%TAB%]nLigne = Ouvre([%oTmpFic.sNomFen()%], oDb)
      [%TAB%]SI nLigne <> 0 ALORS
      [%TAB%][%TAB%]Actualise(nLigne)
      [%TAB%]FIN
      FIN
      ]
     type : 458752
   -
     name : Gen_btns_action_Supp
     procedure_id : 1462973334023908600
     type_code : 12
     code : |1+
      procedure prive Gen_btns_action_Supp():chaine
      
      RENVOYER [
      SI TableOccurrence([%:m_sNomChampsTbl%]) > 0 ALORS
      [%TAB%]SI OuiNon(Non, "Confirmez-vous la suppression de l’enregistrement ?") ALORS
      [%TAB%][%TAB%]oDb est un [%:m_oClasseGen.sNomClasseGen()%] <- [%:m_oClasseGen.sNomClasseGen()%].GetObj([%:m_sNomChampsTbl%].COL_ID)
      [%TAB%][%TAB%]QUAND EXCEPTION DANS
      [%TAB%][%TAB%][%TAB%]oDb.ZF_Supprimer()
      [%TAB%][%TAB%][%TAB%]oDb.ZF_Enregistre()
      [%TAB%][%TAB%]FAIRE
      [%TAB%][%TAB%][%TAB%]Erreur(ExceptionInfo(errMessage))
      [%TAB%][%TAB%]SINON
      [%TAB%][%TAB%][%TAB%][%:m_sNomColGlobale%].ZF_Supp(oDb)
      [%TAB%][%TAB%][%TAB%]TableSupprime([%:m_sNomChampsTbl%])
      [%TAB%][%TAB%]FIN
      [%TAB%]FIN
      FIN
      ]
     type : 458752
