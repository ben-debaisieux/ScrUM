        code : |1+
         stPlanner_Projet est un STR_PLANNER_PROJET
         aStBucket est un tableau associatif de STR_PLANNER_Bucket
         
         stPlanner_Projet.sIDPlanner = CBO_Planner..ValeurMémorisée
         
         aStBucket = COL_PLANNER.PLANNER_LISTE_BUCKET(CBO_Planner..ValeurMémorisée,Vrai)
         
         SI aStBucket[cstPlanner_Bucket_A_Faire]..Existe ALORS
         	stPlanner_Projet.sIDBucket_A_Faire = aStBucket[cstPlanner_Bucket_A_Faire].sIDBucket
         FIN
         
         SI aStBucket[cstPlanner_Bucket_A_Livrer]..Existe ALORS
         	stPlanner_Projet.sIDBucket_A_Livrer = aStBucket[cstPlanner_Bucket_A_Livrer].sIDBucket
         FIN
         
         SI aStBucket[cstPlanner_Bucket_A_Tester]..Existe ALORS
         	stPlanner_Projet.sIDBucket_A_Tester = aStBucket[cstPlanner_Bucket_A_Tester].sIDBucket
         FIN
         
         SI aStBucket[cstPlanner_Bucket_En_Cours]..Existe ALORS
         	stPlanner_Projet.sIDBucket_En_Cours = aStBucket[cstPlanner_Bucket_En_Cours].sIDBucket
         FIN
         
         Ferme(FEN_Planner_Synchro,stPlanner_Projet)
         
         
