 controls :
  -
    name : GRF_Projection
    identifier : 0x2fbe70b91263fb1c
    internal_properties : 
    type : 33
    code_elements :
     type_code : 72
     p_codes :
      -
        code : |1-
         stAnalyse		est un COL_Charge_UI.STR_Evol_Charge_Projet
         stSprint_Tmp	est un COL_Charge_UI.STR_Evol_Charge_Projet_Spr
         nCle			est un entier
         
         stAnalyse = COL_Charge_UI.Graphe_Evol_Charge_projet(znIdProjet_P)
         
         grSupprimeSérie(GRF_Projection)
         grLibellé(GRF_Projection, grAfficheEtiquette, Vrai)
         grTitre(GRF_Projection, stAnalyse.sTitre)
         
         POUR TOUT stSprint_Tmp de stAnalyse.aStSeries
         	nCle++
         	grEtiquetteCatégorie(GRF_Projection,nCle,stSprint_Tmp.sEtiquette)
         	grAjouteDonnée(GRF_Projection,1, nCle,stSprint_Tmp.nCharge)
         	grAjouteDonnée(GRF_Projection,2, nCle,stSprint_Tmp.nChargeCumul)
         	si INT_AvecImponderable alors
         		grAjouteDonnée(GRF_Projection,3, nCle,stSprint_Tmp.nChargeImpond)
         		grAjouteDonnée(GRF_Projection,4, nCle,stSprint_Tmp.nChargeEngage)
         	fin
         FIn
         
         grEtiquetteSérie(GRF_Projection,1,"Charge ajoutée")
         grCouleurSérie(GRF_Projection,1,COL_Scrum.gnCouleur_OrangeVif)
         
         grEtiquetteSérie(GRF_Projection,2,"Charge cumulée")
         grCouleurSérie(GRF_Projection,2,COL_Scrum.gnCouleur_BleuVif)
         
         SI INT_AvecImponderable ALORS
         	grEtiquetteSérie(GRF_Projection,3,"Charge avec proj. impond.")
         	grCouleurSérie(GRF_Projection,3,COL_Scrum.gnCouleur_Gris)
         	grTypeSérie(GRF_Projection,3,grCourbe)
         	
         	grEtiquetteSérie(GRF_Projection,4,"Charge engagée")
         	grCouleurSérie(GRF_Projection,4,COL_Scrum.gnCouleur_VertVif)
         fin
         
         
         grDessine(GRF_Projection)
        type : 14
