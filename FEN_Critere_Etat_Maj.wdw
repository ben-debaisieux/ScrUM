    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         //Contrôle des valeurs
         SI SAI_Libelle = "" ALORS
         	Erreur("Libelle non renseigné")
         	RepriseSaisie(SAI_Libelle)	
         FIN
         
         //Enregistrement de la fiche
         SI ID_Etat = 0 ALORS HRAZ(Scr_CritereAccept_Etat)
         
         Scr_CritereAccept_Etat.Libelle = SAI_Libelle
         
         
         SI ID_Etat <> 0 ALORS
         	// Modification de la fiche
         	SI PAS HModifie(Scr_CritereAccept_Etat) ALORS
         		Erreur(ErreurInfo(errComplet))
         		RepriseSaisie(SAI_Libelle)
         	FIN
         	
         	Info("Le libelle a été modifier avec Succès !")
         SINON
         	//Ajout	
         	SI PAS HAjoute(Scr_CritereAccept_Etat) ALORS
         		Erreur(ErreurInfo(errComplet))
         		RepriseSaisie(SAI_Libelle)
         	FIN	
         	Info("Le libelle a été Ajouter avec Succès !")
         FIN
         MaFenêtre..ValeurRenvoyée = Vrai
         Ferme()
