   -
     name : Ajout_Nouveau_Commentaire
     procedure_id : 1454227255913353757
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Ajout_Nouveau_Commentaire ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure Ajout_Nouveau_Commentaire()
      
      aoGLPI_Ticket				est un tableau de ClGLPI_Ticket
      oGPLIMoteur					est un ClGLPI_Moteur(aoGLPI_Ticket)
      nIndice						est un entier
      sCommentaire				est une chaine
      bAffiche					est un booléen
      aStCommentaire_A_Ajouter	est un tableau de ClTrello_req.STR_Commentaire_Trello
      stUnCommentaire				est un ClTrello_req.STR_Commentaire_Trello
      nIndice_Com					est un entier
      
      
      Sablier(Vrai)
      
      TableSupprimeTout(TBL_GLPI)
      
      
      oGPLIMoteur.GetTicketsOpenedWinmat(nIDAttribué_GLPI, "WinMat","06-04-2023+00:00")
      
      
      zaoTickets = oGPLIMoteur.m_aopclEntite
      
      
      // récupération pièce jointe
      POUR nLigne = 1 _À_ TableauOccurrence(zaoTickets)
      	POUR nLigne_Doc = 1 _À_ TableauOccurrence(zaoTickets[nLigne].m_astDocument)
      		
      		zaoTickets[nLigne].m_astDocument[nLigne_Doc].buFFichier = oGPLIMoteur.GetTicketDocumentBuffer(zaoTickets[nLigne].m_astDocument[nLigne_Doc].nID)
      		
      	FIN
      FIN
      
      TableauTrie(zaoTickets, ttMembre, "-m_nid")
      
      //Parcoure tableau des nouveau ticket GLPI
      POUR nLigne = 1 _À_ TableauOccurrence(zaoTickets)
      	bAffiche = Faux
      	SI TableauOccurrence(zaoTickets[nLigne].m_astSuiviCom) > 0 ALORS
      		
      		aID_Ticket_Trello					est un tableau de chaine
      		aCommentaire_Ticket_Trello			est un tableau de chaine
      		sTitre								est une chaine	= zaoTickets[nLigne].m_nId + " " + zaoTickets[nLigne].m_sSujet + " ( " + zaoTickets[nLigne].m_sUtilisateur + " - " + zaoTickets[nLigne].m_sLieu + " )"
      		
      		
      		aID_Ticket_Trello = ClTrello_req.Rechercher_Ticket(sTitre)
      		
      		SI TableauOccurrence(aID_Ticket_Trello) > 0 ALORS
      			
      			POUR i = 1 _À_ TableauOccurrence(aID_Ticket_Trello)
      				aCommentaire_Ticket_Trello = ClTrello_req.Recup_Commentaire_Ticket(aID_Ticket_Trello[i])
      				
      				
      				stSuivi est un ClGLPI_Ticket.STR_Suivi_Commentaire
      				
      				POUR TOUT stSuivi DE zaoTickets[nLigne].m_astSuiviCom
      					
      					sCommentaire	= stSuivi.sUtilisateur + "(" + stSuivi.sDateAffichage + ")" + " : "
      					sCommentaire	+= [RC] + stSuivi.buContent
      					
      					sCommentaire	= HTMLVersTexte(sCommentaire)
      					
      					
      					nIndice			= aCommentaire_Ticket_Trello.Cherche(tcLinéairePremier, sCommentaire)
      					
      					
      					sCommentaire = ChaîneVersUTF8(sCommentaire)
      					
      					
      					SI nIndice = -1 ALORS
      						stUnCommentaire.sCommentaire	= sCommentaire
      						stUnCommentaire.sID_Ticket		= aID_Ticket_Trello[i]
      						TableauAjoute(aStCommentaire_A_Ajouter,stUnCommentaire)
      					FIN
      				FIN
      			FIN
      		FIN
      	FIN
      FIN
      
      
      SI OuiNon(Non,"Voulez-vous ajouter les nouveaux commentaires sur les tickets trello déja créé ([%TableauOccurrence(aStCommentaire_A_Ajouter)%] nouveaux commentaires) ?") ALORS
      	POUR nIndice_Com = 1 _À_ TableauOccurrence(aStCommentaire_A_Ajouter)
      		ClTrello_req.ADD_Commentaire_Ticket(aStCommentaire_A_Ajouter[nIndice_Com].sCommentaire,aStCommentaire_A_Ajouter[nIndice_Com].sID_Ticket)
      	FIN
      FIN
      
      libérer(oGPLIMoteur)
      
      FIN:
      
