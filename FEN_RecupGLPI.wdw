    name : BTN_Appliquer
    identifier : 0x23f8b500219620ba
    internal_properties : 
    properties :
     x : 471
     y : 852
     width : 80
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         // variable
         stTicket			est un ClTrello_req.STR_TICKET_TRELLO
         nIndice				est un entier
         sCheminDoc			est une chaine
         sRetour				est une chaine
         dh_Date_Echeance	est une dateheure
         vRetour				est un variant
         bUserDejaAssign		est un booleen
         stUtilisateur_LDAP est STR_LDAP_UTILISATEUR
         sInfo_Demandeur est une chaine
         
         
         // récupération information de l'utilisateur (mail + numéro fixe et portable)
         stUtilisateur_LDAP = COL_Scrum.LDAP_Recherche_Par_DisplayName(TBL_GLPI.COL_DEMANDEUR)
         sInfo_Demandeur = "Adresse mail demandeur : " + stUtilisateur_LDAP.sMail
         sInfo_Demandeur += [RC] + "Numéro téléphone portable demandeur : " + stUtilisateur_LDAP.sPortable
         sInfo_Demandeur += [RC] + "Numéro téléphone fixe demandeur : " + stUtilisateur_LDAP.sFixe
         
         // recherche les information du ticket
         nIndice = TableauCherche(zaoTickets,tcLinéaire,"m_nId",TBL_GLPI.COL_ID)
         
         
         // création ok
         oMoteur est un ClGLPI_Moteur(zaoTickets[nIndice])
         
         // charge le tableau des users affecté au ticket
         vRetour = oMoteur.GetTicketUserAssign(zaoTickets[nIndice].m_nId)
         
         
         // vérifie qu'une action trello est demandé
         SI CBO_Action..ValeurMémorisée <> 0 ALORS
         	// rempli la structure
         	stTicket.sSujet			= "#" + TBL_GLPI.COL_ID + " " + TBL_GLPI.COL_Sujet + " ( " + TBL_GLPI.COL_DEMANDEUR + " - " + TBL_GLPI.COL_Agence + " )"
         	stTicket.sDescription	= SAI_Infos_ticket + RC + sInfo_Demandeur
         	stTicket.sIDLabel		= CBO_Etiquette..ValeurMémorisée
         	stTicket.sIDStatut		= ClTrello_Kanban.cst_StatutKanban_A_faire
         	
         	SI INT_Prioritaire = 1 ALORS
         		stTicket.sIDPrioritaire = ClTrello_Kanban.cst_LabelKanban_Prioritaire
         	FIN
         	
         	SI INT_Attente_Info = 1 ALORS
         		stTicket.sIDEnAttente = ClTrello_Kanban.cst_LabelKanban_En_attente_d_infos
         	FIN
         	
         	SI CBO_Membre..ValeurMémorisée <> "" ALORS
         		stTicket.sIDMembers = CBO_Membre..ValeurMémorisée
         	FIN
         	
         	SI SAI_Date <> "" ALORS
         		dh_Date_Echeance		= SAI_Date + HeureSys()
         		stTicket.sDate_Echeance	= DateVersChaîne(dh_Date_Echeance,"AAAA-MM-JJTHH:mm:SS.CCC")
         	FIN
         	
         	
         	// parcoure toutes les pièces jointes du ticket 
         	POUR TOUT stDoc DE zaoTickets[nIndice].m_astDocument
         		// ajoute le fichier temp
         		sCheminDoc = ComplèteRep(fRépertoireTemp) + stDoc.sNom
         		
         		// créé fichier temp
         		fSauveBuffer(sCheminDoc,stDoc.buFFichier)
         		
         		// ajoute pièce jointe
         		TableauAjoute(stTicket.aPieces_Jointes,sCheminDoc)
         		
         	FIN
         	
         	SI TableauOccurrence(zaoTickets[nIndice].m_astSuiviCom) > 0 ALORS
         		stTicket.astCommentaires = zaoTickets[nIndice].m_astSuiviCom
         	FIN
         	
         	
         	// création du ticket trello 
         	sRetour = ClTrello_req.Add_Trello_Ticket(stTicket)
         	
         	ToastAffiche("Création ticket OK !",toastCourt)
         FIN
         
         
         SI sRetour <> "" ALORS
         	// erreur lors de la création
         	erreur(sRetour)
         SINON
         	
         	// vérifie si il y a un commentaire
         	SI SAI_Commentaire <> "" ALORS
         		SI PAS oMoteur.CreateFollowup(SAI_Commentaire) ALORS
         			Erreur("Erreur lors de l'ajout d'un commentaire dans le ticket Glpi avant le changement de statut.")
         			RepriseSaisie(MaFenêtre)
         		SINON
         			ToastAffiche("Ajout commentaire GLPI ok",toastCourt)
         		FIN
         	FIN
         	
         	SI PAS oMoteur.UpdateTicketCategory(zaoTickets[nIndice].m_nId, CBO_Categories_GLPI..ValeurMémorisée) ALORS
         		Erreur("Impossible de mettre à jour la category du ticket dans GLPI.")
         		RepriseSaisie(MaFenêtre)
         	SINON
         		ToastAffiche("Mise à jour de la catégorie du ticket GLPI ok",toastCourt)
         	FIN
         	
         	
         	SI PAS oMoteur.UpdateTicketStatut(zaoTickets[nIndice].m_nId, CBO_Statuts_GLPI..ValeurMémorisée) ALORS
         		Erreur("Impossible de mettre à jour le statut du ticket dans GLPI.")
         		RepriseSaisie(MaFenêtre)
         	SINON
         		ToastAffiche("Mise à jour du statut du ticket GLPI ok",toastCourt)
         	FIN
         	
         	SI CBO_Affecter_A..ValeurMémorisée <> 0 ALORS
         		
         		POUR nLigne_User = 1 _À_ TableauOccurrence(vRetour)
         			SI vRetour[nLigne_User].users_id = CBO_Affecter_A..ValeurMémorisée _ET_ vRetour[nLigne_User].type = 2 ALORS
         				// le user qu'on veut affecter au ticket et déja affecté
         				bUserDejaAssign = vrai
         			FIN
         		FIN
         		
         		// affecte le user uniquement si il n'est pas déja affecté au ticket
         		SI bUserDejaAssign = Faux ALORS
         			SI PAS oMoteur.UpdateTicketAssign(zaoTickets[nIndice].m_nId, CBO_Affecter_A..ValeurMémorisée) ALORS
         				Erreur("Impossible de mettre à jour le champ assigné à dans GLPI.")
         				RepriseSaisie(MaFenêtre)
         			SINON
         				ToastAffiche("Affectation ok",toastCourt)
         			FIN
         		FIN
         	FIN
         	
         FIN
         
         libérer(oMoteur)
         
         POUR nLigne = 1 _À_ TableauOccurrence(stTicket.aPieces_Jointes)
         	
         	// supprime fichier tmp
         	fSupprime(stTicket.aPieces_Jointes[nLigne])
         	
         FIN
         
         SAI_Commentaire = ""
         
         SI CBO_Statuts_GLPI..ValeurMémorisée <> nIDNouveau_GLPI ALORS
         	TableSupprimeSelect(TBL_GLPI)
         	ExécuteTraitement(TBL_GLPI,trtSélection)
         FIN
