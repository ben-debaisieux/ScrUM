        code : |1+
         nIndice			est un entier
         sCommentaire	est une chaine
         nLigne			est un entier
         image_Doc		est une Image
         
         nIndice					= TableauCherche(zaoTickets,tcLinéaire,"m_nId",TBL_GLPI.COL_ID)
         
         si nIndice > 0 alors
         	SAI_Infos_ticket	= HTMLVersTexte(zaoTickets[nIndice].m_bufDescription)
         	
         	SAI_Infos_ticket	+= RC + RC + "Commentaires :" + RC + RC
         	
         	POUR TOUT stCommentaire de zaoTickets[nIndice].m_astSuiviCom
         		sCommentaire	= stCommentaire.sUtilisateur + "(" + stCommentaire.sDateAffichage + ")" + " : "
         		sCommentaire	+= [RC] + stCommentaire.buContent
         		
         		sCommentaire	= HTMLVersTexte(sCommentaire)
         		
         		SAI_Infos_ticket += RC + RC + sCommentaire
         	FIN
         	
         	TableSupprimeTout(TBL_Piece_Jointe)
         	
         	// pieces jointes
         	POUR TOUT stDoc DE zaoTickets[nIndice].m_astDocument
         		// ajoute le fichier temp
         		nLigne = TableAjouteLigne(TBL_Piece_Jointe)
         		
         		SELON Minuscule(stDoc.sExtension)
         			CAS "xlsx","xls"
         				image_Doc = IMG_Exel
         			CAS "pdf"
         				image_Doc = IMG_PDF
         			CAS "txt","odt","doc","docx"
         				image_Doc = IMG_Word
         			AUTRE CAS
         				image_Doc = stDoc.buFFichier
         		FIN
         		
         		TBL_Piece_Jointe[nLigne].COL_Piece_Jointe = image_Doc
         		
         	FIN
         FIN
         
