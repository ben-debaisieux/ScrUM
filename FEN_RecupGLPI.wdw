    name : BTN_Rechercher
    identifier : 0x23f8b50721993b86
    internal_properties : 
    properties :
     x : 1157
     y : 15
     width : 80
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         aoGLPI_Ticket	est un tableau de ClGLPI_Ticket
         oGPLIMoteur		est un ClGLPI_Moteur(aoGLPI_Ticket)
         nIndice_Table est une entier
         
         Sablier(Vrai)
         
         TableSupprimeTout(TBL_GLPI)
         
         
         oGPLIMoteur.GetTicketsOpenedWinmat(nIDNouveau_GLPI, "WinMat")
         
         
         zaoTickets = oGPLIMoteur.m_aopclEntite
         
         
         // récupération pièce jointe
         POUR nLigne = 1 _À_ TableauOccurrence(zaoTickets)
         	POUR nLigne_Doc = 1 _À_ TableauOccurrence(zaoTickets[nLigne].m_astDocument)
         		
         		zaoTickets[nLigne].m_astDocument[nLigne_Doc].buFFichier = oGPLIMoteur.GetTicketDocumentBuffer(zaoTickets[nLigne].m_astDocument[nLigne_Doc].nID)
         		
         	FIN
         FIN
         
         TableauTrie(zaoTickets, ttMembre, "-m_nid")
         
         //Parcoure tableau des nouveau ticket GLPI
         POUR nLigne = 1 _À_ TableauOccurrence(zaoTickets)
         	nIndice_Table = TableAjouteLigne(TBL_GLPI)
         
         	TBL_GLPI[nIndice_Table].COL_ID				= zaoTickets[nLigne].m_nId
         	TBL_GLPI[nIndice_Table].COL_Sujet			= zaoTickets[nLigne].m_sSujet
         	TBL_GLPI[nIndice_Table].COL_DEMANDEUR		= zaoTickets[nLigne].m_sUtilisateur
         	TBL_GLPI[nIndice_Table].COL_Date_Creation	= zaoTickets[nLigne].m_dhDateCreation
         	TBL_GLPI[nIndice_Table].COL_Agence			=  zaoTickets[nLigne].m_sLieu
         	
         FIN
         
         
         TableAffiche(TBL_GLPI)
         
         libérer(oGPLIMoteur)
         
         FIN:
         Sablier(Faux)
         
         ExécuteTraitement(TBL_GLPI,trtSélection)
         ExécuteTraitement(CBO_Action,trtSélection)
